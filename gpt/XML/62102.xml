<configuration>\n     <system.webServer>\n       <handlers>\n         <add name="Python FastCGI" path="*" verb="*" modules="FastCgiModule" scriptProcessor="C:\path\to\python.exe|C:\path\to\wfastcgi.py" resourceType="Unspecified" />\n       </handlers>\n       <fastCgi>\n         <application fullPath="C:\path\to\python.exe" arguments="C:\path\to\wfastcgi.py">\n           <environmentVariables>\n             <environmentVariable name="WSGI_HANDLER" value="<your_module>.<your_callable>" />\n             <environmentVariable name="PYTHONPATH" value="<path_to_your_application>" />\n           </environmentVariables>\n         </application>\n       </fastCgi>\n     </system.webServer>\n   </configuration>