<configuration>\n      <system.web>\n        <profile defaultProvider="SQLProfileProvider">\n          <providers>\n            <add name="SQLProfileProvider"\n                 type="System.Web.Profile.SqlProfileProvider"\n                 connectionStringName="ProfileConnectionString"/>\n          </providers>\n          <properties>\n            <add name="CustomSetting1" type="System.String"/>\n            <add name="CustomSetting2" type="System.Int32"/>\n          </properties>\n        </profile>\n      </system.web>\n      <connectionStrings>\n        <add name="ProfileConnectionString" connectionString="your SQL Server connection string here"/>\n      </connectionStrings>\n    </configuration>