<Policy xmlns="urn:oasis:names:tc:xacml:3.0:core:schema:wd-17"\n        PolicyId="ExamplePolicy"\n        RuleCombiningAlgId="urn:oasis:names:tc:xacml:1.0:rule-combining-algorithm:permit-overrides">\n    <Target>\n        <AnyOf>\n            <AllOf>\n                <Match MatchId="urn:oasis:names:tc:xacml:1.0:function:string-equal">\n                    <AttributeValue DataType="http://www.w3.org/2001/XMLSchema#string">read</AttributeValue>\n                    <AttributeDesignator AttributeId="action" Category="urn:oasis:names:tc:xacml:3.0:attribute-category:action" DataType="http://www.w3.org/2001/XMLSchema#string"/>\n                </Match>\n            </AllOf>\n        </AnyOf>\n    </Target>\n    <Rule RuleId="PermitRead" Effect="Permit">\n        <Target>\n            <AnyOf>\n                <AllOf>\n                    <Match MatchId="urn:oasis:names:tc:xacml:1.0:function:string-equal">\n                        <AttributeValue DataType="http://www.w3.org/2001/XMLSchema#string">admin</AttributeValue>\n                        <AttributeDesignator AttributeId="role" Category="urn:oasis:names:tc:xacml:1.0:subject-category:access-subject" DataType="http://www.w3.org/2001/XMLSchema#string"/>\n                    </Match>\n                </AllOf>\n            </AnyOf>\n        </Target>\n    </Rule>\n    <Rule RuleId="DenyAll" Effect="Deny"/>\n</Policy>