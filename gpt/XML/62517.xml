<configuration>\n  <system.webServer>\n    <rewrite>\n      <rules>\n        <rule name="Rewrite example.net to subdirectory" stopProcessing="true">\n          <match url="(.*)" />\n          <conditions>\n            <add input="{HTTP_HOST}" pattern="^www\.example\.net$" />\n          </conditions>\n          <action type="Rewrite" url="/other/{R:1}" />\n        </rule>\n      </rules>\n    </rewrite>\n  </system.webServer>\n</configuration>