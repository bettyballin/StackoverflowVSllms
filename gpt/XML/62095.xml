<!-- Example in web.xml for configuring security constraints -->\n   <security-constraint>\n       <web-resource-collection>\n           <web-resource-name>Secured Area</web-resource-name>\n           <url-pattern>/secure/*</url-pattern>\n       </web-resource-collection>\n       <auth-constraint>\n           <role-name>USER</role-name>\n       </auth-constraint>\n   </security-constraint>