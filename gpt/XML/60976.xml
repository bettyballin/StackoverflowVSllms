<configuration>\n  <system.web>\n    <httpHandlers>\n      <add path="*.dll" verb="GET" type="System.Web.StaticFileHandler" />\n    </httpHandlers>\n  </system.web>\n  <system.webServer>\n    <handlers>\n      <add name="DllHandler" path="*.dll" verb="GET" type="System.Web.StaticFileHandler" resourceType="Unspecified" />\n    </handlers>\n    <staticContent>\n      <mimeMap fileExtension=".dll" mimeType="application/octet-stream" />\n    </staticContent>\n  </system.webServer>\n</configuration>