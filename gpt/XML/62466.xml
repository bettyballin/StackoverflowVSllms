<?xml version="1.0" encoding="UTF-8"?>\n<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform"\n                xmlns:exsl="http://exslt.org/common"\n                xmlns:str="http://exslt.org/strings"\n                exclude-result-prefixes="exsl str"\n                version="1.0">\n\n    <!-- Include EXSLT common functions -->\n    <xsl:import href="http://exslt.org/common"/>\n    <!-- Include EXSLT string functions -->\n    <xsl:import href="http://exslt.org/strings"/>\n\n    <xsl:output method="xml" indent="yes"/>\n\n    <xsl:template match="/">\n        <matches>\n            <xsl:for-each select="$set1">\n                <xsl:variable name="set1Item" select="."/>\n                <xsl:for-each select="$set2">\n                    <xsl:if test="str:starts-with($set1Item, .)">\n                        <match>\n                            <set1>\n                                <xsl:value-of select="$set1Item"/>\n                            </set1>\n                            <set2>\n                                <xsl:value-of select="."/>\n                            </set2>\n                        </match>\n                    </xsl:if>\n                </xsl:for-each>\n            </xsl:for-each>\n        </matches>\n    </xsl:template>\n\n</xsl:stylesheet>