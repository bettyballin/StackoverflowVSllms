<?xml version="1.0" encoding="UTF-8"?>\n<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">\n    <xsl:output method="text" encoding="UTF-8" />\n\n    <xsl:template match="/Import/Users">\n        <xsl:for-each select="User">\n            <xsl:value-of select="Contact/@FirstName"/> <xsl:text>,</xsl:text>\n            <xsl:value-of select="Contact/@LastName"/> <xsl:text>,</xsl:text>\n            <xsl:value-of select="Address/@Street1"/> <xsl:text>,</xsl:text>\n            <xsl:value-of select="Address/@City"/> <xsl:text>,</xsl:text>\n            <xsl:value-of select="Address/@State"/> <xsl:text>\n</xsl:text>\n        </xsl:for-each>\n    </xsl:template>\n</xsl:stylesheet>