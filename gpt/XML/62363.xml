<?xml version="1.0" encoding="utf-8" ?>\n<configuration>\n  <securityPolicy>\n    <policyLevels>\n      <policyLevel version="1.0" name="CustomMedium">\n        <namedPermissionSets>\n          <permissionSet class="System.Security.NamedPermissionSet" version="1" Name="CustomMedium" Description="Custom Medium Trust Level">\n            <!-- Inherit the default medium trust permissions -->\n            <IPermission class="System.Security.Permissions.SecurityPermission, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" version="1" Flags="Execution, SkipVerification, ControlThread, ControlPrincipal, ControlAppDomain, SerializationFormatter" />\n            <IPermission class="System.Security.Permissions.ReflectionPermission, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" version="1" Flags="ReflectionEmit" />\n            <IPermission class="System.Security.Permissions.FileIOPermission, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" version="1" Unrestricted="true" />\n            <IPermission class="System.Security.Permissions.SocketPermission, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" version="1" Unrestricted="true"/>\n            <!-- Add other permissions as necessary -->\n          </permissionSet>\n        </namedPermissionSets>\n        <codeGroups>\n          <codeGroup class="UnionCodeGroup" version="1" PermissionSetName="CustomMedium" Name="Custom_Medium_CodeGroup" Description="Code group for Custom Medium trust level">\n            <IMembershipCondition class="AllMembershipCondition" version="1" />\n          </codeGroup>\n        </codeGroups>\n      </policyLevel>\n    </policyLevels>\n  </securityPolicy>\n</configuration>