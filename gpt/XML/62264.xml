// Example flow configuration in XML for Nifi\n<flow>\n    <processor id="1" name="GenerateTableFetch">\n        <property name="Database Connection Pooling Service" value="DBCPConnectionPool"/>\n        <property name="Table Name" value="your_table_name"/>\n        <property name="Columns to Return" value="*"/>\n    </processor>\n    <processor id="2" name="PutFile">\n        <property name="Directory" value="/path/to/export"/>\n        <property name="Conflict Resolution Strategy" value="replace"/>\n    </processor>\n    <connection from="1" to="2"/>\n</flow>