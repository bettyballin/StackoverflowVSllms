<target name="compile-gwt">\n  <java classname="com.google.gwt.dev.Compiler" fork="true" failonerror="true">\n    <arg value="-war"/>\n    <arg value="target/war"/> <!-- Specify the output directory here -->\n    <arg value="com.example.MyModule"/> <!-- Your GWT module -->\n    <classpath>\n      <path refid="gwt.classpath"/>\n    </classpath>\n  </java>\n</target>\n\n<target name="copy-static-resources">\n  <copy todir="target/war">\n    <fileset dir="src/main/webapp">\n      <include name="**/*.html"/>\n      <include name="**/*.jsp"/>\n      <include name="**/*.css"/>\n      <include name="**/*.js"/>\n    </fileset>\n  </copy>\n</target>\n\n<target name="build" depends="compile-gwt, copy-static-resources"/>