<?xml version="1.0" encoding="UTF-8"?>\n<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">\n    <xsl:output method="text" />\n\n    <xsl:key name="pTags" match="p" use="generate-id(..)" />\n\n    <xsl:template match="/">\n        <xsl:variable name="maxCount" select="max(//*/count(key('pTags', generate-id())))" />\n        <xsl:for-each select="//*">\n            <xsl:if test="count(key('pTags', generate-id())) = $maxCount">\n                <xsl:value-of select="name()" />\n                <xsl:text> - </xsl:text>\n                <xsl:value-of select="count(key('pTags', generate-id()))" />\n                <xsl:text>&#10;</xsl:text>\n            </xsl:if>\n        </xsl:for-each>\n    </xsl:template>\n</xsl:stylesheet>