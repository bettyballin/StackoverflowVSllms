Public Function GetAssetDisplayList() As List(Of AssetDisplay)\n        Dim assetDisplayList As New List(Of AssetDisplay)\n\n        ' Assuming you have a method to execute the SQL and return a data reader\n        Dim sql As String = "SELECT a.ID_Asset, c.CategoryName, cust.CustodianName, m.ManufacturerName, s.SignedOutToName FROM Assets a " &\n                            "JOIN Categories c ON a.CategoryID = c.CategoryID " &\n                            "JOIN Custodians cust ON a.CustodianID = cust.CustodianID " &\n                            "JOIN Manufacturers m ON a.ManufacturerID = m.ManufacturerID " &\n                            "JOIN SignedOutTo s ON a.SignedOutToID = s.SignedOutToID"\n\n        Using dataReader As SqlDataReader = ExecuteReader(sql)\n            While dataReader.Read()\n                Dim assetDisplay As New AssetDisplay With {\n                    .ID_Asset = dataReader("ID_Asset"),\n                    .CategoryName = dataReader("CategoryName"),\n                    .CustodianName = dataReader("CustodianName"),\n                    .ManufacturerName = dataReader("ManufacturerName"),\n                    .SignedOutToName = dataReader("SignedOutToName")\n                }\n                assetDisplayList.Add(assetDisplay)\n            End While\n        End Using\n\n        Return assetDisplayList\n    End Functio