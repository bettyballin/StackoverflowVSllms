Public Function GetPopup(Of F As {Form, IDoSomething})() As F\n    ' Assuming GetInterfaces(Of F)() returns a list of forms implementing F\n    Dim lstIBasePopupChildForm As List(Of F) = GetInterfaces(Of F)()\n\n    ' Initialize and return the first popup form found\n    If lstIBasePopupChildForm.Count > 0 Then\n        Return lstIBasePopupChildForm(0)\n    Else\n        Return Nothing\n    End If\nEnd Functio