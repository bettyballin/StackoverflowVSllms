Imports System.Data.OleDb\n\nModule Module1\n    Sub Main()\n        ' Define the connection string (update the path to your Access database file)\n        Dim connectionString As String = "Provider=Microsoft.ACE.OLEDB.12.0;Data Source=C:\path\to\your\database.accdb"\n\n        ' Create a connection to the database\n        Using connection As New OleDbConnection(connectionString)\n            Try\n                ' Open the connection\n                connection.Open()\n\n                ' Define the INSERT SQL statement\n                Dim insertCommand As String = "INSERT INTO Orders (OrderNo, Customer) VALUES (@OrderNo, @Customer)"\n\n                ' Create an OleDbCommand object\n                Using command As New OleDbCommand(insertCommand, connection)\n                    ' Add parameters to the command\n                    command.Parameters.AddWithValue("@OrderNo", 21)\n                    command.Parameters.AddWithValue("@Customer", "ABC001")\n\n                    ' Execute the command\n                    Dim rowsAffected As Integer = command.ExecuteNonQuery()\n\n                    ' Check if the insert was successful\n                    If rowsAffected > 0 Then\n                        Console.WriteLine("Row inserted successfully.")\n                    Else\n                        Console.WriteLine("Failed to insert row.")\n                    End If\n                End Using\n\n            Catch ex As Exception\n                ' Handle any errors that may have occurred\n                Console.WriteLine("Error: " & ex.Message)\n            Finally\n                ' Ensure the connection is closed even if an error occurs\n                If connection.State = ConnectionState.Open Then\n                    connection.Close()\n                End If\n            End Try\n        End Using\n    End Sub\nEnd Module