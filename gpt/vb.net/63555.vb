Dim startInfo As New ProcessStartInfo("firefox.exe")\nstartInfo.Arguments = "www.stackoverflow.com"\nstartInfo.WindowStyle = ProcessWindowStyle.Minimized\n\nDim proc As Process = Process.Start(startInfo)\n\n' Directly accessing the StartInfo from the same Process instance.\nDebug.Print(String.Format("ProcessID={0}; Arguments={1}", proc.Id, proc.StartInfo.Arguments))\n\n' This will not have the StartInfo details.\nFor Each procInList As Process In Process.GetProcessesByName("firefox")\n    Debug.Print(String.Format("ProcessID={0}; Arguments={1}", procInList.Id, procInList.StartInfo.Arguments))\nNext procInList