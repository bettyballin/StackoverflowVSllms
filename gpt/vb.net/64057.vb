Function FindWord(board As Char(,), word As String) As Boolean\n    Dim rows As Integer = board.GetLength(0)\n    Dim cols As Integer = board.GetLength(1)\n    \n    For i As Integer = 0 To rows - 1\n        For j As Integer = 0 To cols - 1\n            If DFS(board, word, i, j, 0) Then\n                Return True\n            End If\n        Next\n    Next\n    \n    Return False\nEnd Function\n\nFunction DFS(board As Char(,), word As String, i As Integer, j As Integer, index As Integer) As Boolean\n    If index = word.Length Then\n        Return True\n    End If\n    \n    If i < 0 Or i >= board.GetLength(0) Or j < 0 Or j >= board.GetLength(1) Or board(i, j) <> word(index) Then\n        Return False\n    End If\n    \n    Dim temp As Char = board(i, j)\n    board(i, j) = "#"c ' Mark as visited\n    \n    If DFS(board, word, i + 1, j, index + 1) Or\n       DFS(board, word, i - 1, j, index + 1) Or\n       DFS(board, word, i, j + 1, index + 1) Or\n       DFS(board, word, i, j - 1, index + 1) Or\n       DFS(board, word, i + 1, j + 1, index + 1) Or\n       DFS(board, word, i - 1, j - 1, index + 1) Or\n       DFS(board, word, i + 1, j - 1, index + 1) Or\n       DFS(board, word, i - 1, j + 1, index + 1) Then\n        Button(i, j).DoClick() ' Highlight the button\n        Return True\n    End If\n    \n    board(i, j) = temp ' Unmark the visited cell\n    Return False\nEnd Functio