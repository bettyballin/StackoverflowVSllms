Public Sub SaveObjectToDatabase(obj As MyObject, connectionString As String)\n       Dim serializedObject As Byte() = SerializeObject(obj)\n\n       Using connection As New SqlConnection(connectionString)\n           connection.Open()\n           Using command As New SqlCommand("INSERT INTO MyTable (ObjectData) VALUES (@ObjectData)", connection)\n               command.Parameters.AddWithValue("@ObjectData", serializedObject)\n               command.ExecuteNonQuery()\n           End Using\n       End Using\n   End Sub