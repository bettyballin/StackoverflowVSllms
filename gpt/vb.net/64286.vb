Imports System.Web\nImports System.Collections.Specialized\n\nModule Module1\n    Sub Main()\n        Dim queryString As String = "?a=b&c=d&e=f"\n        Dim nvc As NameValueCollection = HttpUtility.ParseQueryString(queryString)\n\n        For Each key As String In nvc.AllKeys\n            Console.WriteLine($"{key} = {nvc(key)}")\n        Next\n\n        ' Access specific parameter\n        Dim aValue As String = nvc("a")\n        Console.WriteLine($"Value of 'a': {aValue}")\n    End Sub\nEnd Module