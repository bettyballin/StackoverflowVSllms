Imports HtmlAgilityPack\n\nModule Module1\n    Sub Main()\n        ' Load HTML document\n        Dim html As String = "<html><body><h1>Hello, World!</h1><p>This is a paragraph.</p></body></html>"\n        Dim doc As New HtmlDocument()\n        doc.LoadHtml(html)\n\n        ' Select nodes using XPath\n        Dim nodes As HtmlNodeCollection = doc.DocumentNode.SelectNodes("//h1 | //p")\n\n        ' Iterate through the selected nodes\n        For Each node As HtmlNode In nodes\n            Console.WriteLine(node.Name & ": " & node.InnerText)\n        Next\n    End Sub\nEnd Module