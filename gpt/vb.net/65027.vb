Imports System.Text.RegularExpressions\n\nModule Module1\n    Sub Main()\n        Dim sqlText As String = "CREATE TABLE MyTable (ID INT, Name VARCHAR(50), Age INT, Address TEXT)"\n        \n        ' Regular expression to match column definitions\n        Dim columnPattern As String = "\b(\w+)\s+(\w+(\(\d+\))?)\b"\n        Dim matches As MatchCollection = Regex.Matches(sqlText, columnPattern)\n        \n        Dim columns As New List(Of String)()\n        Dim dataTypes As New List(Of String)()\n\n        For Each match As Match In matches\n            columns.Add(match.Groups(1).Value)\n            dataTypes.Add(match.Groups(2).Value)\n        Next\n\n        ' Display results\n        Console.WriteLine("Columns: " & String.Join(", ", columns))\n        Console.WriteLine("Data Types: " & String.Join(", ", dataTypes))\n    End Sub\nEnd Module