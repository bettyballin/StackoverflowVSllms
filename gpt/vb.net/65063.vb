Imports System.Security.Cryptography\nImports System.Text\n\nModule KeyGenerator\n    Function GenerateRegistrationKey(userInfo As String, secretKey As String) As String\n        Using sha256 As SHA256 = SHA256.Create()\n            Dim combinedInfo As String = userInfo & secretKey\n            Dim hashBytes As Byte() = sha256.ComputeHash(Encoding.UTF8.GetBytes(combinedInfo))\n            Return BitConverter.ToString(hashBytes).Replace("-", "")\n        End Using\n    End Function\n\n    Sub Main()\n        Dim userInfo As String = "username@example.com" ' Replace with actual user info\n        Dim secretKey As String = "YourSecretKey" ' Use a consistent secret key\n        Dim registrationKey As String = GenerateRegistrationKey(userInfo, secretKey)\n        Console.WriteLine("Registration Key: " & registrationKey)\n    End Sub\nEnd Module