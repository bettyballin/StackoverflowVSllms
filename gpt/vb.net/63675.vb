Imports CrystalDecisions.CrystalReports.Engine\nImports CrystalDecisions.Shared\nImports System.Data\n\nPublic Class MainForm\n    Private Sub GenerateReport()\n        ' Create a DataTable and populate it with data\n        Dim dataTable As New DataTable("MyDataTable")\n        dataTable.Columns.Add("Column1", GetType(String))\n        dataTable.Columns.Add("Column2", GetType(Integer))\n\n        ' Add some rows to the DataTable\n        dataTable.Rows.Add("Row1", 1)\n        dataTable.Rows.Add("Row2", 2)\n\n        ' Create a DataSet and add the DataTable to it\n        Dim dataSet As New DataSet()\n        dataSet.Tables.Add(dataTable)\n\n        ' Load the Crystal Report\n        Dim report As New ReportDocument()\n        report.Load("PathToYourCrystalReport.rpt")\n\n        ' Set the data source of the report to the DataSet\n        report.SetDataSource(dataSet)\n\n        ' Assign the report to the CrystalReportViewer control\n        CrystalReportViewer1.ReportSource = report\n        CrystalReportViewer1.Refresh()\n    End Sub\nEnd Class