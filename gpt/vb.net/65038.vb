Imports Microsoft.Win32\n\nModule Module1\n    Sub Main()\n        Dim sqlCeVersion As String = GetSqlCeVersion()\n        If String.IsNullOrEmpty(sqlCeVersion) Then\n            Console.WriteLine("SQL CE is not installed.")\n        Else\n            Console.WriteLine("SQL CE Version: " & sqlCeVersion)\n        End If\n    End Sub\n\n    Function GetSqlCeVersion() As String\n        Dim version As String = String.Empty\n        Try\n            Dim key As RegistryKey = Registry.LocalMachine.OpenSubKey("SOFTWARE\Microsoft\Microsoft SQL Server Compact Edition")\n            If key IsNot Nothing Then\n                For Each subKeyName As String In key.GetSubKeyNames()\n                    Dim subKey As RegistryKey = key.OpenSubKey(subKeyName)\n                    If subKey IsNot Nothing Then\n                        Dim versionValue As Object = subKey.GetValue("Version")\n                        If versionValue IsNot Nothing Then\n                            version = versionValue.ToString()\n                        End If\n                        subKey.Close()\n                    End If\n                Next\n            End If\n        Catch ex As Exception\n            Console.WriteLine("Error accessing the registry: " & ex.Message)\n        End Try\n        Return version\n    End Function\nEnd Module