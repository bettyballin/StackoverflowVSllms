' Assuming you have an array or list called myFile\nDim myFile = New List(Of Object) From {\n    New With {.Last_Name = "Doe", .First_Name = "John", .Day = DateTime.Now},\n    New With {.Last_Name = "Smith", .First_Name = "Jane", .Day = DateTime.Now.AddDays(1)}\n}\n\n' Create a list of anonymous types using LINQ\nDim anonymousList = (From index In Enumerable.Range(0, myFile.Count)\n                     Select New With {\n                         .Name = myFile(index).Last_Name & ", " & myFile(index).First_Name,\n                         .StartDate = myFile(index).Day,\n                         .EndDate = myFile(index).Day\n                     }).ToList()\n\n' Accessing the elements in the anonymous list\nFor Each item In anonymousList\n    Console.WriteLine($"Name: {item.Name}, StartDate: {item.StartDate}, EndDate: {item.EndDate}")\nNext