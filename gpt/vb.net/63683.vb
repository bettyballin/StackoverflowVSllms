Private Sub ucboPatientInfo_RowSelected(ByVal sender As Object, ByVal e As Infragistics.Win.UltraWinGrid.RowSelectedEventArgs) Handles ucboPatientInfo.RowSelected\n    ' Ensure a row is selected\n    If ucboPatientInfo.SelectedRow IsNot Nothing Then\n        ' Get the value from the "LastName" column\n        Dim lastName As String = ucboPatientInfo.SelectedRow.GetCellValue("LastName").ToString()\n        ' Set the value to the textbox\n        txtLastName.Text = lastName\n    End If\nEnd Sub