' Example using Moq for unit testing\nDim mockRepo As New Mock(Of IProductRepository)()\nmockRepo.Setup(Function(repo) repo.GetProduct(It.IsAny(Of Integer)())).Returns(New Product() With {.ProductID = 1, .Name = "Test Product"})\n\nDim productService As New ProductService(mockRepo.Object)\n\nDim prod As Product = productService.GetProduct(1)\nAssert.AreEqual("Test Product", prod.Name)