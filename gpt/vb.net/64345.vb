Imports System.IO.Ports\n\nModule Module1\n    Sub Main()\n        Dim serialPort As New SerialPort("COM1", 9600, Parity.None, 8, StopBits.One)\n        AddHandler serialPort.DataReceived, AddressOf DataReceivedHandler\n        serialPort.Open()\n\n        Console.WriteLine("Listening for incoming calls...")\n        Console.ReadLine()\n        serialPort.Close()\n    End Sub\n\n    Private Sub DataReceivedHandler(sender As Object, e As SerialDataReceivedEventArgs)\n        Dim sp As SerialPort = CType(sender, SerialPort)\n        Dim inData As String = sp.ReadExisting()\n        Console.WriteLine($"Data Received: {inData}")\n\n        ' Decode the caller ID info here\n        If inData.Contains("NMBR") Then\n            Dim phoneNumber As String = ExtractPhoneNumber(inData)\n            Console.WriteLine($"Incoming call from: {phoneNumber}")\n        End If\n    End Sub\n\n    Private Function ExtractPhoneNumber(data As String) As String\n        ' Assuming the data contains "NMBR = 1234567890"\n        Dim start As Integer = data.IndexOf("NMBR") + 5\n        Dim [end] As Integer = data.IndexOf(vbCr, start)\n        Return data.Substring(start, [end] - start).Trim()\n    End Function\nEnd Module