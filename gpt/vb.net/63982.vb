Imports System.Data.Odbc\n\nModule Module1\n    Sub Main()\n        ' Define the connection string (adjust the connection string according to your database)\n        Dim connectionString As String = "Driver={SQL Server};Server=your_server;Database=your_database;Trusted_Connection=yes;"\n\n        ' Create a new OdbcConnection object\n        Using connection As New OdbcConnection(connectionString)\n            Try\n                ' Open the connection\n                connection.Open()\n\n                ' Define the query to execute\n                Dim query As String = "SELECT * FROM your_table"\n\n                ' Create an OdbcCommand object\n                Using command As New OdbcCommand(query, connection)\n                    ' Execute the query and obtain an OdbcDataReader\n                    Using reader As OdbcDataReader = command.ExecuteReader()\n                        ' Check if the reader has any rows\n                        If reader.HasRows Then\n                            ' Iterate through the data\n                            While reader.Read()\n                                ' Example: Retrieve data by column index\n                                Dim columnValue As String = reader.GetString(0)\n                                Console.WriteLine(columnValue)\n                            End While\n                        Else\n                            Console.WriteLine("No rows found.")\n                        End If\n                    End Using\n                End Using\n\n            Catch ex As Exception\n                ' Handle exceptions (e.g., connection errors, query errors)\n                Console.WriteLine("An error occurred: " & ex.Message)\n            End Try\n        End Using\n    End Sub\nEnd Module