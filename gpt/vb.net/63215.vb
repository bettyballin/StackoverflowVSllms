Imports System.Xml.Linq\nImports System.Linq\n\nModule Module1\n\n    Sub Main()\n        Dim xmlData As String = "\n            <django-objects version='1.0'>\n                <object pk='1' model='app.person'>\n                    <field type='CharField' name='reference_num'>001</field>\n                    <field type='DateField' name='archive_date'><None></None></field>\n                </object>\n                <object pk='2' model='app.person'>\n                    <field type='DateField' name='archive_date'>2022-01-01</field>\n                </object>\n            </django-objects>"\n\n        Dim doc As XDocument = XDocument.Parse(xmlData)\n\n        ' Find and replace <None></None> with 1899-01-01\n        Dim targetElements = From el In doc.Descendants("field")\n                             Where el.Attribute("type")?.Value = "DateField" AndAlso\n                                   el.Attribute("name")?.Value = "archive_date" AndAlso\n                                   el.Value = "<None></None>"\n\n        For Each el In targetElements\n            el.Value = "1899-01-01"\n        Next\n\n        ' Output the modified XML\n        Console.WriteLine(doc.ToString())\n    End Sub\n\nEnd Module