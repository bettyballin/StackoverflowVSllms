Public Class Page\n\n  Private _IdList As List(Of Integer) = New List(Of Integer)()\n  Private _HasNewItems As Boolean = False\n\n  Public Sub AddItems(ByVal Items As List(Of Item))\n    Dim itemIDs As List(Of Integer) = Items.Select(Function(item) item.ID).ToList()\n\n    If _IdList.Intersect(itemIDs).Count() = itemIDs.Count Then\n      _HasNewItems = False\n    Else\n      _HasNewItems = True\n      _IdList.AddRange(itemIDs.Except(_IdList))\n    End If\n  End Sub\n\nEnd Class