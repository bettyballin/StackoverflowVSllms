Imports System\n\nModule Module1\n    Function NetworkDays(startDate As DateTime, endDate As DateTime, Optional holidays As List(Of DateTime) = Nothing) As Integer\n        Dim totalDays As Integer = 0\n        Dim currentDate As DateTime = startDate\n\n        While currentDate <= endDate\n            If currentDate.DayOfWeek <> DayOfWeek.Saturday AndAlso currentDate.DayOfWeek <> DayOfWeek.Sunday Then\n                If holidays Is Nothing OrElse Not holidays.Contains(currentDate) Then\n                    totalDays += 1\n                End If\n            End If\n            currentDate = currentDate.AddDays(1)\n        End While\n\n        Return totalDays\n    End Function\n\n    Sub Main()\n        Dim startDate As DateTime = New DateTime(2023, 10, 1)\n        Dim endDate As DateTime = New DateTime(2023, 10, 31)\n        Dim holidays As New List(Of DateTime) From {\n            New DateTime(2023, 10, 9) ' Example holiday\n        }\n\n        Dim result As Integer = NetworkDays(startDate, endDate, holidays)\n        Console.WriteLine("Number of network days: " & result)\n    End Sub\nEnd Module