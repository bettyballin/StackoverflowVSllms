Try\n       Me.Adapter.Fill(dataTable)\n   Catch ex As ConstraintException\n       Trace.WriteLine("ConstraintException: " & ex.Message)\n       For Each TRow As DataRow In dataTable.Rows\n           If TRow.HasErrors Then\n               Trace.WriteLine("Row Error: " & TRow.RowError)\n               For Each col As DataColumn In TRow.GetColumnsInError()\n                   Trace.WriteLine("Column Error: " & col.ColumnName & " - " & TRow.GetColumnError(col))\n               Next\n           End If\n       Next\n   Catch ex As Exception\n       Trace.WriteLine("General Error: " & ex.Message)\n   End Try