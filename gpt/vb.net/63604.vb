Imports System.Xml.Xsl\nImports System.Xml\n\nModule Module1\n    Sub Main()\n        Dim xslt As New XslCompiledTransform()\n        xslt.Load("path_to_your_xsl_file.xsl")\n\n        Dim xmlDoc As New XmlDocument()\n        xmlDoc.LoadXml("<root/>")\n\n        Using writer As XmlWriter = XmlWriter.Create("output.html")\n            xslt.Transform(xmlDoc, writer)\n        End Using\n    End Sub\nEnd Module