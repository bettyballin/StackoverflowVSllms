Sub SumBooleanAttributes()\n    Dim db As DAO.Database\n    Dim rs As DAO.Recordset\n    Dim fld As DAO.Field\n    Dim sql As String\n    Dim sumSql As String\n    Dim attribCount As Integer\n    Dim attribName As String\n    Dim sumResults As New Collection\n    Dim result As Variant\n    \n    ' Open the database\n    Set db = CurrentDb()\n    \n    ' Construct the base SQL\n    sql = "SELECT "\n    \n    ' Loop through the table fields to find attribute columns\n    Set rs = db.OpenRecordset("SELECT TOP 1 * FROM YourTableName")\n    sumSql = ""\n    For Each fld In rs.Fields\n        If Left(fld.Name, 6) = "attrib" Then\n            sumSql = sumSql & "SUM(IIF(" & fld.Name & ", 1, 0)) AS " & fld.Name & ", "\n        End If\n    Next fld\n    rs.Close\n    \n    ' Remove the trailing comma and space\n    If Len(sumSql) > 0 Then\n        sumSql = Left(sumSql, Len(sumSql) - 2)\n    End If\n    \n    ' Complete the SQL query\n    sql = sql & sumSql & " FROM YourTableName"\n    \n    ' Execute the SQL query\n    Set rs = db.OpenRecordset(sql)\n    \n    ' Process the results\n    For Each fld In rs.Fields\n        attribName = fld.Name\n        attribCount = fld.Value\n        sumResults.Add Array(attribName, attribCount)\n    Next fld\n    rs.Close\n    \n    ' Sort the results by count in descending order\n    Set sumResults = SortCollectionByCount(sumResults)\n    \n    ' Output the results\n    For Each result In sumResults\n        Debug.Print "Attribute: " & result(0) & " Count: " & result(1)\n    Next result\n    \n    ' Clean up\n    Set rs = Nothing\n    Set db = Nothing\nEnd Sub\n\nFunction SortCollectionByCount(col As Collection) As Collection\n    Dim sortedCol As New Collection\n    Dim i As Integer, j As Integer\n    Dim temp As Variant\n    \n    ' Bubble sort the collection\n    For i = 1 To col.Count - 1\n        For j = i + 1 To col.Count\n            If col(i)(1) < col(j)(1) Then\n                ' Swap\n                temp = col(i)\n                Set col(i) = col(j)\n                Set col(j) = temp\n            End If\n        Next j\n    Next i\n    \n    ' Copy sorted items to new collection\n    For i = 1 To col.Count\n        sortedCol.Add col(i)\n    Next i\n    \n    Set SortCollectionByCount = sortedCol\nEnd Functio