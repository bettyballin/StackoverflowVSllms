Imports System.Data.SqlClient\n\nPublic Class Form1\n    Private Sub Form1_Load(sender As Object, e As EventArgs) Handles MyBase.Load\n        ' Assume DataGridView1 is your DataGridView control\n        Dim sqlParts As HashSet(Of String) = GetSqlParts()\n        Dim cadParts As HashSet(Of String) = GetCadParts()\n\n        ' Populate the DataGridView with SQL parts first\n        For Each part In sqlParts\n            Dim index As Integer = DataGridView1.Rows.Add(part)\n            DataGridView1.Rows(index).DefaultCellStyle.BackColor = Color.Yellow\n        Next\n\n        ' Check each part from CAD parts\n        For Each part In cadParts\n            Dim found As Boolean = False\n            For Each row As DataGridViewRow In DataGridView1.Rows\n                If row.Cells(0).Value.ToString() = part Then\n                    row.DefaultCellStyle.BackColor = Color.Transparent\n                    found = True\n                    Exit For\n                End If\n            Next\n\n            ' If part is not found in DataGridView, add it and color it red\n            If Not found Then\n                Dim index As Integer = DataGridView1.Rows.Add(part)\n                DataGridView1.Rows(index).DefaultCellStyle.BackColor = Color.Red\n            End If\n        Next\n    End Sub\n\n    Private Function GetSqlParts() As HashSet(Of String)\n        Dim parts As New HashSet(Of String)\n        Using conn As New SqlConnection("your_connection_string")\n            conn.Open()\n            Dim cmd As New SqlCommand("SELECT PartNumber FROM PartsTable", conn)\n            Using reader As SqlDataReader = cmd.ExecuteReader()\n                While reader.Read()\n                    parts.Add(reader("PartNumber").ToString())\n                End While\n            End Using\n        End Using\n        Return parts\n    End Function\n\n    Private Function GetCadParts() As HashSet(Of String)\n        Dim parts As New HashSet(Of String)\n        ' Load parts from CAD files\n        ' For example:\n        ' parts.Add("CAD_Part1")\n        ' parts.Add("CAD_Part2")\n        ' ... load actual CAD parts\n        Return parts\n    End Function\nEnd Class