Public Function GetAllChildNodes(parentNode As TreeNode) As List(Of String)\n    Dim nodes As New List(Of String)\n    GetChildNodesRecursive(parentNode, nodes)\n    Return nodes\nEnd Function\n\nPrivate Sub GetChildNodesRecursive(currentNode As TreeNode, nodes As List(Of String))\n    For Each childNode As TreeNode In currentNode.Nodes\n        nodes.Add(childNode.Text)\n        If childNode.Nodes.Count > 0 Then\n            GetChildNodesRecursive(childNode, nodes)\n        End If\n    Next\nEnd Sub