Sub RemoveRecentFile()\n    Dim recentFile As RecentFile\n    Dim fileNameToRemove As String\n\n    ' Specify the name of the file you want to remove\n    fileNameToRemove = "C:\Path\To\Your\File.xlsx"\n\n    ' Loop through the recent files collection\n    For Each recentFile In Application.RecentFiles\n        ' Check if the current recent file matches the file to remove\n        If recentFile.Path = fileNameToRemove Then\n            ' Remove the recent file from the list\n            recentFile.Delete\n            Exit For\n        End If\n    Next recentFile\n\n    MsgBox "File removed from recent documents list."\nEnd Sub