Imports System.Xml\n\nPublic Class InfoPathDocument\n    Private _sVersion As String\n    Private _pVersion As String\n    Private _piVersion As String\n    Private _href As String\n    Private _name As String\n\n    Public Property SolutionVersion() As String\n        Get\n            Return _sVersion\n        End Get\n        Set(ByVal value As String)\n            _sVersion = value\n        End Set\n    End Property\n\n    Public Property ProductVersion() As String\n        Get\n            Return _pVersion\n        End Get\n        Set(ByVal value As String)\n            _pVersion = value\n        End Set\n    End Property\n\n    Public Property PIVersion() As String\n        Get\n            Return _piVersion\n        End Get\n        Set(ByVal value As String)\n            _piVersion = value\n        End Set\n    End Property\n\n    Public Property href() As String\n        Get\n            Return _href\n        End Get\n        Set(ByVal value As String)\n            _href = value\n        End Set\n    End Property\n\n    Public Property name() As String\n        Get\n            Return _name\n        End Get\n        Set(ByVal value As String)\n            _name = value\n        End Set\n    End Property\nEnd Class\n\nPrivate Sub ReadXMLAttributes(ByVal oXML As String)\n    ReadXMLAttributes(oXML, "mso-infoPathSolution")\nEnd Sub\n\nPrivate Sub ReadXMLAttributes(ByVal oXML As String, ByVal oTagName As String)\n    Try\n        Dim XmlDoc As New XmlDocument()\n        XmlDoc.LoadXml(oXML)\n        Dim oFileInfo As New InfoPathDocument()\n        Dim XmlNodes As XmlNodeList = XmlDoc.GetElementsByTagName(oTagName)\n\n        For Each xNode As XmlNode In XmlNodes\n            With xNode\n                If .Attributes("solutionVersion") IsNot Nothing Then\n                    oFileInfo.SolutionVersion = .Attributes("solutionVersion").Value\n                End If\n                If .Attributes("productVersion") IsNot Nothing Then\n                    oFileInfo.ProductVersion = .Attributes("productVersion").Value\n                End If\n                If .Attributes("PIVersion") IsNot Nothing Then\n                    oFileInfo.PIVersion = .Attributes("PIVersion").Value\n                End If\n                If .Attributes("href") IsNot Nothing Then\n                    oFileInfo.href = .Attributes("href").Value\n                End If\n                If .Attributes("name") IsNot Nothing Then\n                    oFileInfo.name = .Attributes("name").Value\n                End If\n            End With\n        Next\n    Catch ex As Exception\n        MsgBox(ex.Message, MsgBoxStyle.OkOnly, "ReadXMLAttributes")\n    End Try\nEnd Sub