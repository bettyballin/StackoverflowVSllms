Dim countries As New Generic.List(Of String)\nFor Each ci As Globalization.CultureInfo In Globalization.CultureInfo.GetCultures(Globalization.CultureTypes.AllCultures And Globalization.CultureTypes.NeutralCultures)\n    Try\n        Dim ri As New Globalization.RegionInfo(ci.LCID)\n        If Not countries.Contains(ri.NativeName) Then\n            countries.Add(ri.NativeName)\n        End If\n    Catch ex As ArgumentException\n        ' Ignore cultures that do not have a region\n    End Try\nNext ci\n\ncountries.Sort()\nFor Each country As String In countries\n    Console.WriteLine(country)\nNext