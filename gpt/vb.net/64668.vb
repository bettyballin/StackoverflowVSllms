Imports Microsoft.Win32\nImports System.Text\n\nModule Module1\n    Sub Main()\n        ' Example: Reading a REG_BINARY value from the registry\n        Dim key As RegistryKey = Registry.CurrentUser.OpenSubKey("Software\YourSoftwareKey")\n        If key IsNot Nothing Then\n            Dim binaryValue As Byte() = CType(key.GetValue("YourBinaryValueName"), Byte())\n            If binaryValue IsNot Nothing Then\n                Dim readableString As String = Encoding.Unicode.GetString(binaryValue)\n                Console.WriteLine(readableString)\n            Else\n                Console.WriteLine("Binary value not found.")\n            End If\n            key.Close()\n        Else\n            Console.WriteLine("Registry key not found.")\n        End If\n    End Sub\nEnd Module