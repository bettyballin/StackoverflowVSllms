Imports HtmlAgilityPack\nImports System.Data\n\nPublic Class WebScraper\n\n    Public Function ExtractAddressFromWebPage(url As String) As DataSet\n        Dim web As New HtmlWeb()\n        Dim doc As HtmlDocument = web.Load(url)\n\n        ' Extract address information (you may need to adjust the XPath based on the actual HTML structure)\n        Dim addresses As HtmlNodeCollection = doc.DocumentNode.SelectNodes("//address")\n\n        Dim ds As New DataSet()\n        Dim dt As New DataTable("Addresses")\n        dt.Columns.Add("Address", GetType(String))\n\n        If addresses IsNot Nothing Then\n            For Each addressNode As HtmlNode In addresses\n                Dim row As DataRow = dt.NewRow()\n                row("Address") = addressNode.InnerText.Trim()\n                dt.Rows.Add(row)\n            Next\n        End If\n\n        ds.Tables.Add(dt)\n        Return ds\n    End Function\n\nEnd Class