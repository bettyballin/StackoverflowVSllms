Sub ExampleTransaction()\n    Dim db As DAO.Database\n    Set db = CurrentDb\n    \n    On Error GoTo ErrHandler\n    \n    ' Begin transaction\n    db.BeginTrans\n    \n    ' Perform some database operations\n    db.Execute "INSERT INTO table_name (column1, column2) VALUES (value1, value2)"\n    db.Execute "UPDATE table_name SET column1 = value WHERE condition"\n    \n    ' Commit transaction\n    db.CommitTrans\n    \n    Exit Sub\n    \nErrHandler:\n    ' Rollback transaction in case of an error\n    db.RollbackTrans\n    MsgBox "Transaction failed: " & Err.Description\nEnd Sub