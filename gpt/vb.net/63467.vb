Private Sub GetHtmlContent()\n    Dim html As String\n    html = "<html><body><p>This is a paragraph.</p><div>This is a div.</div></body></html>"\n\n    Dim doc As Object\n    Set doc = CreateObject("htmlfile")\n    doc.write html\n    doc.close\n\n    ' Extract the text content\n    Dim content As String\n    content = doc.body.innerText\n\n    ' Output the content\n    MsgBox content\nEnd Sub