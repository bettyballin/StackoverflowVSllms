Imports Microsoft.Office.Interop.Excel\n\nModule Module1\n    Sub Main()\n        ' Path to the Excel file\n        Dim filePath As String = "C:\path\to\your\file.xlsx"\n\n        ' Create a new instance of Excel application\n        Dim excelApp As New Application()\n\n        ' Make Excel invisible to the user\n        excelApp.Visible = False\n\n        ' Open the Excel file\n        Dim workbook As Workbook = excelApp.Workbooks.Open(filePath)\n        Dim worksheet As Worksheet = workbook.Sheets(1)\n\n        ' Read data from the first cell (A1)\n        Dim cellValue As String = worksheet.Cells(1, 1).Value.ToString()\n\n        Console.WriteLine("Value in A1: " & cellValue)\n\n        ' Clean up\n        workbook.Close(False)\n        excelApp.Quit()\n\n        ' Release COM objects\n        ReleaseComObject(worksheet)\n        ReleaseComObject(workbook)\n        ReleaseComObject(excelApp)\n\n        Console.ReadLine()\n    End Sub\n\n    Private Sub ReleaseComObject(ByVal obj As Object)\n        Try\n            If obj IsNot Nothing Then\n                System.Runtime.InteropServices.Marshal.ReleaseComObject(obj)\n                obj = Nothing\n            End If\n        Catch ex As Exception\n            obj = Nothing\n        Finally\n            GC.Collect()\n        End Try\n    End Sub\nEnd Module