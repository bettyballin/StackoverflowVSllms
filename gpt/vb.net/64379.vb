Imports System.Web.Services.Protocols\n\nPartial Class SearchPage\n    Inherits System.Web.UI.Page\n\n    Protected Sub Page_Load(ByVal sender As Object, ByVal e As EventArgs) Handles Me.Load\n        If Not IsPostBack Then\n            PerformSearch("your search query")\n        End If\n    End Sub\n\n    Private Sub PerformSearch(ByVal queryText As String)\n        ' Create an instance of the search query web service\n        Dim searchService As New SearchService.QueryService()\n        searchService.Credentials = System.Net.CredentialCache.DefaultCredentials\n\n        ' Create a query request\n        Dim queryRequest As New SearchService.QueryRequest()\n        queryRequest.QueryText = queryText\n        queryRequest.QueryOptions = "<QueryOptions><EnableStemming>true</EnableStemming><TrimDuplicates>true</TrimDuplicates></QueryOptions>"\n        queryRequest.ResultTypes = SearchService.ResultType.RelevantResults\n\n        Try\n            ' Execute the query\n            Dim queryResponse As SearchService.QueryResponse = searchService.Query(queryRequest)\n            Dim results As XmlNode = queryResponse.Results\n\n            ' Process the results (you can bind it to a control like GridView or process it as needed)\n            ' Example: Display the raw XML\n            Response.Write(results.OuterXml)\n        Catch ex As SoapException\n            ' Handle exception\n            Response.Write("Error: " & ex.Message)\n        End Try\n    End Sub\nEnd Class