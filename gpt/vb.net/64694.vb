Public Sub CopyAttachments(SourceTable As String, SourceField As String, TargetTable As String, TargetField As String)\n    Dim db As DAO.Database\n    Dim rsSource As DAO.Recordset\n    Dim rsTarget As DAO.Recordset\n    Dim rsSourceAttachments As DAO.Recordset\n    Dim rsTargetAttachments As DAO.Recordset\n    Dim fld As DAO.Field\n\n    Set db = CurrentDb()\n    Set rsSource = db.OpenRecordset("SELECT * FROM " & SourceTable)\n    Set rsTarget = db.OpenRecordset("SELECT * FROM " & TargetTable)\n\n    Do While Not rsSource.EOF\n        Set rsSourceAttachments = rsSource.Fields(SourceField).Value\n        rsTarget.AddNew\n        ' Copy other fields as needed\n        rsTarget.Update\n        rsTarget.Bookmark = rsTarget.LastModified\n        Set rsTargetAttachments = rsTarget.Fields(TargetField).Value\n\n        Do While Not rsSourceAttachments.EOF\n            rsTargetAttachments.AddNew\n            For Each fld In rsSourceAttachments.Fields\n                rsTargetAttachments.Fields(fld.Name).Value = fld.Value\n            Next fld\n            rsTargetAttachments.Update\n            rsSourceAttachments.MoveNext\n        Loop\n\n        rsSource.MoveNext\n    Loop\nEnd Sub