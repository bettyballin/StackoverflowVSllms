Dim encryptedData As New CAPICOM.EncryptedData\nencryptedData.Algorithm.Name = CAPICOM_ENCRYPTION_ALGORITHM_3DES\n\n' Set your secret data\nencryptedData.Content = "Your secret data here"\n\n' Encrypt the data\nencryptedData.SetSecret "yourpassword"\nDim encryptedString As String\nencryptedString = encryptedData.Encrypt()\n\n' Decrypt the data\nDim decryptedData As New CAPICOM.EncryptedData\ndecryptedData.SetSecret "yourpassword"\ndecryptedData.Decrypt(encryptedString)\nDim decryptedString As String\ndecryptedString = decryptedData.Content\n\n' Output the decrypted data\nMsgBox decryptedString