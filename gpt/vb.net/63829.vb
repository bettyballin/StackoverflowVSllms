<%@ Page Language="VB" %>\n<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">\n<script runat="server">\n    Public TextFromString As String = "test text test text"\n    Public TextFromMethod As String\n\n    Protected Sub Page_Load(ByVal sender As Object, ByVal e As EventArgs)\n        If Not IsPostBack Then\n            TextFromMethod = RepeatChar("S", 50)\n            TextBox1.Text = TextFromString\n            TextBox2.Text = TextFromMethod\n        End If\n    End Sub\n\n    Public Function RepeatChar(ByVal Input As String, ByVal Count As Integer) As String\n        Return New String(Input(0), Count)\n    End Function\n</script>\n<html xmlns="http://www.w3.org/1999/xhtml">\n<head id="Head1" runat="server">\n    <title>Test Page</title>\n</head>\n<body>\n    <form id="form1" runat="server">\n    <div>\n        <%= TextFromString %>\n        <br />\n        <asp:TextBox ID="TextBox1" runat="server"></asp:TextBox>\n        <br />\n        <%= TextFromMethod %>\n        <br />\n        <asp:TextBox ID="TextBox2" runat="server"></asp:TextBox>        \n    </div>   \n    </form>\n</body>\n</html>