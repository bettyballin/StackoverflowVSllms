Dim bloops As List(Of Bloop) = GetBloops() ' Assuming GetBloops() returns a list of Bloop objects\n\nDim razzies As List(Of Razzie) = (From b In bloops\n                                  Select New Razzie With {\n                                      .FirstName = b.FirstName,\n                                      .LastName = b.LastName\n                                  }).ToList()