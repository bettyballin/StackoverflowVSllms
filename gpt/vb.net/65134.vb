Public Class Form1\n    Private Sub Form1_Load(sender As Object, e As EventArgs) Handles MyBase.Load\n        ' Create the WebBrowser control\n        Dim webBrowser As New WebBrowser()\n        \n        ' Set properties for the WebBrowser control\n        webBrowser.Name = "dynamicWebBrowser"\n        webBrowser.Dock = DockStyle.Fill\n        \n        ' Add the WebBrowser control to the form\n        Me.Controls.Add(webBrowser)\n        \n        ' Connect the DocumentCompleted event to the event handler\n        AddHandler webBrowser.DocumentCompleted, AddressOf WebBrowser_DocumentCompleted\n        \n        ' Optionally, navigate to a URL\n        webBrowser.Navigate("https://example.com")\n    End Sub\n\n    ' Event handler for the WebBrowser's DocumentCompleted event\n    Private Sub WebBrowser_DocumentCompleted(sender As Object, e As WebBrowserDocumentCompletedEventArgs)\n        MessageBox.Show("Document Loaded: " & e.Url.ToString())\n    End Sub\nEnd Class