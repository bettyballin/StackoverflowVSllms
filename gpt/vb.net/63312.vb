Imports Microsoft.Office.Interop.MSProject\n\nModule Module1\n    Sub Main()\n        Dim projectApp As New Application()\n        Try\n            projectApp.FileOpen("C:\path\to\your\projectfile.mpp", ReadOnly:=True)\n\n            Dim project As Project = projectApp.ActiveProject\n\n            For Each task As Task In project.Tasks\n                If task IsNot Nothing Then\n                    Console.WriteLine($"Task Name: {task.Name}, Start Date: {task.Start}, Finish Date: {task.Finish}")\n                End If\n            Next\n\n            projectApp.FileCloseAll(Save:=PjSaveType.pjDoNotSave)\n        Catch ex As Exception\n            Console.WriteLine("Error: " & ex.Message)\n        Finally\n            projectApp.Quit(PjSaveType.pjDoNotSave)\n            Marshal.ReleaseComObject(projectApp)\n        End Try\n    End Sub\nEnd Module