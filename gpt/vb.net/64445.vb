Imports System.Reflection\n\nPublic Sub AddAllDocTemplates()\n    ' Get the assembly where the classes are defined\n    Dim assembly As Assembly = Assembly.GetExecutingAssembly()\n    \n    ' Get all types in the assembly\n    Dim types As Type() = assembly.GetTypes()\n    \n    ' Iterate over all types\n    For Each t As Type In types\n        ' Check if the type inherits from docMain and its name starts with "doc"\n        If t.IsSubclassOf(GetType(docMain)) AndAlso t.Name.StartsWith("doc") Then\n            ' Create an instance of the type\n            Dim instance As docMain = CType(Activator.CreateInstance(t), docMain)\n            ' Add the instance to the list\n            addTemplateToList(instance)\n        End If\n    Next\nEnd Sub