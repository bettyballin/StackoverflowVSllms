Imports System.Text.RegularExpressions\n\nModule Module1\n    Sub Main()\n        Dim calculationText As String = "0 50 100 200 400 218 9.8 ??? 6.65 6.31"\n        Dim positions() As Integer = {0, 50, 100, 200, 400, 218, 9.8, ???, 6.65, 6.31}\n\n        Dim result() As String = CustomSplit(calculationText, positions)\n        \n        For Each value As String In result\n            Console.WriteLine(value)\n        Next\n    End Sub\n\n    Function CustomSplit(ByVal input As String, ByVal positions() As Integer) As String()\n        Dim result(positions.Length - 1) As String\n        Dim currentIndex As Integer = 0\n\n        For i As Integer = 0 To positions.Length - 1\n            Dim nextIndex As Integer = positions(i)\n            If nextIndex <= input.Length Then\n                result(i) = input.Substring(currentIndex, nextIndex - currentIndex).Trim()\n                currentIndex = nextIndex\n            Else\n                result(i) = input.Substring(currentIndex).Trim()\n                Exit For\n            End If\n        Next\n\n        Return result\n    End Function\nEnd Module