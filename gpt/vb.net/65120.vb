Public Sub btnSubmit_click(ByVal sender As Object, ByVal e As System.EventArgs)\n    Dim queryString As String = "nextpage.aspx?"\n    Dim aBoat As String = Request("aBoat")\n    Dim bBoat As String = Request("bBoat")\n    Dim cBoat As String = Request("cBoat")\n\n    Dim queryParameters As New List(Of String)()\n\n    If Not String.IsNullOrEmpty(aBoat) Then\n        queryParameters.Add("aBoat=" & HttpUtility.UrlEncode(aBoat))\n    End If\n\n    If Not String.IsNullOrEmpty(bBoat) Then\n        queryParameters.Add("bBoat=" & HttpUtility.UrlEncode(bBoat))\n    End If\n\n    If Not String.IsNullOrEmpty(cBoat) Then\n        queryParameters.Add("cBoat=" & HttpUtility.UrlEncode(cBoat))\n    End If\n\n    If queryParameters.Count > 0 Then\n        queryString &= String.Join("&", queryParameters)\n    End If\n\n    Response.Redirect(queryString)\nEnd Sub