<%\n' Include your database connection settings\nDim conn, rs, sql\nSet conn = Server.CreateObject("ADODB.Connection")\nconn.Open "Provider=Microsoft.Jet.OLEDB.4.0;Data Source=path_to_your_database.mdb"\n\n' Function to load data into session variable\nSub LoadData()\n    sql = "SELECT * FROM YourTable"\n    Set rs = conn.Execute(sql)\n    \n    Dim data\n    data = rs.GetRows()\n    Session("DataSet") = data\n    rs.Close()\nEnd Sub\n\n' Check if data is already in session\nIf IsEmpty(Session("DataSet")) Then\n    Call LoadData()\nEnd If\n\n' Function to display data\nSub DisplayData()\n    Dim data\n    data = Session("DataSet")\n    \n    Response.Write "<table border='1'>"\n    For i = 0 To UBound(data, 2)\n        Response.Write "<tr>"\n        For j = 0 To UBound(data, 1)\n            Response.Write "<td>" & data(j, i) & "</td>"\n        Next\n        Response.Write "</tr>"\n    Next\n    Response.Write "</table>"\nEnd Sub\n\n' Example of manipulating data (delete first record)\nSub DeleteFirstRecord()\n    Dim data\n    data = Session("DataSet")\n    \n    ' Remove the first record\n    If UBound(data, 2) >= 0 Then\n        For i = 0 To UBound(data, 1)\n            data(i, 0) = Null\n        Next\n        ReDim Preserve data(UBound(data, 1), UBound(data, 2) - 1)\n    End If\n    \n    Session("DataSet") = data\nEnd Sub\n\n' Call the function to delete the first record (for demo purposes)\nCall DeleteFirstRecord()\n\n' Display the manipulated data\nCall DisplayData()\n\nconn.Close()\nSet conn = Nothing\n%>