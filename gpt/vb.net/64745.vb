Sub ExtractDescriptions()\n    Dim lastRow As Long\n    Dim i As Long\n    Dim inputText As String\n    Dim descStart As Long\n    Dim descEnd As Long\n    \n    lastRow = Cells(Rows.Count, 1).End(xlUp).Row\n    \n    For i = 1 To lastRow\n        inputText = Cells(i, 1).Value\n        descStart = InStrRev(inputText, """", InStrRev(inputText, """") - 1) + 1\n        descEnd = InStr(descStart, inputText, """") - 1\n        Cells(i, 2).Value = Mid(inputText, descStart, descEnd - descStart + 1)\n    Next i\nEnd Sub