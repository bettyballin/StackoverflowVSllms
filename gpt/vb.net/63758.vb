Imports Acrobat\nImports System.Runtime.InteropServices\n\nModule Module1\n\n    Sub Main()\n        Dim app As New AcroApp\n        Dim avDoc As CAcroAVDoc = New AcroAVDoc()\n        Dim pdfDoc As CAcroPDDoc\n        Dim inputFilePath As String = "C:\path\to\your\file.snp"\n        Dim outputFilePath As String = "C:\path\to\your\output.pdf"\n\n        ' Open the document\n        If avDoc.Open(inputFilePath, "") Then\n            pdfDoc = avDoc.GetPDDoc()\n\n            ' Save the document as PDF\n            pdfDoc.Save(PDSaveFlags.PDSaveFull, outputFilePath)\n\n            ' Close the document\n            avDoc.Close(True)\n            Marshal.ReleaseComObject(pdfDoc)\n        Else\n            Console.WriteLine("Failed to open the document.")\n        End If\n\n        ' Close Adobe Acrobat\n        app.Exit()\n        Marshal.ReleaseComObject(avDoc)\n        Marshal.ReleaseComObject(app)\n    End Sub\n\nEnd Module