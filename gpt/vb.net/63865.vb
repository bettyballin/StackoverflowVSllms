<%\nDim conn, cmd, rs, sql, param1, param2\n\n' Create and open a connection object\nSet conn = Server.CreateObject("ADODB.Connection")\nconn.Open "YourConnectionString"\n\n' Define your SQL query with placeholders for parameters\nsql = "SELECT * FROM YourTable WHERE Column1 = ? AND Column2 = ?"\n\n' Create a command object\nSet cmd = Server.CreateObject("ADODB.Command")\ncmd.ActiveConnection = conn\ncmd.CommandText = sql\ncmd.CommandType = adCmdText\n\n' Set parameter values\nparam1 = "Value1"\nparam2 = "Value2"\n\n' Append parameters to command object\ncmd.Parameters.Append cmd.CreateParameter("param1", adVarChar, adParamInput, 50, param1)\ncmd.Parameters.Append cmd.CreateParameter("param2", adVarChar, adParamInput, 50, param2)\n\n' Execute the query\nSet rs = cmd.Execute\n\n' Process the results\nDo While Not rs.EOF\n    Response.Write rs("ColumnName")\n    rs.MoveNext\nLoop\n\n' Clean up\nrs.Close\nSet rs = Nothing\nSet cmd = Nothing\nconn.Close\nSet conn = Nothing\n%>