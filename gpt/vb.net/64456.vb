Imports System.Globalization\n\nPublic Function GetWeekStartEndDates(weekNumber As Integer, year As Integer) As Tuple(Of DateTime, DateTime)\n    Dim jan1 As New DateTime(year, 1, 1)\n    Dim daysOffset As Integer = DayOfWeek.Thursday - jan1.DayOfWeek\n    Dim firstThursday As DateTime = jan1.AddDays(daysOffset)\n    Dim cal As Calendar = CultureInfo.CurrentCulture.Calendar\n    Dim firstWeek As DateTime = firstThursday.AddDays(-((cal.GetDayOfWeek(firstThursday) - DayOfWeek.Monday) + 3))\n    Dim resultStartDate As DateTime = firstWeek.AddDays((weekNumber - 1) * 7)\n    Dim resultEndDate As DateTime = resultStartDate.AddDays(6)\n    Return Tuple.Create(resultStartDate, resultEndDate)\nEnd Functio