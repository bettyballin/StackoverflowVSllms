Public Sub Bind(webForm As System.Web.UI.Page, bindingObjects As Dictionary(Of String, Object))\n    For Each ctl As Control In GetAllControls(webForm)\n        If ctl.ID IsNot Nothing AndAlso ctl.ID.Contains(".") Then\n            Dim parts = ctl.ID.Split("."c)\n            Dim instanceName As String = parts(0)\n            Dim propertyName As String = parts(1)\n\n            If bindingObjects.ContainsKey(instanceName) Then\n                Dim instance = bindingObjects(instanceName)\n                Dim propertyInfo = instance.GetType().GetProperty(propertyName)\n                If propertyInfo IsNot Nothing Then\n                    Dim value = propertyInfo.GetValue(instance, Nothing)\n                    If TypeOf ctl Is TextBox Then\n                        DirectCast(ctl, TextBox).Text = value.ToString()\n                    End If\n                End If\n            End If\n        End If\n    Next\nEnd Sub\n\nPrivate Function GetAllControls(control As Control) As IEnumerable(Of Control)\n    Dim controls As New List(Of Control)\n    controls.Add(control)\n    For Each childControl As Control In control.Controls\n        controls.AddRange(GetAllControls(childControl))\n    Next\n    Return controls\nEnd Functio