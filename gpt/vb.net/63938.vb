Dim ConnLocation, ConnDSN, ConnCreateCube, Connection\n\nConnLocation = "LOCATION=C:\test.cub;"\nConnDSN = "SOURCE_DSN=DSN=TEST;UID=test;PWD=pass;"\nConnCreateCube = "CREATECUBE=CREATE CUBE [TestCube] (DIMENSION [account_code]);"\n\n' Create and open the connection\nSet Connection = CreateObject("ADODB.Connection")\nConnection.Provider = "msolap.3" ' Ensure you have the correct MSOLAP provider\nConnection.ConnectionString = ConnLocation & ConnDSN\nConnection.Open\n\n' Execute the CREATECUBE command\nConnection.Execute ConnCreateCube\n\n' Clean up\nConnection.Close\nSet Connection = Nothing