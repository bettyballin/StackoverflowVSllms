Imports System.Data.SqlClient\nImports System.IO\n\nPublic Sub ReplaceImageWithEmpty()\n    Dim t_id As Integer = Convert.ToInt32(Request.QueryString("id"))\n    Dim emptyImagePath As String = Server.MapPath("~/images/empty.jpg") ' Path to the "empty" image\n\n    ' Read the image file into a byte array\n    Dim imageBytes As Byte() = File.ReadAllBytes(emptyImagePath)\n\n    ' Create SQL parameters\n    Dim picture As New SqlParameter("@picture", imageBytes)\n    Dim id As New SqlParameter("@id", t_id)\n\n    ' Use your connection string\n    Dim connString As String = "Your_Connection_String_Here"\n    \n    Using conn As New SqlConnection(connString)\n        conn.Open()\n        \n        Using myCommand As New SqlCommand("spChangeImage", conn)\n            myCommand.CommandType = CommandType.StoredProcedure\n\n            ' Add parameters to the command\n            myCommand.Parameters.Add(picture)\n            myCommand.Parameters.Add(id)\n\n            ' Execute the command\n            myCommand.ExecuteNonQuery()\n        End Using\n    End Using\nEnd Sub