Public Class MainWindow\n\n    Private Sub SaveDictionary()\n        Dim myDict As New Dictionary(Of String, String) From {\n            {"Key1", "Value1"},\n            {"Key2", "Value2"}\n        }\n        SettingsHelper.SaveDictionaryToSettings(myDict)\n    End Sub\n\n    Private Sub LoadDictionary()\n        Dim myDict As Dictionary(Of String, String) = SettingsHelper.GetDictionaryFromSettings()\n        For Each kvp As KeyValuePair(Of String, String) In myDict\n            Console.WriteLine($"Key: {kvp.Key}, Value: {kvp.Value}")\n        Next\n    End Sub\n\nEnd Class