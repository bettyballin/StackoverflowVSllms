Imports System.Management\n\nPublic Class Form1\n    Private Sub Form1_Load(sender As Object, e As EventArgs) Handles MyBase.Load\n        Dim processorID As String = GetProcessorID()\n        Dim machineID As String = GetMachineID()\n\n        MessageBox.Show("Processor ID: " & processorID)\n        MessageBox.Show("Machine ID: " & machineID)\n    End Sub\n\n    Private Function GetProcessorID() As String\n        Dim searcher As New ManagementObjectSearcher("select ProcessorId from Win32_Processor")\n        For Each obj As ManagementObject In searcher.Get()\n            Return obj("ProcessorId").ToString()\n        Next\n        Return String.Empty\n    End Function\n\n    Private Function GetMachineID() As String\n        Dim searcher As New ManagementObjectSearcher("select SerialNumber from Win32_BIOS")\n        For Each obj As ManagementObject In searcher.Get()\n            Return obj("SerialNumber").ToString()\n        Next\n        Return String.Empty\n    End Function\nEnd Class