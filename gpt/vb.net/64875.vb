Public Function ExecuteCalculation(scriptId As Integer, numbers As List(Of Double)) As Double\n       Dim result As Double = 0.0\n       Using conn As New SqlConnection("your_connection_string")\n           conn.Open()\n           Dim cmd As New SqlCommand()\n           cmd.Connection = conn\n           cmd.CommandType = CommandType.Text\n\n           ' Load the script from the database\n           cmd.CommandText = "SELECT Script FROM CalculationScripts WHERE Id = @scriptId"\n           cmd.Parameters.AddWithValue("@scriptId", scriptId)\n           Dim script As String = cmd.ExecuteScalar().ToString()\n\n           ' Execute the script\n           cmd.CommandText = script\n           cmd.Parameters.Clear()\n           Dim numbersTable As New DataTable()\n           numbersTable.Columns.Add("value", GetType(Double))\n           For Each number In numbers\n               numbersTable.Rows.Add(number)\n           Next\n           cmd.Parameters.AddWithValue("@numbers", numbersTable)\n           result = Convert.ToDouble(cmd.ExecuteScalar())\n       End Using\n\n       Return result\n   End Functio