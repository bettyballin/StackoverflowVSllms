Dim client As New TcpClient()\nclient.Connect(server, port)\n\nDim stream As NetworkStream = client.GetStream()\nDim buffer(1024) As Byte ' Adjust buffer size as needed\nDim requestData As Byte() = Encoding.ASCII.GetBytes(request)\nDim responseData As New StringBuilder()\n\n' Send the request\nstream.Write(requestData, 0, requestData.Length)\n\n' Read the response\nDim bytesRead As Integer\n\nDo\n    bytesRead = stream.Read(buffer, 0, buffer.Length)\n    responseData.Append(Encoding.ASCII.GetString(buffer, 0, bytesRead))\nLoop While stream.DataAvailable\n\n' Output buffer and return results...\nDim result As String = responseData.ToString()\n\n' Clean up\nstream.Close()\nclient.Close()