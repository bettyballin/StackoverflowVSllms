Dim conn, cmd, bytes, byteArray\n\n' Create an ADO connection\nSet conn = CreateObject("ADODB.Connection")\nconn.Open "your_connection_string_here"\n\n' Create an ADO command\nSet cmd = CreateObject("ADODB.Command")\ncmd.ActiveConnection = conn\ncmd.CommandText = "INSERT INTO rawtest (byt) VALUES (?)"\n\n' Prepare the byte array (example data)\nbytes = Array(&H12, &H34, &H56, &H78, &H90, &H12, &H34, &H56, &H78, &H90, &H12, &H34)\n\n' Convert the byte array to a safe array\nSet byteArray = CreateObject("System.Byte[]")\n\nFor i = LBound(bytes) To UBound(bytes)\n    byteArray.Add bytes(i)\nNext\n\n' Create a parameter for the binary data\nSet param = cmd.CreateParameter(, 204, 1, LenB(byteArray), byteArray) ' 204 = adVarBinary, 1 = adParamInput\ncmd.Parameters.Append param\n\n' Execute the command\ncmd.Execute\n\n' Clean up\nconn.Close\nSet cmd = Nothing\nSet conn = Nothing