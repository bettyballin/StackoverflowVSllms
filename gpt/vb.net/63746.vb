Imports Microsoft.VisualBasic.FileIO\nImports System.Collections.Generic\n\nModule Module1\n    Sub Main()\n        Dim filePath As String = "path/to/your/csvfile.csv"\n        Dim records As New List(Of List(Of String))()\n\n        Using parser As New TextFieldParser(filePath)\n            parser.Delimiters = New String() {","}\n            parser.HasFieldsEnclosedInQuotes = True\n            parser.TrimWhiteSpace = True\n\n            While Not parser.EndOfData\n                Try\n                    Dim fields As String() = parser.ReadFields()\n                    records.Add(New List(Of String)(fields))\n                Catch ex As MalformedLineException\n                    Console.WriteLine($"Line {ex.Message} is invalid. Skipping.")\n                End Try\n            End While\n        End Using\n\n        ' Process records as needed\n        For Each record As List(Of String) In records\n            For Each field As String In record\n                Console.Write(field & " | ")\n            Next\n            Console.WriteLine()\n        Next\n    End Sub\nEnd Module