Imports Microsoft.Office.Interop.Excel\nImports System.Runtime.InteropServices\n\nPublic Class Form1\n    Dim eApp As Excel.Application = Nothing\n    Dim w As Excel.Workbook = Nothing\n\n    Try\n        eApp = New Excel.Application\n        w = eApp.Workbooks.Open("path.xls", ReadOnly:=True)\n        \n        ' .. Processing Code ..\n\n    Catch ex As Exception\n        ' Handle exception\n    Finally\n        ' Properly close workbook\n        If w IsNot Nothing Then\n            w.Close(False)\n            Marshal.ReleaseComObject(w)\n            w = Nothing\n        End If\n\n        ' Properly quit Excel application\n        If eApp IsNot Nothing Then\n            eApp.Workbooks.Close()\n            eApp.Quit()\n            Marshal.ReleaseComObject(eApp)\n            eApp = Nothing\n        End If\n\n        ' Force garbage collection to release any remaining COM objects\n        GC.Collect()\n        GC.WaitForPendingFinalizers()\n    End Try\nEnd Class