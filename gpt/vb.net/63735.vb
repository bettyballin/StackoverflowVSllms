' Example using DiffPlex library\nImports DiffPlex\nImports DiffPlex.DiffBuilder\n\nSub Main()\n    Dim oldText As String = "The quick brown fox jumps over the lazy dog."\n    Dim newText As String = "The swift brown fox jumps over the lazy dog."\n\n    Dim diffBuilder = New InlineDiffBuilder(New Differ())\n    Dim diffResult = diffBuilder.BuildDiffModel(oldText, newText)\n\n    For Each line In diffResult.Lines\n        Select Case line.Type\n            Case DiffPlex.DiffBuilder.Model.ChangeType.Inserted\n                Console.WriteLine("+ " & line.Text)\n            Case DiffPlex.DiffBuilder.Model.ChangeType.Deleted\n                Console.WriteLine("- " & line.Text)\n            Case Else\n                Console.WriteLine("  " & line.Text)\n        End Select\n    Next\nEnd Sub