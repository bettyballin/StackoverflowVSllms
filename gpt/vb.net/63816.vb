Dim KeySortPairs = From d In MyDataTable.AsEnumerable()\n                   Group d By Key = d("LookupValue").ToString() Into Group\n                   Select Key, Group\n\nDim Duplicates = From grp In KeySortPairs\n                 Where grp.Group.Select(Function(x) x("SortOrder").ToString()).Distinct().Count() > 1\n                 Select grp.Key\n\nFor Each Entry In Duplicates\n    Console.WriteLine("Multiple Sort Orders for LookupValue: " & Entry)\nNext