Public Class SumFormula\n    Implements IFormula\n\n    Public Shared ReadOnly Property XMLTag() As String\n        Get\n            Return "sum"\n        End Get\n    End Property\n\n    Private X As IFormula\n    Private Y As IFormula\n\n    Public Sub New(ByVal xmlText As Xml.XmlNode)\n        ' Code to read the sum parameters from XML.'\n        ' Assuming the XML node contains elements for X and Y:\n        Me.X = FormulaFactory.ConstructFormula(xmlText.SelectSingleNode("X"))\n        Me.Y = FormulaFactory.ConstructFormula(xmlText.SelectSingleNode("Y"))\n    End Sub\n\n    Public Function Evaluate(ByVal p As Parameters) As Result Implements IFormula.Evaluate\n        Return X.Evaluate(p) + Y.Evaluate(p)\n    End Function\n\n    Public Function ToXml() As String Implements IFormula.ToXml\n        Return $"<sum>{X.ToXml()}{Y.ToXml()}</sum>"\n    End Function\nEnd Class