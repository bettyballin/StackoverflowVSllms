Imports System.IO\n\nDim filePath As String = "path_to_your_file.txt" ' Replace with actual file path\nDim lines As String() = File.ReadAllLines(filePath)\nDim dataList As New List(Of Double)()\n\nFor Each line As String In lines\n    Dim fields As String() = line.Split(New Char() {" "c, vbTab}, StringSplitOptions.RemoveEmptyEntries)\n    For Each field As String In fields\n        If field <> "????" Then\n            Dim value As Double\n            If Double.TryParse(field, value) Then\n                dataList.Add(value)\n            End If\n        End If\n    Next\nNext\n\nDim dataArray As Double() = dataList.ToArray()