Public Class Form1\n    Private Sub PictureBox_MouseUp(sender As Object, e As MouseEventArgs) Handles PictureBox1.MouseUp\n        ' Define the target position\n        Dim targetPosition As New Point(100, 100)\n        ' Define the snapping threshold (distance to target)\n        Dim snapThreshold As Integer = 50\n\n        ' Get the current position of the PictureBox\n        Dim currentPos As Point = PictureBox1.Location\n\n        ' Calculate the distance between the current position and the target position\n        Dim distance As Double = Math.Sqrt(Math.Pow(currentPos.X - targetPosition.X, 2) + Math.Pow(currentPos.Y - targetPosition.Y, 2))\n\n        ' If the distance is within the threshold, snap the PictureBox to the target position\n        If distance <= snapThreshold Then\n            PictureBox1.Location = targetPosition\n        End If\n    End Sub\nEnd Class