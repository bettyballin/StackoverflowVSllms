Dim conn As ADODB.Connection\nDim cmd As ADODB.Command\nDim param As ADODB.Parameter\nDim bytes() As Byte\n\n' Initialize bytes array with example data\nbytes = Array(&H12, &H34, &H56, &H78, &H90, &H12, &H34, &H56, &H78, &H90, &H12, &H34)\n\n' Create and open the ADO connection\nSet conn = New ADODB.Connection\nconn.Open "your_connection_string_here"\n\n' Create and configure the ADO command\nSet cmd = New ADODB.Command\ncmd.ActiveConnection = conn\ncmd.CommandText = "INSERT INTO rawtest (byt) VALUES (?)"\n\n' Create the parameter for the binary data\nSet param = cmd.CreateParameter(, adVarBinary, adParamInput, UBound(bytes) - LBound(bytes) + 1, bytes)\ncmd.Parameters.Append param\n\n' Execute the command\ncmd.Execute\n\n' Clean up\nconn.Close\nSet cmd = Nothing\nSet conn = Nothing