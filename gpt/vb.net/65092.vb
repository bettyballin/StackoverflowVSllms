Imports DTS\n\nModule Module1\n    Sub Main()\n        ' Initialize DTS application object\n        Dim dtsApp As New Application()\n\n        ' Load the DTS package\n        Dim dtsPkg As Package\n        dtsPkg = dtsApp.LoadFromStorageFile("C:\Path\To\Your\DTS_Package.dts", Nothing, Nothing, Nothing)\n\n        ' Execute the DTS package\n        Dim pkgResult As DTSExecResult\n        pkgResult = dtsPkg.Execute()\n\n        ' Check the result of the execution\n        If pkgResult = DTSExecResult.Success Then\n            Console.WriteLine("DTS package executed successfully.")\n        Else\n            Console.WriteLine("DTS package execution failed.")\n        End If\n\n        ' Release resources\n        dtsPkg.UnInitialize()\n    End Sub\nEnd Module