Imports Word = Microsoft.Office.Interop.Word\n\nModule Module1\n    Sub Main()\n        Dim app As New Word.Application\n        Dim doc As Word.Document\n        doc = app.Documents.Open("C:\myDoc.doc")\n\n        ' Access the checkbox form field by its bookmark name\n        Dim chkBox As Word.FormField\n        chkBox = doc.FormFields("MyCheckbox")\n\n        ' Check if the checkbox is checked or not\n        Dim isChecked As Boolean\n        isChecked = chkBox.CheckBox.Value\n\n        ' Output the result\n        Console.WriteLine("Checkbox 'MyCheckbox' is " & If(isChecked, "checked", "unchecked"))\n\n        ' Clean up\n        doc.Close(False)\n        app.Quit()\n    End Sub\nEnd Module