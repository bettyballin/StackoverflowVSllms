' Custom part of the partial class\nPartial Class MyPartialClass\n    Implements INotifyPropertyChanged\n\n    ' Custom property that depends on an auto-generated property\n    Private _myProperty As String\n    Public Property MyProperty As String\n        Get\n            Return _myProperty\n        End Get\n        Set(value As String)\n            If _myProperty <> value Then\n                _myProperty = value\n                RaiseEvent PropertyChanged(Me, New PropertyChangedEventArgs(NameOf(MyProperty)))\n            End If\n        End Set\n    End Property\n\n    ' Constructor to subscribe to the PropertyChanged event\n    Public Sub New()\n        AddHandler Me.PropertyChanged, AddressOf Me.MyProperty_PropertyChangedHandler\n    End Sub\n\n    ' Event handler for the PropertyChanged event\n    Private Sub MyProperty_PropertyChangedHandler(ByVal sender As Object, ByVal e As PropertyChangedEventArgs)\n        If e.PropertyName = "AutoGenProperty" Then\n            ' Raise PropertyChanged event for the custom property\n            RaiseEvent PropertyChanged(Me, New PropertyChangedEventArgs(NameOf(MyProperty)))\n        End If\n    End Sub\n\n    ' Implementation of INotifyPropertyChanged interface\n    Public Event PropertyChanged As PropertyChangedEventHandler Implements INotifyPropertyChanged.PropertyChanged\nEnd Class