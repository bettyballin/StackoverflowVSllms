Sub ScanBox()\n    ' Fetch the next available location\n    Dim query As String = "SELECT TOP 1 * FROM BoxLocations WHERE Status = 'Empty' ORDER BY Timestamp"\n    Dim nextLocation As DataRow = ExecuteQuery(query) ' Your method to execute query and fetch the result\n\n    If nextLocation IsNot Nothing Then\n        ' Update the status to 'Filled'\n        Dim updateQuery As String = $"UPDATE BoxLocations SET Status = 'Filled', Timestamp = GETDATE() WHERE Floor = {nextLocation("Floor")} AND Row = {nextLocation("Row")} AND Column = {nextLocation("Column")} AND Shelf = {nextLocation("Shelf")} AND Box = {nextLocation("Box")}"\n        ExecuteQuery(updateQuery) ' Your method to execute update\n\n        ' Print the label\n        Dim label As String = $"f{nextLocation("Floor")}r{nextLocation("Row")}c{nextLocation("Column")}s{nextLocation("Shelf")}b{nextLocation("Box")}"\n        PrintLabel(label) ' Your method to print the label\n    End If\nEnd Sub