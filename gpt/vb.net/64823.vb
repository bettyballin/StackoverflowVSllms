Imports ZXing\nImports ZXing.Common\nImports System.Drawing\n\nModule Module1\n    Sub Main()\n        ' The text to encode\n        Dim textToEncode As String = "12345"\n\n        ' Create a barcode writer instance\n        Dim barcodeWriter As New BarcodeWriter()\n        barcodeWriter.Format = BarcodeFormat.CODE_39\n        barcodeWriter.Options = New EncodingOptions() With {\n            .Width = 300,\n            .Height = 100,\n            .Margin = 10\n        }\n\n        ' Generate the barcode\n        Dim barcodeBitmap As Bitmap = barcodeWriter.Write(textToEncode)\n\n        ' Save the barcode to a file\n        barcodeBitmap.Save("Code39Barcode.png", Imaging.ImageFormat.Png)\n\n        ' Display a message to indicate the barcode was generated\n        Console.WriteLine("Barcode generated and saved as Code39Barcode.png")\n    End Sub\nEnd Module