Dim oCookie As HttpCookie = Request.Cookies("authInfo")\nIf oCookie IsNot Nothing Then\n    Dim expirationValue As String = oCookie.Values("Expires")\n    If Not String.IsNullOrEmpty(expirationValue) Then\n        Dim expiration As DateTime\n        If DateTime.TryParse(expirationValue, expiration) Then\n            Response.Output.WriteLine("Cookie Expires: " & expiration.ToString())\n        Else\n            Response.Output.WriteLine("Invalid expiration value in cookie.")\n        End If\n    Else\n        Response.Output.WriteLine("No expiration value in cookie.")\n    End If\nElse\n    Response.Output.WriteLine("Cookie not found.")\nEnd If