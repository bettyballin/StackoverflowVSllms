Public Class W3CError\n    Public Property Line As Integer\n    Public Property Col As Integer\n    Public Property Message As String\n    Public Property MessageId As String\n    Public Property Explanation As String\n    Public Property Source As String\nEnd Class\n\nSub ExtractXmlData(xmlContent As String)\n    Dim xmlDoc As New XmlDocument()\n    xmlDoc.LoadXml(xmlContent)\n\n    Dim nsmgr As New XmlNamespaceManager(xmlDoc.NameTable)\n    nsmgr.AddNamespace("env", "http://www.w3.org/2003/05/soap-envelope")\n    nsmgr.AddNamespace("m", "http://www.w3.org/2005/10/markup-validator")\n\n    Dim uri As String = xmlDoc.SelectSingleNode("//m:uri", nsmgr).InnerText\n    Dim checkedby As String = xmlDoc.SelectSingleNode("//m:checkedby", nsmgr).InnerText\n    Dim doctype As String = xmlDoc.SelectSingleNode("//m:doctype", nsmgr).InnerText\n    Dim charset As String = xmlDoc.SelectSingleNode("//m:charset", nsmgr).InnerText\n    Dim validity As Boolean = Boolean.Parse(xmlDoc.SelectSingleNode("//m:validity", nsmgr).InnerText)\n\n    Dim errorList As New List(Of W3CError)\n    For Each errorNode As XmlNode In xmlDoc.SelectNodes("//m:errors/m:errorlist/m:error", nsmgr)\n        Dim errorItem As New W3CError With {\n            .Line = Integer.Parse(errorNode.SelectSingleNode("m:line", nsmgr).InnerText),\n            .Col = Integer.Parse(errorNode.SelectSingleNode("m:col", nsmgr).InnerText),\n            .Message = errorNode.SelectSingleNode("m:message", nsmgr).InnerText,\n            .MessageId = errorNode.SelectSingleNode("m:messageid", nsmgr).InnerText,\n            .Explanation = errorNode.SelectSingleNode("m:explanation", nsmgr).InnerText,\n            .Source = errorNode.SelectSingleNode("m:source", nsmgr).InnerText\n        }\n        errorList.Add(errorItem)\n    Next\n\n    Dim warningList As New List(Of W3CError)\n    For Each warningNode As XmlNode In xmlDoc.SelectNodes("//m:warnings/m:warninglist/m:warning", nsmgr)\n        Dim warningItem As New W3CError With {\n            .Line = Integer.Parse(warningNode.SelectSingleNode("m:line", nsmgr).InnerText),\n            .Col = Integer.Parse(warningNode.SelectSingleNode("m:col", nsmgr).InnerText),\n            .Message = warningNode.SelectSingleNode("m:message", nsmgr).InnerText,\n            .MessageId = warningNode.SelectSingleNode("m:messageid", nsmgr).InnerText,\n            .Explanation = warningNode.SelectSingleNode("m:explanation", nsmgr).InnerText,\n            .Source = warningNode.SelectSingleNode("m:source", nsmgr).InnerText\n        }\n        warningList.Add(warningItem)\n    Next\n\n    ' Output or use the extracted values\n    Console.WriteLine($"Uri: {uri}")\n    Console.WriteLine($"Checkedby: {checkedby}")\n    Console.WriteLine($"Doctype: {doctype}")\n    Console.WriteLine($"Charset: {charset}")\n    Console.WriteLine($"Validity: {validity}")\n    Console.WriteLine($"Error Count: {errorList.Count}")\n    Console.WriteLine($"Warning Count: {warningList.Count}")\nEnd Sub