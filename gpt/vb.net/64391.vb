Private Function LoadSaved() As Boolean\n    ' Loads saved clippings if the user wants us to\n    Dim ZomgSavedClips As StringCollection = My.Settings.SavedClips\n\n    ' Check if ZomgSavedClips is Nothing and initialize if necessary\n    If ZomgSavedClips Is Nothing Then\n        ZomgSavedClips = New StringCollection()\n        My.Settings.SavedClips = ZomgSavedClips\n    End If\n\n    ' Add a blank piece of text to avoid empty collection issues\n    ZomgSavedClips.Add(" ")\n\n    Dim i As Integer = 0\n    While i < ZomgSavedClips.Count\n        ClipListings.Rows.Add(ZomgSavedClips(i))\n        i += 1 ' Ensure we increment the counter to avoid an infinite loop\n    End While\n\n    Return True\nEnd Functio