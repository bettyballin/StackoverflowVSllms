Dim ListHours()\nHoursInCurrentDay = 24\nRedim ListHours(HoursInCurrentDay - 1)\nFor Hours = 0 To 23\n    ListHours(Hours) = Hours\nNext\nSQLARR2 = ListHours\nList = "["\n\nFor x = 0 To UBound(SQLARR2)\n    SQL = "SELECT COUNT(DISTINCT VISITORIP) AS TOTALVISITS"\n    SQL = SQL & " FROM STATS"\n    SQL = SQL & " WHERE DATEPART(hh, DATEENTERED) = " & SQLARR2(x)\n    SQL = SQL & " AND DATEENTERED >= DATEADD(hour, -24, GETDATE())"\n    Set objVisits = objConn.Execute(SQL)\n    \n    If Not objVisits.EOF Then\n        List = List & "[" & SQLARR2(x) & "," & objVisits("TOTALVISITS") & "],"\n    End If\nNext\n\n' Remove the trailing comma and close the array\nIf Right(List, 1) = "," Then\n    List = Left(List, Len(List) - 1)\nEnd If\nList = List & "]"\n\nResponse.Write List