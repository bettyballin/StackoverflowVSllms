Public Class FakeRepository\n    Private _clients As List(Of Client)\n    Private _administrators As List(Of Administrator)\n    Private _clientAdministrators As List(Of ClientAdministrator)\n\n    Public Sub New()\n        ' Initialize lists\n        _clients = New List(Of Client)()\n        _administrators = New List(Of Administrator)()\n        _clientAdministrators = New List(Of ClientAdministrator)()\n\n        ' Create some fake data\n        Dim admin1 As New Administrator With {.ID = 1, .UserName = "Admin1"}\n        Dim admin2 As New Administrator With {.ID = 2, .UserName = "Admin2"}\n\n        Dim client1 As New Client With {.ID = 1, .ClientName = "Client1"}\n        Dim client2 As New Client With {.ID = 2, .ClientName = "Client2"}\n\n        ' Establish many-to-many relationships\n        Dim ca1 As New ClientAdministrator With {.Client = client1, .Administrator = admin1}\n        Dim ca2 As New ClientAdministrator With {.Client = client1, .Administrator = admin2}\n        Dim ca3 As New ClientAdministrator With {.Client = client2, .Administrator = admin1}\n\n        ' Add to lists\n        _clients.Add(client1)\n        _clients.Add(client2)\n        _administrators.Add(admin1)\n        _administrators.Add(admin2)\n        _clientAdministrators.Add(ca1)\n        _clientAdministrators.Add(ca2)\n        _clientAdministrators.Add(ca3)\n\n        ' Set up navigation properties\n        client1.ClientAdministrators.Add(ca1)\n        client1.ClientAdministrators.Add(ca2)\n        client2.ClientAdministrators.Add(ca3)\n        admin1.ClientAdministrators.Add(ca1)\n        admin1.ClientAdministrators.Add(ca3)\n        admin2.ClientAdministrators.Add(ca2)\n    End Sub\n\n    Public Function GetAllClients() As List(Of Client)\n        Return _clients\n    End Function\nEnd Class