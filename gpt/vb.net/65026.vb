Module Module1\n    Sub Main()\n        Dim sentence As String = "This is a sample sentence with words like and, or, to, a, no, with, for etc."\n        Dim wordsToRemove As List(Of String) = New List(Of String) From {"and", "or", "to", "a", "no", "with", "for"}\n\n        For Each word In wordsToRemove\n            sentence = sentence.Replace(" " & word & " ", " ")\n        Next\n\n        Console.WriteLine(sentence)\n    End Sub\nEnd Module