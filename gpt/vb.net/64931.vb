Imports System.Data.SqlClient\n\nPartial Class Tags\n    Inherits System.Web.UI.Page\n\n    Protected Sub btnSubmit_Click(ByVal sender As Object, ByVal e As EventArgs)\n        Dim tags As String = txtTags.Text\n        Dim tagArray As String() = tags.Split(" "c)\n\n        Using conn As New SqlConnection("your_connection_string_here")\n            conn.Open()\n            For Each tag As String In tagArray\n                If Not String.IsNullOrWhiteSpace(tag) Then\n                    Dim cmd As New SqlCommand("INSERT INTO Tags (TagName) VALUES (@TagName)", conn)\n                    cmd.Parameters.AddWithValue("@TagName", tag.Trim())\n                    cmd.ExecuteNonQuery()\n                End If\n            Next\n        End Using\n    End Sub\nEnd Class