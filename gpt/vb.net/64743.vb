Dim db As DAO.Database\nDim rs As DAO.Recordset\nDim sql As String\nDim recordIDList As String\n\nrecordIDList = "1, 2, 3, 4" ' List of RecordIDs as a comma-separated string\n\nsql = "SELECT * FROM Table WHERE RecordID IN (" & recordIDList & ")"\n\nSet db = CurrentDb\nSet rs = db.OpenRecordset(sql)\n\nDo While Not rs.EOF\n    ' Process result set\n    rs.MoveNext\nLoop\n\nrs.Close\nSet rs = Nothing\nSet db = Nothing