Sub BatchInsert()\n    Dim conn As Object\n    Dim rs As Object\n    Dim i As Long\n    Dim j As Long\n    Dim sql As String\n\n    ' Create a new ADODB connection\n    Set conn = CreateObject("ADODB.Connection")\n    conn.Open "Provider=SQLOLEDB;Data Source=your_server_name;Initial Catalog=your_database_name;User ID=your_user_id;Password=your_password;"\n\n    ' Loop to insert data in batches\n    For i = 1 To 100\n        ' Begin transaction\n        conn.BeginTrans\n\n        ' Insert 1000 records in each batch\n        For j = 1 To 1000\n            sql = "INSERT INTO YourTableName (YourSequentialGUIDColumn, OtherColumn) VALUES (NEWSEQUENTIALID(), 'SomeValue')"\n            conn.Execute sql\n        Next j\n\n        ' Commit transaction\n        conn.CommitTrans\n    Next i\n\n    ' Clean up\n    conn.Close\n    Set conn = Nothing\nEnd Sub