Imports System.Reflection\nImports System.Resources\nImports System.Drawing\n\nPublic Sub PopulateImageListFromResources()\n    Dim imageList As New ImageList()\n\n    ' Get the type of the resource class (replace My.Resources.MyImages with your actual resource class)\n    Dim resourceType As Type = GetType(My.Resources.MyImages)\n    ' Get all properties of the resource class\n    Dim properties As PropertyInfo() = resourceType.GetProperties(BindingFlags.NonPublic Or BindingFlags.Public Or BindingFlags.Static)\n\n    For Each prop As PropertyInfo In properties\n        ' Check if the property is of type Bitmap\n        If prop.PropertyType Is GetType(Bitmap) Then\n            Dim resourceName As String = prop.Name\n            Dim resourceImage As Bitmap = CType(prop.GetValue(Nothing, Nothing), Bitmap)\n            imageList.Images.Add(resourceName, resourceImage)\n        End If\n    Next\nEnd Sub