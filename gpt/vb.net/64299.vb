Sub Main()\n\n    Dim i As Integer\n    Dim b As Boolean\n\n    ' Implicit conversion\n    i = 1\n    b = i\n    i = b\n    Console.WriteLine(i)  ' This will print -1 (because b is True)\n\n    ' Explicit conversion using Convert.ToInt32\n    i = Convert.ToInt32(b)\n    Console.WriteLine(i)  ' This will print 1 (because b is True)\n\n    ' TryParse example, note that "True" cannot be parsed to an integer\n    Dim success As Boolean = Int32.TryParse("True", i)\n    Console.WriteLine(i)  ' This will print 0 because TryParse fails and sets i to 0\n\nEnd Sub