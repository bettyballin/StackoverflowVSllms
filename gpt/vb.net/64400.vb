Imports System.Data.SqlClient\n\n' Assuming you have a connection string defined\nDim connectionString As String = "your_connection_string_here"\n\nmySample.Tags = tagsTextBox.Text\nDim tags As String = mySample.Tags\nDim tagarray() As String\nDim count As Integer\ntagarray = tags.Split(" ")\n\nUsing connection As New SqlConnection(connectionString)\n    connection.Open()\n\n    For count = 0 To tagarray.Length - 1\n        Dim tag As String = tagarray(count).Trim()\n\n        ' Ensure the tag is not empty before inserting\n        If Not String.IsNullOrEmpty(tag) Then\n            Dim query As String = "INSERT INTO YourTableName (TagColumnName) VALUES (@Tag)"\n            Using command As New SqlCommand(query, connection)\n                command.Parameters.AddWithValue("@Tag", tag)\n                command.ExecuteNonQuery()\n            End Using\n        End If\n    Next\nEnd Using