oXml = XmlReader.Create(path, oXmlSettings)\nWhile oXml.Read()\n    Select Case oXml.NodeType\n        Case XmlNodeType.Element\n            ' Read Element as-is\n            If taglist.Contains(oXml.Name) Then\n                stringbuilder.Append(oXml.ReadOuterXml())\n                ' Skip any subsequent text or end elements related to the processed element\n                While oXml.NodeType = XmlNodeType.Text OrElse oXml.NodeType = XmlNodeType.EndElement\n                    oXml.Read()\n                End While\n            End If\n        Case XmlNodeType.Text\n            stringbuilder.Append(oXml.Value)\n    End Select\nEnd While