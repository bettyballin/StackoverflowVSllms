Private Sub ShowCustomMessageBox()\n    Dim msgBox As New CustomMessageBox()\n    msgBox.Message = "Your custom message here"\n    msgBox.Button1Text = "Custom Yes"\n    msgBox.Button2Text = "Custom No"\n    msgBox.ShowDialog()\n\n    If msgBox.Result = DialogResult.Yes Then\n        MessageBox.Show("You clicked Custom Yes")\n    ElseIf msgBox.Result = DialogResult.No Then\n        MessageBox.Show("You clicked Custom No")\n    End If\nEnd Sub