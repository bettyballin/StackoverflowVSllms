Public Class Location\n    Private _Address1 As String\n    Private _Address2 As String\n    Private _City As String\n    Private _State As String\n    Private _ZIP As String\n\n    Public Function isValid() As Boolean\n        ' Check if "City" and "State" are both provided\n        If Not String.IsNullOrEmpty(_City) AndAlso Not String.IsNullOrEmpty(_State) Then\n            Return True\n        End If\n        \n        ' Check if "ZIP" is provided\n        If Not String.IsNullOrEmpty(_ZIP) Then\n            Return True\n        End If\n        \n        ' Check if "Address1" and "City" are both provided\n        If Not String.IsNullOrEmpty(_Address1) AndAlso Not String.IsNullOrEmpty(_City) Then\n            Return True\n        End If\n\n        ' Check if "Address1" and "ZIP" are both provided\n        If Not String.IsNullOrEmpty(_Address1) AndAlso Not String.IsNullOrEmpty(_ZIP) Then\n            Return True\n        End If\n\n        ' If none of the above conditions are met, the address is not valid\n        Return False\n    End Function\nEnd Class