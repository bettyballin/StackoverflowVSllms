Imports System.Drawing\nImports System.Drawing.Imaging\n\nModule Module1\n    Sub Main()\n        ' Load the multi-page TIFF file\n        Dim tiffImage As Image = Image.FromFile("path_to_tiff_file.tiff")\n\n        ' Get the number of frames in the TIFF\n        Dim frameCount As Integer = tiffImage.GetFrameCount(FrameDimension.Page)\n\n        For i As Integer = 0 To frameCount - 1\n            ' Select the frame\n            tiffImage.SelectActiveFrame(FrameDimension.Page, i)\n            \n            ' Create a new bitmap from the selected frame\n            Dim frameBitmap As New Bitmap(tiffImage)\n\n            ' Save the frame as a JPEG\n            frameBitmap.Save("output_path_" & i & ".jpg", ImageFormat.Jpeg)\n        Next\n\n        ' Clean up\n        tiffImage.Dispose()\n    End Sub\nEnd Module