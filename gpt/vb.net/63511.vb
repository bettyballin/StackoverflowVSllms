Sub test(ByVal path As String)\n    Dim oData As GSDataObject = GetDataObj(path)\n    EmptyComboBoxes()\n    Dim oDT As New Data.DataTable\n    Try\n        Dim t = From r In oData.GetTable(String.Format("SELECT * FROM {0}gsobj\paths ORDER BY keyid", AddBS(path))) _\n                Select New With {\n                    .key_code = r("key_code").ToString.Trim(),\n                    .descript = r("descript").ToString.Trim()\n                }\n\n        If t.Any() Then\n            oDT.Columns.Add("key_code", GetType(String))\n            oDT.Columns.Add("descript", GetType(String))\n\n            For Each item In t\n                Dim row As DataRow = oDT.NewRow()\n                row("key_code") = item.key_code\n                row("descript") = item.descript\n                oDT.Rows.Add(row)\n            Next\n\n            dataPathComboBox.DataSource = oDT\n            dataPathComboBox.DisplayMember = "descript"\n            dataPathComboBox.ValueMember = "key_code"\n            dataPathComboBox.SelectedIndex = 0\n            dataPathComboBox.Enabled = True\n        End If\n    Catch ex As Exception\n        ' Handle the exception (e.g., log it or display a message)\n    End Try\nEnd Sub