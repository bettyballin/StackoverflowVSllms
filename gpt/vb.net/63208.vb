Sub InsertNode(ByVal doc As XmlDocument)\n    Dim City As XmlNode = doc.DocumentElement\n\n    ' Check if the store element with the attribute test="test.doc" exists\n    Dim storeNode As XmlNode = City.SelectSingleNode("store[@test='test.doc']")\n\n    If storeNode Is Nothing Then\n        ' Create and add the new store element if it does not exist\n        Dim Location As XmlElement = doc.CreateElement("store")\n        Location.SetAttribute("test", "test.doc")\n\n        Dim books As XmlElement = doc.CreateElement("books")\n        books.InnerXml = "New Words"\n        Location.AppendChild(books)\n\n        City.AppendChild(Location)\n    End If\nEnd Sub 'InsertNode