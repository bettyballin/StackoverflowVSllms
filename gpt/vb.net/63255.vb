Imports System.IO\n\nModule Module1\n\n    Sub Main()\n        Try\n            Dim sourcePdb As String = "path\to\obj\debug\yourproject.pdb"\n            Dim destPdb As String = "path\to\output\folder\yourproject.pdb"\n            Dim sourceXml As String = "path\to\obj\debug\yourproject.xml"\n            Dim destXml As String = "path\to\output\folder\yourproject.xml"\n\n            File.Copy(sourcePdb, destPdb, True)\n            File.Copy(sourceXml, destXml, True)\n\n            Console.WriteLine("Files copied successfully.")\n        Catch ex As Exception\n            Console.WriteLine("Error: " & ex.Message)\n        End Try\n    End Sub\n\nEnd Module