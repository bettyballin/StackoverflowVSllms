Module ExampleModule\n    Sub Main()\n        Dim value As Integer = 10\n        ModifyByVal(value)\n        Console.WriteLine("ByVal: " & value) ' Output will be 10\n\n        ModifyByRef(value)\n        Console.WriteLine("ByRef: " & value) ' Output will be 20\n    End Sub\n\n    Sub ModifyByVal(ByVal number As Integer)\n        number = 20\n    End Sub\n\n    Sub ModifyByRef(ByRef number As Integer)\n        number = 20\n    End Sub\nEnd Module