Dim emfFile As String = Server.MapPath("/FileName.emf")\nDim pngFile As String = Server.MapPath("/FileName.png")\n\nUsing stream As New FileStream(emfFile, FileMode.Open, FileAccess.Read)\n    Using Img As New System.Drawing.Imaging.Metafile(stream)\n        Using bmp As New Bitmap(Img.Width, Img.Height)\n            Using g As Graphics = Graphics.FromImage(bmp)\n                g.Clear(Color.Transparent)\n                g.DrawImage(Img, 0, 0)\n                bmp.Save(pngFile, System.Drawing.Imaging.ImageFormat.Png)\n            End Using\n        End Using\n    End Using\nEnd Using