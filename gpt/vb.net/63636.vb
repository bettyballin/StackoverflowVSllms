Private Sub ExecuteSQLScript(resourceID As Integer)\n       Dim sqlScript As String\n       Dim conn As Object\n       Dim rs As Object\n\n       ' Load SQL script from resources\n       sqlScript = StrConv(LoadResData(resourceID, "RCDATA"), vbUnicode)\n\n       ' Create and open database connection (example uses ADODB)\n       Set conn = CreateObject("ADODB.Connection")\n       conn.Open "Provider=SQLOLEDB;Data Source=YourServer;Initial Catalog=YourDatabase;User ID=YourUserID;Password=YourPassword;"\n\n       ' Execute the SQL script\n       Set rs = conn.Execute(sqlScript)\n\n       ' Clean up\n       rs.Close\n       Set rs = Nothing\n       conn.Close\n       Set conn = Nothing\n   End Sub\n\n   ' Example usage\n   Private Sub Form_Load()\n       ' Execute the SQL script with resource ID 101\n       ExecuteSQLScript 101\n   End Sub