Sub GenerateSQL()\n    Dim db As DAO.Database\n    Dim tdf As DAO.TableDef\n    Dim fld As DAO.Field\n    Dim idx As DAO.Index\n    Dim sql As String\n\n    Set db = CurrentDb\n    \n    For Each tdf In db.TableDefs\n        ' Skip system tables\n        If Left(tdf.Name, 4) <> "MSys" Then\n            sql = "CREATE TABLE " & tdf.Name & " (" & vbCrLf\n            For Each fld In tdf.Fields\n                sql = sql & "    " & fld.Name & " " & FieldTypeToSQL(fld.Type) & "," & vbCrLf\n            Next fld\n            sql = Left(sql, Len(sql) - 3) & vbCrLf & ");" & vbCrLf & vbCrLf\n            \n            Debug.Print sql\n        End If\n    Next tdf\nEnd Sub\n\nFunction FieldTypeToSQL(typ As Integer) As String\n    Select Case typ\n        Case dbText\n            FieldTypeToSQL = "TEXT"\n        Case dbMemo\n            FieldTypeToSQL = "MEMO"\n        Case dbByte\n            FieldTypeToSQL = "BYTE"\n        Case dbInteger\n            FieldTypeToSQL = "INTEGER"\n        Case dbLong\n            FieldTypeToSQL = "LONG"\n        Case dbCurrency\n            FieldTypeToSQL = "CURRENCY"\n        Case dbSingle\n            FieldTypeToSQL = "SINGLE"\n        Case dbDouble\n            FieldTypeToSQL = "DOUBLE"\n        Case dbDate\n            FieldTypeToSQL = "DATETIME"\n        Case dbBinary\n            FieldTypeToSQL = "BINARY"\n        Case dbBoolean\n            FieldTypeToSQL = "YESNO"\n        Case Else\n            FieldTypeToSQL = "TEXT"\n    End Select\nEnd Functio