Imports System.Net.Mail\nImports System.IO\n\nModule Module1\n    Sub Main()\n        Try\n            ' Read HTML content from file\n            Dim htmlBody As String = File.ReadAllText("path_to_your_html_file.html")\n\n            ' Create a new MailMessage object\n            Dim mail As New MailMessage()\n            mail.From = New MailAddress("your_email@example.com")\n            mail.To.Add("recipient@example.com")\n            mail.Subject = "Your Email Subject"\n            mail.Body = htmlBody\n            mail.IsBodyHtml = True\n\n            ' Configure the SMTP client\n            Dim smtpClient As New SmtpClient("your_smtp_server")\n            smtpClient.Port = 587 ' or the port used by your SMTP server\n            smtpClient.Credentials = New System.Net.NetworkCredential("your_username", "your_password")\n            smtpClient.EnableSsl = True ' if your server requires SSL\n\n            ' Send the email\n            smtpClient.Send(mail)\n            Console.WriteLine("Email sent successfully.")\n        Catch ex As Exception\n            Console.WriteLine("Error sending email: " & ex.Message)\n        End Try\n    End Sub\nEnd Module