Module Module1\n    Sub Main()\n        Dim strTag As String = "<#vermittler_person_Name>"\n        Dim tmp As String = "<#vermittler_person_Name>"\n\n        ' Print character codes\n        Console.WriteLine("strTag character codes:")\n        For Each ch As Char In strTag\n            Console.WriteLine(AscW(ch))\n        Next\n\n        Console.WriteLine("tmp character codes:")\n        For Each ch As Char In tmp\n            Console.WriteLine(AscW(ch))\n        Next\n\n        ' Trim and compare\n        strTag = strTag.Trim()\n        tmp = tmp.Trim()\n\n        Dim areEqual As Boolean = String.Equals(strTag, tmp, StringComparison.Ordinal)\n        Console.WriteLine("Are Equal (Ordinal): " & areEqual)\n    End Sub\nEnd Module