Imports System.Reflection\n\nPublic Function CompareObjects(obj1 As Object, obj2 As Object) As Boolean\n    If obj1 Is Nothing OrElse obj2 Is Nothing Then\n        Return False\n    End If\n\n    ' Get the type of the objects\n    Dim type1 As Type = obj1.GetType()\n    Dim type2 As Type = obj2.GetType()\n\n    ' Ensure both objects are of the same type\n    If type1 IsNot type2 Then\n        Return False\n    End If\n\n    ' Get all fields of the objects\n    Dim fields As FieldInfo() = type1.GetFields(BindingFlags.Public Or BindingFlags.NonPublic Or BindingFlags.Instance)\n\n    ' Compare each field value\n    For Each field As FieldInfo In fields\n        Dim value1 As Object = field.GetValue(obj1)\n        Dim value2 As Object = field.GetValue(obj2)\n\n        If Not Object.Equals(value1, value2) Then\n            Return False\n        End If\n    Next\n\n    Return True\nEnd Functio