Sub Loadreport()\n    Try\n        Dim reportPath As String = Server.MapPath("ClientList.rpt")\n        ' Debug: Output the resolved path\n        System.Diagnostics.Debug.WriteLine("Report Path: " & reportPath)\n        \n        ' Check if the file exists at the resolved path\n        If Not System.IO.File.Exists(reportPath) Then\n            Throw New FileNotFoundException("Report file not found at: " & reportPath)\n        End If\n\n        Dim sreport As New ReportDocument()\n        sreport.Load(reportPath)\n        sreport.FileName = reportPath\n\n    Catch ex As Exception\n        ' Log the error message for debugging\n        System.Diagnostics.Debug.WriteLine("Error: " & ex.Message)\n    End Try\nEnd Sub