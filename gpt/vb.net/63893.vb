Public Class ValidationItem\n    Implements IComparable(Of ValidationItem)\n\n    Public Property Item As String\n    Public Property Message As String\n\n    Public Sub New(item As String, message As String)\n        Me.Item = item\n        Me.Message = message\n    End Sub\n\n    Public Overrides Function Equals(obj As Object) As Boolean\n        Dim other As ValidationItem = TryCast(obj, ValidationItem)\n        If other Is Nothing Then\n            Return False\n        End If\n        Return Me.Item = other.Item AndAlso Me.Message = other.Message\n    End Function\n\n    Public Overrides Function GetHashCode() As Integer\n        Return Item.GetHashCode() Xor Message.GetHashCode()\n    End Function\n\n    Public Function CompareTo(other As ValidationItem) As Integer Implements IComparable(Of ValidationItem).CompareTo\n        If other Is Nothing Then\n            Return 1\n        End If\n        Dim itemComparison As Integer = String.Compare(Me.Item, other.Item, StringComparison.Ordinal)\n        If itemComparison <> 0 Then\n            Return itemComparison\n        End If\n        Return String.Compare(Me.Message, other.Message, StringComparison.Ordinal)\n    End Function\nEnd Class