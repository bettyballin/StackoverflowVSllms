' KeypadUserControl.vb\nPublic Class KeypadUserControl\n    Public Event ButtonClicked(ByVal value As String)\n\n    Private Sub Button_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) _\n        Handles Button0.Click, Button1.Click, Button2.Click, Button3.Click, Button4.Click, _\n                Button5.Click, Button6.Click, Button7.Click, Button8.Click, Button9.Click\n        Dim button As Button = CType(sender, Button)\n        RaiseEvent ButtonClicked(button.Text)\n    End Sub\nEnd Class