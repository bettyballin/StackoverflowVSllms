Imports System.Globalization\n\nModule Module1\n    Sub Main()\n        Dim originalString As String = "Date: Wed, 5 Nov 2008 13:12:12 -0500 (EST)"\n        ' Remove "Date: ", parentheses and "EST"\n        Dim formattedString As String = originalString.Remove(0, 6).Replace("(", "").Replace(")", "").Replace("EST", "").Trim()\n        \n        Dim dateValue As DateTime\n        If DateTime.TryParse(formattedString, dateValue) Then\n            Dim resultString As String = dateValue.ToString("yyyyMMdd_HHmmss", CultureInfo.InvariantCulture)\n            Console.WriteLine(resultString)\n        Else\n            Console.WriteLine("nope")\n        End If\n    End Sub\nEnd Module