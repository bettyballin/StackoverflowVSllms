Imports System.Reflection\n\nPrivate Sub ReflectThis(ByVal rawData As Object())\n    If rawData Is Nothing OrElse rawData.Length = 0 Then\n        Throw New ArgumentException("rawData cannot be null or empty")\n    End If\n\n    Dim dataType As Type = rawData(0).GetType()\n    Dim properties As PropertyInfo() = dataType.GetProperties()\n\n    For Each item As Object In rawData\n        For Each prop As PropertyInfo In properties\n            Dim propValue As Object = prop.GetValue(item, Nothing)\n            ' Use reflected property value here\n            Console.WriteLine($"{prop.Name}: {propValue}")\n        Next\n    Next\nEnd Sub