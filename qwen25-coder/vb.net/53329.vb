Imports System.Diagnostics\n\nModule Program\n    Sub Main()\n        ' Define the path to MSBuild.exe and your project file\n        Dim msbuildPath As String = "C:\Program Files (x86)\Microsoft Visual Studio\2019\Community\MSBuild\Current\Bin\MSBuild.exe"\n        Dim projectFile As String = "yourproject.vbproj"\n\n        ' Create a process to run MSBuild with the project file\n        Using process As New Process() With {\n            .StartInfo.FileName = msbuildPath,\n            .StartInfo.Arguments = $"\"{projectFile}\" /p:Configuration=Release",\n            .StartInfo.UseShellExecute = False,\n            .StartInfo.RedirectStandardOutput = True,\n            .StartInfo.RedirectStandardError = True,\n            .StartInfo.CreateNoWindow = True\n        }\n\n            ' Start the process and wait for it to complete\n            process.Start()\n            Dim output As String = process.StandardOutput.ReadToEnd()\n            Dim errorOutput As String = process.StandardError.ReadToEnd()\n            process.WaitForExit()\n\n            ' Output results or errors\n            Console.WriteLine("Build Output: " & output)\n            If Not String.IsNullOrEmpty(errorOutput) Then\n                Console.WriteLine("Build Errors: " & errorOutput)\n            End If\n\n        End Using\n    End Sub\nEnd Module