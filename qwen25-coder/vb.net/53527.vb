Function GetHTMLSource(url As String) As String\n    Dim xmlhttp As Object\n    \n    ' Create an instance of the XMLHTTP object\n    Set xmlhttp = CreateObject("MSXML2.XMLHTTP60")\n    \n    ' Open a connection to the URL using a GET request\n    xmlhttp.Open "GET", url, False\n    \n    ' Send the request\n    xmlhttp.Send\n    \n    ' Check if the response is ready and successful\n    If xmlhttp.Status = 200 Then\n        GetHTMLSource = xmlhttp.responseText\n    Else\n        GetHTMLSource = "Error: " & xmlhttp.statusText\n    End If\n    \n    ' Clean up\n    Set xmlhttp = Nothing\nEnd Function\n\nSub TestGetHTMLSource()\n    Dim url As String\n    Dim html As String\n    \n    ' Specify the URL you want to fetch HTML from\n    url = "https://example.com"\n    \n    ' Fetch the HTML source code\n    html = GetHTMLSource(url)\n    \n    ' Output the HTML to a cell in Excel (for example, A1)\n    Range("A1").Value = html\nEnd Sub