Protected Sub TenWordsTextBoxValidator_ServerValidate(ByVal source As Object, ByVal args As System.Web.UI.WebControls.ServerValidateEventArgs) Handles TenWordsTextBoxValidator.ServerValidate\n    ' Use regular expression to split the input into words\n    Dim matches As MatchCollection = Regex.Matches(args.Value.Trim(), "\b[\w']+\b")\n    ' Check if the number of words is less than or equal to 10\n    args.IsValid = matches.Count <= 10\nEnd Sub