Imports System.Collections.Generic\n\nPublic Class ExampleService\n    Private collection As Dictionary(Of String, Object)\n\n    Public Sub New()\n        ' Initialize the dictionary\n        collection = New Dictionary(Of String, Object)()\n        \n        ' Adding items to the dictionary\n        collection.Add("Key1", "Value1")\n        collection.Add("Key2", "Value2")\n    End Sub\n\n    Public Function GetValueByKey(key As String) As Object\n        If collection.ContainsKey(key) Then\n            Return collection(key)\n        Else\n            Throw New KeyNotFoundException("The specified key was not found in the collection.")\n        End If\n    End Function\n\n    Public Sub IterateThroughCollection()\n        For Each kvp In collection\n            Console.WriteLine($"Key: {kvp.Key}, Value: {kvp.Value}")\n        Next\n    End Sub\nEnd Class