Private Function CreateDictionary(Of T, U)(ByVal ParamArray items() As<KeyValuePair(T, U)) As System.Collections.Generic.Dictionary(Of T, U)\n    Dim dict As New System.Collections.Generic.Dictionary(Of T, U)()\n    For Each kvp In items\n        dict.Add(kvp.Key, kvp.Value)\n    Next\n    Return dict\nEnd Function\n\nPrivate mStringDict As System.Collections.Generic.Dictionary(Of String, String) = CreateDictionary(New KeyValuePair(Of String, String)("Key1", "Value1"), New KeyValuePair(Of String, String)("Key2", "Value2"))