Option Compare Database\nOption Explicit\n\nPrivate Sub x()\n    Dim PDFdoc As Acrobat.CAcroPDDoc\n    Set PDFdoc = CreateObject("AcroExch.PDDoc")\n    \n    If PDFdoc.Open("C:\Reports\MRIR\mrir.pdf") Then\n        Dim numpages As Long\n        numpages = PDFdoc.GetNumPages\n        \n        If numpages > 1 Then ' Ensure there are multiple pages to move\n            \n            ' Copy the last page and insert it as the first page\n            Dim LastPage As Acrobat.CAcroPDPage\n            Set LastPage = PDFdoc.AcquirePage(numpages - 1)\n            \n            If Not LastPage Is Nothing Then\n                Dim NewPDDoc As Acrobat.CAcroPDDoc\n                Set NewPDDoc = CreateObject("AcroExch.PDDoc")\n                \n                ' Create a new document to hold the reordered pages\n                If NewPDDoc.Create Then\n                    NewPDDoc.InsertPages 0, PDFdoc, numpages - 1, 1\n                    \n                    ' Insert all other pages after the first page\n                    Dim i As Long\n                    For i = 0 To numpages - 2\n                        LastPage = PDFdoc.AcquirePage(i)\n                        NewPDDoc.InsertPages i + 1, PDFdoc, i, 1\n                    Next i\n                    \n                    ' Save the new document\n                    If NewPDDoc.Save(PDSaveFull, "C:\Reports\MRIR\Switched.pdf") Then\n                        MsgBox "PDF saved successfully."\n                    Else\n                        MsgBox "Failed to save PDF."\n                    End If\n                End If\n                \n                NewPDDoc.Close\n            End If\n            \n        Else\n            MsgBox "The PDF has only one page or is empty."\n        End If\n        \n        PDFdoc.Close\n    Else\n        MsgBox "Failed to open the PDF file."\n    End If\n    \n    Set LastPage = Nothing\n    Set NewPDDoc = Nothing\n    Set PDFdoc = Nothing\nEnd Sub