Imports System\n\nFunction DynamicCast(value As String, typeName As String) As Object\n    Dim targetType As Type = Type.GetType(typeName)\n    If targetType Is Nothing Then\n        Throw New ArgumentException("Invalid type name.")\n    End If\n    \n    Select Case Type.GetTypeCode(targetType)\n        Case TypeCode.String\n            Return DirectCast(value, String)\n        Case TypeCode.Int32\n            Return Integer.Parse(value)\n        Case TypeCode.Boolean\n            Return Boolean.Parse(value)\n        Case Else\n            Throw New InvalidCastException("Unsupported type.")\n    End Select\nEnd Function\n\n' Example usage:\nDim myValue As Object = DynamicCast("123", "System.Int32")\nConsole.WriteLine(myValue)  ' Outputs: 123