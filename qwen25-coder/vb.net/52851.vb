<%\n' Create an instance of the MSXML2.XMLHTTP object\nSet xmlhttp = Server.CreateObject("MSXML2.XMLHTTP")\n\n' Define the URL for the REST web service\nurl = "https://api.example.com/data"\n\n' Open a connection to the server using a GET request\nxmlhttp.Open "GET", url, False\n\n' Send the request\nxmlhttp.Send()\n\n' Check if the request was successful\nIf xmlhttp.Status = 200 Then\n    ' Output the response text (e.g., JSON)\n    Response.Write(xmlhttp.responseText)\nElse\n    ' Handle the error case\n    Response.Write("Error: " & xmlhttp.statusText & " (" & xmlhttp.Status & ")")\nEnd If\n\n' Clean up the object\nSet xmlhttp = Nothing\n%>