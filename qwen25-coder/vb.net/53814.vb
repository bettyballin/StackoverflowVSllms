Imports System.Runtime.InteropServices\n\nModule Module1\n    Sub Main()\n        Dim accessApp As Object = Nothing\n        Try\n            ' Create an instance of the Access Application\n            accessApp = Activator.CreateInstance(Type.GetTypeFromProgID("Access.Application"))\n\n            ' Open a database programmatically\n            accessApp.OpenCurrentDatabase("C:\path\to\your\database.accdb")\n\n            ' Add your code here to interact with the Access application\n\n        Catch ex As Exception\n            Console.WriteLine($"An error occurred: {ex.Message}")\n        Finally\n            ' Close and quit Access Application if it was created\n            If accessApp IsNot Nothing Then\n                accessApp.Quit()\n                Marshal.ReleaseComObject(accessApp)\n            End If\n        End Try\n    End Sub\nEnd Module