Imports System.IO\n\nModule Module1\n    Sub Main()\n        ' Define the source file path (the icon file)\n        Dim sourceFilePath As String = "C:\Path\To\YourIcon.ico"\n\n        ' Get the Startup folder path for the current user\n        Dim startupFolderPath As String = Environment.GetFolderPath(Environment.SpecialFolder.Startup)\n\n        ' Define the destination file path in the Startup folder\n        Dim destinationFilePath As String = Path.Combine(startupFolderPath, "YourIcon.ico")\n\n        Try\n            ' Check if the source file exists\n            If File.Exists(sourceFilePath) Then\n                ' Copy the file to the Startup folder\n                File.Copy(sourceFilePath, destinationFilePath, True)\n                Console.WriteLine("File copied successfully.")\n            Else\n                Console.WriteLine("Source file does not exist.")\n            End If\n        Catch ex As Exception\n            Console.WriteLine("An error occurred: " & ex.Message)\n        End Try\n    End Sub\nEnd Module