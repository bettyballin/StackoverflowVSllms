Imports System.Data.OleDb\n\nModule Module1\n    Sub Main()\n        Dim connectionString As String = "Provider=Microsoft.ACE.OLEDB.12.0;Data Source=C:\Path\To\Your\Database.accdb;"\n        Try\n            Using connection As New OleDbConnection(connectionString)\n                connection.Open()\n                Console.WriteLine("The database is not password protected.")\n            End Using\n        Catch ex As InvalidOperationException\n            If ex.Message.Contains("not a valid file format") Then\n                Console.WriteLine("The database might be corrupt or in a different format.")\n            Else\n                Console.WriteLine("Failed to open the database: " & ex.Message)\n            End If\n        Catch ex As OleDbException\n            If ex.ErrorCode = -2147217865 Then ' This error code indicates that a password is required\n                Console.WriteLine("The database is password protected.")\n            Else\n                Console.WriteLine("An unexpected error occurred: " & ex.Message)\n            End If\n        End Try\n    End Sub\nEnd Module