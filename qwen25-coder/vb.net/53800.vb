Imports System.Management\n\nModule Module1\n    Sub Main()\n        Dim searcher As New ManagementObjectSearcher("SELECT * FROM Win32_NetworkAdapter")\n        For Each nic As ManagementObject In searcher.Get()\n            Console.WriteLine($"Name: {nic("Name")}")\n            Console.WriteLine($"NetConnectionID: {If(nic.Properties("NetConnectionID").Value IsNot Nothing, nic("NetConnectionID").ToString(), "N/A")}")\n            Console.WriteLine($"PhysicalAdapter: {nic("PhysicalAdapter")}")\n            Console.WriteLine($"NetEnabled: {nic("NetEnabled")}")\n            Console.WriteLine()\n        Next\n    End Sub\nEnd Module