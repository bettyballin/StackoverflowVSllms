Dim rptDocument As New ReportDocument\nDim rptPath As String = Server.MapPath("Reports/Employees.rpt")\nrptDocument.Load(rptPath)\n\n' Set any necessary parameters or data sources here\n\n' Define the directory and filename for the PDF output\nDim pdfPath As String = Server.MapPath("~/Exports/EmployeesReport.pdf")\n\n' Export Options\nDim pdfExportOptions As New DiskFileDestinationOptions()\npdfExportOptions.DiskFileName = pdfPath\n\n' Set export options\nrptDocument.ExportOptions.ExportFormatType = ExportFormatType.PortableDocFormat\nrptDocument.ExportOptions.DestinationOptions = pdfExportOptions\n\n' Export to PDF\nrptDocument.ExportToDisk(ExportFormatType.PortableDocFormat, pdfPath)\n\n' Optionally, you can send this PDF file back to the client for download\nResponse.Clear()\nResponse.ContentType = "application/pdf"\nResponse.AddHeader("Content-Disposition", $"attachment; filename=EmployeesReport.pdf")\nResponse.WriteFile(pdfPath)\nResponse.End()