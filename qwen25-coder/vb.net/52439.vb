Imports Redemption\n\nPublic Sub SendEmailWithRedemption()\n    Dim Session As New RDOSession\n    Session.MAPIOBJECT = Outlook.Application.Session.MAPIOBJECT ' Use existing session\n\n    Dim SafeMail As RDOMail = Session.GetDefaultFolder(rdoDefaultFolders.olFolderOutbox).Items.Add("IPM.Note")\n    With SafeMail\n        .Recipients.Add("recipient@example.com").Resolve()\n        .Subject = "Test Email"\n        .Body = "This is a test email sent using Redemption."\n        .Send()\n    End With\n\n    Marshal.ReleaseComObject(Session)\nEnd Sub