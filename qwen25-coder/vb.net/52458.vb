' Initialize a List of Object to hold anonymous types\nDim lineItems As New List(Of Object)()\n\n' Assuming myFile is an array or collection of objects with Last_Name, First_Name, and Day properties\nFor index As Integer = 0 To myFile.Count - 1\n    Dim lineItem = New With {\n        .Name = myFile(index).Last_Name & ", " & myFile(index).First_Name,\n        .StartDate = myFile(index).Day,\n        .EndDate = myFile(index).Day\n    }\n    \n    ' Add the anonymous object to the list\n    lineItems.Add(lineItem)\nNext\n\n' Example of accessing the items (Note: You need to use CType for type casting)\nFor Each item As Object In lineItems\n    Dim name As String = CType(item, Dynamic).Name\n    Dim startDate As Integer = CType(item, Dynamic).StartDate\n    Dim endDate As Integer = CType(item, Dynamic).EndDate\n    \n    Console.WriteLine($"{name}: {startDate} - {endDate}")\nNext