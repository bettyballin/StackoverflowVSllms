Sub CreateOutlookEvents()\n    Dim olApp As Outlook.Application\n    Dim olCal As Outlook.CalendarFolder\n    Dim olApt As Outlook.AppointmentItem\n    \n    Set olApp = New Outlook.Application\n    Set olCal = olApp.Session.GetDefaultFolder(olFolderCalendar)\n    \n    Set rng = ThisWorkbook.Sheets("Sheet1").Range("A2:A" & Range("A" & Rows.Count).End(xlUp).Row) ' Adjust "Sheet1" and column as necessary\n    \n    For Each cell In rng\n        If IsDate(cell.Value) Then\n            eventDate = CDate(cell.Value)\n            Set olApt = olCal.Items.Add\n            With olApt\n                .Subject = "Reminder for " & cell.Offset(0, 1).Value ' Assuming the subject is in another column\n                .Start = eventDate - 30\n                .End = eventDate - 29\n                .Location = "Conference Room" ' Customize as needed\n                .Save\n            End With\n        End If\n    Next cell\n    \n    Set olApt = Nothing\n    Set olCal = Nothing\n    Set olApp = Nothing\nEnd Sub