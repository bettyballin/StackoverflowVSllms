Sub PassByVal(ByVal x As Integer)\n    ' x is passed by value\n    x = 10\nEnd Sub\n\nSub PassByRef(ByRef y As Integer)\n    ' y is passed by reference\n    y = 20\nEnd Sub\n\nSub Main()\n    Dim num As Integer = 5\n    PassByVal(num)  ' num remains 5 because changes are made to a copy\n    Console.WriteLine("After ByVal: " & num)\n\n    PassByRef(num)  ' num becomes 20 because the original is modified\n    Console.WriteLine("After ByRef: " & num)\nEnd Sub