Imports System.Net.Http\nImports System.Text\n\nModule Module1\n    Sub Main()\n        Dim client As New HttpClient()\n\n        ' Example GET request\n        Dim getResponse = client.GetAsync("https://api.example.com/data").Result\n        If getResponse.IsSuccessStatusCode Then\n            Dim getResult = getResponse.Content.ReadAsStringAsync().Result\n            Console.WriteLine("GET Response: " & getResult)\n        Else\n            Console.WriteLine("GET Request failed.")\n        End If\n\n        ' Example POST request with JSON body\n        Dim postData As New StringContent("{""key"": ""value""}", Encoding.UTF8, "application/json")\n        Dim postResponse = client.PostAsync("https://api.example.com/data", postData).Result\n        If postResponse.IsSuccessStatusCode Then\n            Dim postResult = postResponse.Content.ReadAsStringAsync().Result\n            Console.WriteLine("POST Response: " & postResult)\n        Else\n            Console.WriteLine("POST Request failed.")\n        End If\n\n        ' Always dispose the HttpClient when done\n        client.Dispose()\n    End Sub\nEnd Module