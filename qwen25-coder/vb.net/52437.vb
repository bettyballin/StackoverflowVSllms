Public Sub CheckActiveXRegistration(strCLSID As String)\n    On Error Resume Next\n    Dim objTest As Object\n    Set objTest = CreateObject("{" & strCLSID & "}")\n    If Err.Number <> 0 Then\n        MsgBox "Error: " & Err.Description, vbCritical, "Failed to create ActiveX object"\n    Else\n        MsgBox "ActiveX is registered successfully.", vbInformation\n    End If\n    On Error GoTo 0\nEnd Sub