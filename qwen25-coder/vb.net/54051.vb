Imports SKYPE4COMLib\n\nModule Module1\n    Sub Main()\n        Dim skype As New Skype With {\n            .Attach = True\n        }\n\n        If Not skype.Client.IsRunning Then\n            Console.WriteLine("Skype is not running.")\n            Return\n        End If\n\n        ' Replace "echo123" with the Skype username you want to call\n        Dim targetUser As String = "echo123"\n        \n        Try\n            Dim user As User = skype.User(targetUser)\n            Dim chat As Chat = skype.CreateChatWith(user.Handle)\n            \n            ' To make a call, use the following method:\n            skype.PlaceCall(user.Handle)\n            \n            Console.WriteLine("Calling " & targetUser & "...")\n        Catch ex As Exception\n            Console.WriteLine("Error: " & ex.Message)\n        End Try\n    End Sub\nEnd Module