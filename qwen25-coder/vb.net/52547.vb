Imports Microsoft.Office.Interop.Access\n\nModule Module1\n    Sub Main()\n        ' Create an instance of Access application\n        Dim accessApplication As New Application()\n        \n        ' Open the database\n        Dim dbPath As String = "C:\path\to\yourdatabase.accdb"\n        accessApplication.OpenCurrentDatabase(dbPath)\n        \n        ' Specify the form name\n        Dim formName As String = "YourFormName"\n        Dim frmDoc As FormDocument\n\n        ' Access the specified form\n        frmDoc = accessApplication.CreateObject("Access.Form", formName)\n        frmDoc.Visible = False\n        \n        ' Iterate through controls on the form\n        For Each control In frmDoc.Controls\n            If TypeOf control Is OLEControl Then\n                ' This is an ActiveX control\n                Console.WriteLine("ActiveX Control: " & control.Name)\n            End If\n        Next\n\n        ' Clean up\n        accessApplication.Quit(AcQuitOption.acQuitSaveNone)\n    End Sub\nEnd Module