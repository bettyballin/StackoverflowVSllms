Private Sub GetAllChildNodes(parentNode As TreeNode, ByRef nodes As List(Of String))\n    For Each node As TreeNode In parentNode.Nodes\n        nodes.Add(node.Text)\n        ' Recursively call the function for each child node\n        GetAllChildNodes(node, nodes)\n    Next\nEnd Sub\n\n' Usage\nDim selectedNode As TreeNode = TreeView1.SelectedNode\nIf selectedNode IsNot Nothing Then\n    Dim allChildNodes As New List(Of String)()\n    GetAllChildNodes(selectedNode, allChildNodes)\n    \n    ' Now, allChildNodes contains all the child nodes of the selected node\nEnd If