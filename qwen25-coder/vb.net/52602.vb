Private Function GetCcB() As Collection(Of Reports_ucColumn)\n    Dim cc As New Collection(Of Reports_ucColumn)\n    cc.Add(Me.ucColumn0)\n    cc.Add(Me.ucColumn1)\n    cc.Add(Me.ucColumn2)\n    Return cc\nEnd Function\n\nSub ProcessColumns()\n    Dim columns As Collection(Of Reports_ucColumn) = GetCcB() ' Store the result in a variable\n    For i As Integer = 0 To columns.Count - 1\n        If csB(i).Contains("Invalid") Or csB(i).Contains("Duplicate") Then\n            columns.Item(i).isValid = False\n            columns.Item(i).title = csB(i)\n            If csB(i).Contains("Invalid") Then \n                columns.Item(i).errCode = "005W"\n            Else ' Assuming you want to set a different error code for duplicates\n                columns.Item(i).errCode = "006D" ' Replace with your actual duplicate err code\n            End If\n        Else\n            columns.Item(i).isValid = True\n            columns.Item(i).title = csB(i)\n            columns.Item(i).errCode = String.Empty\n        End If\n    Next\nEnd Sub