Imports System.Data.OleDb\n\n' Path to the FoxPro table file\nDim filePath As String = "C:\data\table1.dbf"\n\n' Create and open the connection\nUsing myOledbConnection As New OleDbConnection("Provider=VFPOLEDB.1;Data Source=" & IO.Path.GetDirectoryName(filePath) & ";Collating Sequence=MACHINE")\n    myOledbConnection.Open()\n\n    ' Get schema information for columns in the specified table\n    Dim schemaTable As DataTable = myOledbConnection.GetSchema("Columns", New String() {Nothing, Nothing, IO.Path.GetFileNameWithoutExtension(filePath), Nothing})\n\n    ' Iterate through the schema table to get column names\n    For Each row As DataRow In schemaTable.Rows\n        Console.WriteLine(row("COLUMN_NAME"))\n    Next\n\nEnd Using