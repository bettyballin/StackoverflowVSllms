Sub ExportDataToSQL()\n    Dim conn As Object\n    Dim cellValue As String\n    Dim ws As Worksheet\n    \n    ' Set the worksheet\n    Set ws = ThisWorkbook.Sheets("Sheet1")\n    \n    ' Get the value from a specific cell, e.g., A1\n    cellValue = ws.Range("A1").Value\n    \n    ' Define the connection string (adjust as necessary)\n    Set conn = CreateObject("ADODB.Connection")\n    On Error GoTo ErrorHandler\n    conn.Open "Provider=SQLOLEDB;Data Source=YourServerName;Initial Catalog=YourDatabaseName;Integrated Security=SSPI;"\n    \n    ' SQL Statement to insert data into a table\n    Dim sql As String\n    sql = "INSERT INTO YourTableName (ColumnName) VALUES ('" & cellValue & "')"\n    \n    ' Execute the query\n    conn.Execute sql\n    \n    MsgBox "Data exported successfully!"\n    Exit Sub\n\nErrorHandler:\n    MsgBox "Error: " & Err.Description\nEnd Sub