Public Function ToInteger(ByVal s As Object) As Integer\n    Dim result As Integer = 0\n\n    ' Ensure the input is treated as a string if it's not already one.\n    If Not IsDBNull(s) AndAlso s IsNot Nothing Then\n        ' Try to parse the string representation of the object to an integer.\n        Int32.TryParse(s.ToString(), result)\n    End If\n\n    Return result\nEnd Functio