Imports System.Data\nImports System.Data.OleDb\nImports Oracle.ManagedDataAccess.Client ' Make sure you have the Oracle.ManagedDataAccess package installed\n\nModule Module1\n    Sub Main()\n        Dim connectionString As String = "Your_OLEDB_Connection_String_To_Excel"\n        Dim excelQuery As String = "SELECT * FROM [Sheet1$]"\n\n        Using connection As New OleDbConnection(connectionString)\n            connection.Open()\n\n            ' Load Excel Data into DataTable\n            Dim dataTable As New DataTable()\n            Using command As New OleDbCommand(excelQuery, connection)\n                Using reader As OleDbDataReader = command.ExecuteReader()\n                    dataTable.Load(reader)\n                End Using\n            End Using\n\n            ' Oracle Connection String\n            Dim oracleConnectionString As String = "Your_Oracle_Connection_String"\n\n            Using oracleConnection As New OracleConnection(oracleConnectionString)\n                oracleConnection.Open()\n\n                ' Set up OracleBulkCopy to insert the data into Oracle Table\n                Using bulkCopy As New OracleBulkCopy(oracleConnection)\n                    bulkCopy.DestinationTableName = "YOUR_ORACLE_TABLE_NAME"\n                    \n                    ' Map columns if necessary, Excel column names map to DataTable by default\n                    ' Example: bulkCopy.ColumnMappings.Add("ExcelColumnName", "OracleColumnName")\n\n                    ' Write data from DataTable to Oracle Table\n                    bulkCopy.WriteToServer(dataTable)\n                End Using\n\n                oracleConnection.Close()\n            End Using\n\n            connection.Close()\n        End Using\n\n        Console.WriteLine("Data exported successfully.")\n    End Sub\nEnd Module