Dim vProcessInfo As New ProcessStartInfo()\nvProcessInfo.FileName = "your_executable_path" ' Set the path to your executable\n\nUsing p As New Process()\n    p.StartInfo = vProcessInfo\n\n    For i = 1 To 100\n        vProcessInfo.Arguments = "some" & i.ToString()\n        If Not p.HasExited Then\n            p.Kill() ' Ensure the process is not running before starting it again\n        End If\n        p.Start()\n        p.WaitForExit()\n    Next\n\n    ' Optionally close the process outside of loop if necessary\nEnd Using