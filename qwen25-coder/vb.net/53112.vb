Dim startInfo As New System.Diagnostics.ProcessStartInfo()\nDim pStart As New System.Diagnostics.Process()\nTry\n    startInfo.FileName = "c:\Program Files\WinZip\WINZIP32.EXE"\n    startInfo.Arguments = "-a -r ""c:\test.zip"" c:\test"\n    startInfo.UseShellExecute = False\n    startInfo.CreateNoWindow = True\n    startInfo.RedirectStandardOutput = True\n    startInfo.RedirectStandardError = True\n\n    pStart.StartInfo = startInfo\n    pStart.Start()\n    \n    ' Optionally, read output and errors from the streams\n    Dim output As String = pStart.StandardOutput.ReadToEnd()\n    Dim errorOutput As String = pStart.StandardError.ReadToEnd()\n\n    pStart.WaitForExit()\n\n    If pStart.ExitCode <> 0 Then\n        Throw New Exception($"WinZip exit code: {pStart.ExitCode}, Error Output: {errorOutput}")\n    End If\n\nCatch ex As Exception\n    ' Handle exception\n    Response.Write("An error occurred: " & ex.Message)\nEnd Try