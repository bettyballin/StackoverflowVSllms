For i = 1000 To 1783 \n    strQuery1 = "Insert Into MIUsInGridAvgs (NumberofMIUs, ProjRSSI, RealRSSI) " & _\n                "Select Count(MIUID), Avg(ProjRSSI), Avg(RealRSSI) " & _\n                "From MIUsInGrid" & i\n\n    Debug.Print "Running Query: " & strQuery1  ' This will display the query in the Immediate Window (Ctrl + G)\n\n    DoCmd.SetWarnings False\n    On Error GoTo ErrorHandler\n    DoCmd.RunSQL strQuery1\nNext i\nDoCmd.SetWarnings True\nExit Sub\n\nErrorHandler:\nMsgBox "Error executing query: " & err.Description\nDoCmd.SetWarnings True