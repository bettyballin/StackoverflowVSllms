Private Sub cmdSubmit_Click()\n    Dim db As DAO.Database\n    Dim rsForm As DAO.Recordset\n\n    Set db = CurrentDb()\n    Set rsForm = db.OpenRecordset("YourTableName", dbOpenDynaset)\n\n    rsForm.AddNew\n        rsForm!FieldName1 = Me.TextBox1.Value\n        rsForm!FieldName2 = Me.TextBox2.Value\n        ' Add more fields as necessary...\n    rsForm.Update\n\n    rsForm.Close\n    Set rsForm = Nothing\nEnd Sub