Imports System.Drawing\n\nModule TiffToJpegConverter\n    Sub Main()\n        Dim tiffPath As String = "input.tiff"\n        Dim outputBasePath As String = "output_"\n\n        ' Load the TIFF image\n        Dim tiffImage As Image = Image.FromFile(tiffPath)\n\n        ' Get all frame dimensions in the image\n        Dim frameDimensions() As Guid = tiffImage.FrameDimensionsList\n\n        ' Check for the time dimension (which refers to multiple pages in a TIFF)\n        Dim GUID_PageDimension As Guid = System.Drawing.Imaging.FrameDimension.Page.Guid\n        For Each dimGuid As Guid In frameDimensions\n            If dimGuid.Equals(GUID_PageDimension) Then\n                ' Get the total number of frames in the image\n                Dim numFrames As Integer = tiffImage.GetFrameCount(System.Drawing.Imaging.FrameDimension.Page)\n\n                ' Loop each frame and save it as a JPEG file\n                For i As Integer = 0 To numFrames - 1\n                    ' Select active frame\n                    tiffImage.SelectActiveFrame(System.Drawing.Imaging.FrameDimension.Page, i)\n                    ' Save the frame to a new JPEG file\n                    Dim outputPath As String = outputBasePath & (i + 1).ToString("D2") & ".jpeg"\n                    tiffImage.Save(outputPath, System.Drawing.Imaging.ImageFormat.Jpeg)\n                Next\n            End If\n        Next\n\n        ' Dispose of the image to free resources\n        tiffImage.Dispose()\n    End Sub\nEnd Module