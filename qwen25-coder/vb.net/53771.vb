Imports System.Collections.Generic\nImports Newtonsoft.Json\n\nPublic Class SettingsManager\n    ' Save dictionary to settings\n    Public Sub SaveSettings(dictionary As Dictionary(Of String, String))\n        Dim json = JsonConvert.SerializeObject(dictionary)\n        My.Settings.MyDictionaryString = json\n        My.Settings.Save()\n    End Sub\n\n    ' Load dictionary from settings\n    Public Function LoadSettings() As Dictionary(Of String, String)\n        If String.IsNullOrEmpty(My.Settings.MyDictionaryString) Then\n            Return New Dictionary(Of String, String)()\n        Else\n            Return JsonConvert.DeserializeObject(Of Dictionary(Of String, String))(My.Settings.MyDictionaryString)\n        End If\n    End Function\nEnd Class