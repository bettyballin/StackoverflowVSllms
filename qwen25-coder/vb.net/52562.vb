Partial Class _Default\n    Inherits System.Web.UI.Page\n\n    Protected Sub Page_Load(ByVal sender As Object, ByVal e As EventArgs) Handles Me.Load\n        ' Initialize game state on page load\n        If Not IsPostBack Then\n            PlacePiece(0, "path_to_image.png")\n        End If\n    End Sub\n\n    Private Sub PlacePiece(cellIndex As Integer, imageUrl As String)\n        Dim panel = FindControl("Panel_" & cellIndex)\n        If panel IsNot Nothing Then\n            Dim pieceImage As New System.Web.UI.WebControls.Image()\n            pieceImage.CssClass = "piece"\n            pieceImage.ImageUrl = imageUrl\n            pieceImage.ID = "Piece" & cellIndex.ToString()\n            panel.Controls.Add(pieceImage)\n        End If\n    End Sub\n\nEnd Class