Dim connectionString As String = "YourConnectionStringHere"\nUsing connection As New OleDbConnection(connectionString)\n    connection.Open()\n    \n    Dim cmdInsert As New OleDbCommand("INSERT INTO your_table (id, column1, column2) VALUES (your_sequence.nextval, :data1, :data2) RETURNING id INTO :return_id", connection)\n    \n    ' Assuming data types\n    cmdInsert.Parameters.Add(":data1", OleDbType.VarChar).Value = "some_value"\n    cmdInsert.Parameters.Add(":data2", OleDbType.Numeric).Value = 12345\n    \n    ' Output parameter to get the ID\n    Dim returnParameter As New OleDbParameter(":return_id", OleDbType.Integer)\n    returnParameter.Direction = ParameterDirection.Output\n    cmdInsert.Parameters.Add(returnParameter)\n    \n    cmdInsert.ExecuteNonQuery()\n    \n    ' Get the returned id value\n    Dim newId As Integer = CType(returnParameter.Value, Integer)\n    Console.WriteLine("Newly inserted ID: " & newId)\nEnd Using