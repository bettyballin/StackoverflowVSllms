Public Class Calculator\n\n    ' Overloading without using the Overloads keyword\n    Public Function Add(ByVal x As Integer, ByVal y As Integer) As Integer\n        Return x + y\n    End Function\n\n    Public Function Add(ByVal x As Double, ByVal y As Double) As Double\n        Return x + y\n    End Function\n\n    ' Overloading with using the Overloads keyword\n    <Overloads>\n    Public Sub PrintResult(ByVal result As Integer)\n        Console.WriteLine("Integer Result: " & result)\n    End Sub\n\n    <Overloads>\n    Public Sub PrintResult(ByVal result As Double)\n        Console.WriteLine("Double Result: " & result)\n    End Sub\nEnd Class