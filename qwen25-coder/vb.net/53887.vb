Imports System.Data.SQLite\n\nModule Module1\n    Sub Main()\n        ' Create a connection string - replace 'YourDatabase.db' with your database file.\n        Dim connectionString As String = "Data Source=YourDatabase.db;Version=3;"\n        \n        Try\n            Using conn As New SQLiteConnection(connectionString)\n                ' Open the connection:\n                conn.Open()\n                \n                Console.WriteLine("Connection to database successful")\n\n                ' Create a command object for SQL commands\n                Dim cmd As New SQLiteCommand(conn)\n\n                ' Example: Execute a query and display results\n                cmd.CommandText = "SELECT * FROM YourTable"\n                Dim reader As SQLiteDataReader = cmd.ExecuteReader()\n\n                While reader.Read()\n                    Console.WriteLine(reader("ColumnName").ToString())\n                End While\n                \n                reader.Close()\n            End Using\n        Catch ex As Exception\n            Console.WriteLine("Error: " & ex.Message)\n        End Try\n        \n    End Sub\nEnd Module