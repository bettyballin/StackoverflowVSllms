Function IsGuid(ByVal str)\n    ' Regular expression pattern for a valid GUID\n    Dim regexPattern\n    regexPattern = "^\{?[0-9a-fA-F]{8}-([0-9a-fA-F]{4}-){3}[0-9a-fA-F]{12}\}?$"\n    \n    ' Create a RegExp object\n    Dim regex\n    Set regex = New RegExp\n    \n    ' Set the pattern and ignore case\n    With regex\n        .Global = False\n        .IgnoreCase = True\n        .Pattern = regexPattern\n    End With\n    \n    ' Test if the string matches the GUID pattern\n    IsGuid = regex.Test(str)\nEnd Function\n\n' Example usage:\nDim testString\ntestString = "123e4567-e89b-12d3-a456-426614174000"\nIf IsGuid(testString) Then\n    WScript.Echo "The string is a valid GUID."\nElse\n    WScript.Echo "The string is not a valid GUID."\nEnd If