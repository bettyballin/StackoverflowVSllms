Imports System.Collections.Generic\n\nPublic Class MultiDimensionalDictionaryExample\n    Private myDictionary As Dictionary(Of String, Dictionary(Of String, Object))\n\n    Public Sub New()\n        ' Initialize the main dictionary\n        myDictionary = New Dictionary(Of String, Dictionary(Of String, Object))()\n\n        ' Add nested dictionaries\n        myDictionary("OuterKey1") = New Dictionary(Of String, Object)()\n        myDictionary("OuterKey2") = New Dictionary(Of String, Object)()\n\n        ' Add items to the nested dictionaries\n        myDictionary("OuterKey1")("InnerKey1") = "Value1"\n        myDictionary("OuterKey1")("InnerKey2") = "Value2"\n        myDictionary("OuterKey2")("InnerKey3") = "Value3"\n    End Sub\n\n    Public Function GetValue(outerKey As String, innerKey As String) As Object\n        If myDictionary.ContainsKey(outerKey) AndAlso myDictionary(outerKey).ContainsKey(innerKey) Then\n            Return myDictionary(outerKey)(innerKey)\n        Else\n            Throw New KeyNotFoundException("The specified key was not found.")\n        End If\n    End Function\n\n    Public Sub SetValue(outerKey As String, innerKey As String, value As Object)\n        ' Ensure the outer dictionary exists\n        If Not myDictionary.ContainsKey(outerKey) Then\n            myDictionary(outerKey) = New Dictionary(Of String, Object)()\n        End If\n\n        ' Set the value in the nested dictionary\n        myDictionary(outerKey)(innerKey) = value\n    End Sub\nEnd Class