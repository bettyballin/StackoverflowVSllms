Imports Microsoft.VisualBasic.FileIO\n\nModule Module1\n    Sub Main()\n        Dim records As New List(Of String())()\n\n        Using parser As New TextFieldParser("yourfile.csv")\n            parser.TextFieldType = FieldType.Delimited\n            parser.SetDelimiters(",")\n            parser.HasFieldsEnclosedInQuotes = True\n\n            While Not parser.EndOfData\n                Try\n                    ' Read fields to array.\n                    Dim fields() As String = parser.ReadFields()\n                    If fields IsNot Nothing Then\n                        records.Add(fields)\n                    End If\n                Catch ex As MalformedLineException\n                    Console.WriteLine("Line " & parser.ErrorLine.ToString() & " is invalid.")\n                End Try\n            End While\n        End Using\n\n        ' Print out parsed data for demonstration purposes.\n        For Each record In records\n            Console.WriteLine(String.Join("|", record))\n        Next\n    End Sub\nEnd Module