Imports System.Data.SqlClient\n\nPartial Class _Default\n    Inherits System.Web.UI.Page\n\n    Protected Sub btnSubmit_Click(sender As Object, e As EventArgs)\n        Dim tagsString As String = txtTags.Text\n        Dim tagsArray() As String = tagsString.Split(" "c)\n\n        Using connection As New SqlConnection("YourConnectionStringHere")\n            connection.Open()\n\n            For Each tag In tagsArray\n                ' Trim whitespace and ignore empty entries\n                Dim trimmedTag As String = tag.Trim()\n                If Not String.IsNullOrEmpty(trimmedTag) Then\n                    Dim query As String = "INSERT INTO Tags (TagName) VALUES (@TagName)"\n                    Using command As New SqlCommand(query, connection)\n                        command.Parameters.AddWithValue("@TagName", trimmedTag)\n                        command.ExecuteNonQuery()\n                    End Using\n                End If\n            Next\n\n            connection.Close()\n        End Using\n\n        ' Optionally clear the text box after submission\n        txtTags.Text = String.Empty\n    End Sub\nEnd Class