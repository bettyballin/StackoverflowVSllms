<Extension()>\nPublic Function ToNullable(Of T As Structure)(ByVal str As String) As Nullable(Of T)\n    Dim result As Object = Nothing\n    Dim type = GetType(T)\n\n    ' Create the TryParse method info\n    Dim tryParseMethod = type.GetMethod("TryParse", New Type() {GetType(String), type.MakeByRefType()})\n\n    If tryParseMethod IsNot Nothing Then\n        Dim parameters(1) As Object\n        parameters(0) = str\n        parameters(1) = result\n\n        ' Invoke the TryParse method\n        If DirectCast(tryParseMethod.Invoke(Nothing, parameters), Boolean) Then\n            Return DirectCast(parameters(1), T)\n        End If\n    End If\n\n    Return Nothing\nEnd Functio