Function FindGuids(ByVal Text As String) As Collections.Generic.List(Of Guid)\n    Dim guidList As New Collections.Generic.List(Of Guid)()\n    Dim regex As New System.Text.RegularExpressions.Regex("[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}")\n    Dim matches As System.Text.RegularExpressions.MatchCollection = regex.Matches(Text)\n\n    For Each match As System.Text.RegularExpressions.Match In matches\n        If Guid.TryParse(match.Value, Nothing) Then\n            guidList.Add(New Guid(match.Value))\n        End If\n    Next\n\n    Return guidList\nEnd Functio