Sub Main()\n    Dim i As Integer\n    Dim b As Boolean\n\n    ' Set boolean to true for demonstration\n    b = True\n    \n    ' Implicit conversion of Boolean to Integer (returns -1 for True)\n    i = CInt(b)\n    Console.WriteLine(i)  ' Outputs: -1\n\n    ' Explicit conversion using Convert.ToInt32 (returns 1 for True)\n    i = Convert.ToInt32(b)\n    Console.WriteLine(i)  ' Outputs: 1\n\n    ' Parsing "True" as an integer (will result in 0, since "True" is not a valid number)\n    Int32.TryParse("True", i)\n    Console.WriteLine(i)  ' Outputs: 0\nEnd Sub