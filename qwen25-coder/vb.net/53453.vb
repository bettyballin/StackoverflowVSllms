Dim MimeMapObj\nSet MimeMapObj = CreateObject("IISNamespace")\n\n' Connect to the metabase and navigate to the appropriate location\nMimeMapObj.SetPath "LM/W3SVC/1/ROOT"\n\n' Create a new MimeMap object and configure it\nDim objNewMap, MimeMaps, ii\nSet MimeMaps = MimeMapObj.GetCollection("MimeMap")\n\nii = 0\nDo Until MimeMaps(ii).MimeType="" Or MimeMaps(ii).Extension=".svg"\n    ii = ii + 1\nLoop\n\nIf MimeMaps(ii).Extension<>".svg" Then\n    Set objNewMap = MimeMapObj.CreateObject("IIS:MimeMap")\n    objNewMap.ExtensionFile = ".svg"\n    objNewMap.MimeType = "image/svg+xml"\n    MimeMapObj.SetCollection "MimeMap", objNewMap, ii\nEnd If\n\nSet MimeMapObj = Nothing