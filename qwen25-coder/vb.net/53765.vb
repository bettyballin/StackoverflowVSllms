Private Sub btnRead_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnRead.Click\n    Dim reader As StreamReader\n    Dim filelocation As String\n\n    filelocation = "F:\kh_matt\ch1.txt"\n    Dim chid As Integer\n\n    chid = 1\n\n    If File.Exists(filelocation) Then\n        Using reader = New StreamReader(filelocation)\n            Dim vArray() As String = reader.ReadToEnd.Split("$"c)\n\n            For d As Integer = 0 To vArray.Length - 2 Step 1 ' Changed to vArray.Length - 2 to avoid checking the last element unnecessarily\n                InsertKh(chid, d + 1, vArray(d))\n            Next\n\n            MsgBox("Done Inserting")\n        End Using\n    Else\n        MsgBox(filelocation, MsgBoxStyle.OkOnly)\n    End If\nEnd Sub