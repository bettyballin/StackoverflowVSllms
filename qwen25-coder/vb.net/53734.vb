Imports System.Globalization\n\nModule Module1\n    Sub Main()\n        Dim year As Integer = 2009\n        Dim weekOfYear As Integer = 1\n        Dim cal As Calendar = CultureInfo.InvariantCulture.Calendar\n        Dim startOfWeek As DateTime = cal.AddWeeks(cal.AddYears(New DateTime(1, 1, 1), year - 1), weekOfYear - 1)\n        While cal.GetWeekOfYear(startOfWeek, CalendarWeekRule.FirstFourDayWeek, DayOfWeek.Monday) <> weekOfYear OrElse startOfWeek.Year <> year\n            startOfWeek = startOfWeek.AddDays(1)\n        End While\n\n        Dim endOfWeek As DateTime = startOfWeek.AddDays(6)\n\n        Console.WriteLine("StartDate: " & startOfWeek.ToShortDateString())\n        Console.WriteLine("EndDate: " & endOfWeek.ToShortDateString())\n    End Sub\nEnd Module