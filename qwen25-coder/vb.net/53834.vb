Imports System.Security.Cryptography\nImports System.Text\n\nPartial Class _Default\n    Inherits System.Web.UI.Page\n\n    Protected Sub Page_Load(ByVal sender As Object, ByVal e As EventArgs) Handles Me.Load\n        Dim dataToSign As String = "1990-01-01" ' Example date of birth\n        Dim signedData As Byte() = SignData(Encoding.UTF8.GetBytes(dataToSign))\n        \n        ' Convert the signature to a string for display or storage\n        Dim signatureString As String = Convert.ToBase64String(signedData)\n\n        Response.Write("Original Data: " & dataToSign)\n        Response.Write("<br/>Signature: " & signatureString)\n    End Sub\n\n    Public Function SignData(ByVal data() As Byte) As Byte()\n        ' Create a new instance of RSACryptoServiceProvider\n        Using rsa As New RSACryptoServiceProvider()\n            ' Generate a public/private key pair.\n            rsa.PersistKeyInCsp = False\n\n            ' Hash the data and store the hash in a byte array.\n            Dim dataHash() As Byte\n            Using sha256 = SHA256.Create()\n                dataHash = sha256.ComputeHash(data)\n            End Using\n\n            ' Sign the hash using rsa object from System.Security.Cryptography\n            Return rsa.SignData(dataHash, CryptoConfig.MapNameToOID("SHA256"))\n        End Using\n    End Function\nEnd Class