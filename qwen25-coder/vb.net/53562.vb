Sub ExecuteSQLScript()\n    Dim conn As Object\n    Set conn = CreateObject("ADODB.Connection")\n    conn.ConnectionString = "Provider=Microsoft.Jet.OLEDB.4.0;Data Source=C:\path_to_your_database.accdb;"\n    conn.Open\n\n    ' Replace with your SQL script path and content\n    Dim sqlScript As String\n    sqlScript = "CREATE TABLE Test (ID INT PRIMARY KEY, Name VARCHAR(50));" & _\n                "INSERT INTO Test (ID, Name) VALUES (1, 'TestName');"\n\n    conn.Execute sqlScript\n\n    conn.Close\n    Set conn = Nothing\nEnd Sub