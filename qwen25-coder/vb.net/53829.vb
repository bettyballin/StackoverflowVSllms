Imports System.Runtime.InteropServices\n\nModule Module1\n    <DllImport("user32.dll", SetLastError:=True)>\n    Private Function EnumWindows(ByVal lpEnumFunc As EnumWindowsProc, ByVal lParam As IntPtr) As Boolean\n    End Function\n\n    <DllImport("user32.dll", SetLastError:=True, CharSet:=CharSet.Auto)>\n    Private Function GetWindowText(ByVal hWnd As IntPtr, ByVal lpString As System.Text.StringBuilder, ByVal nMaxCount As Integer) As Integer\n    End Function\n\n    <DllImport("user32.dll", SetLastError:=True, CharSet:=CharSet.Auto)>\n    Private Function SendMessage(ByVal hWnd As IntPtr, ByVal Msg As UInteger, ByVal wParam As IntPtr, ByVal lParam As IntPtr) As IntPtr\n    End Function\n\n    Public Delegate Boolean EnumWindowsProc(ByVal hWnd As IntPtr, ByVal lParam As IntPtr)\n\n    Private Const WM_GETTEXT As Integer = &HD\n\n    Sub Main()\n        Dim oScript = New ProvideX.Script()\n        ' Setup code here '\n        oScript.Run(commName)\n\n        ' Wait for dialog to be created\n        System.Threading.Thread.Sleep(500) ' Adjust as needed\n\n        ' Find the dialog box handle\n        EnumWindows(AddressOf EnumCallback, IntPtr.Zero)\n    End Sub\n\n    Private Function EnumCallback(ByVal hWnd As IntPtr, ByVal lParam As IntPtr) As Boolean\n        Dim bufferSize As Integer = GetWindowTextLength(hWnd) + 1\n        Dim sb As New System.Text.StringBuilder(bufferSize)\n\n        GetWindowText(hWnd, sb, bufferSize)\n\n        If sb.ToString().Contains("Expected Dialog Title") Then ' Replace with actual dialog title\n            MessageBox.Show("Dialog found: " & hWnd)\n\n            ' Simulate pressing Enter (e.g., to accept the warning)\n            SendMessage(hWnd, &H101, IntPtr.Zero, IntPtr.Zero) ' Button click\n\n            ' Find child windows (textboxes) and send text to them\n            ' You might need to use similar techniques to find button handles if necessary\n\n        End If\n\n        Return True\n    End Function\n\n    <DllImport("user32.dll", SetLastError:=True)>\n    Private Function GetWindowTextLength(ByVal hWnd As IntPtr) As Integer\n    End Function\nEnd Module