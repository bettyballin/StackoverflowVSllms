Dim fld As DAO.Field\nDim db As DAO.Database\nSet db = CurrentDb\n\n' Loop through each field in Records table and check for non-nullables\nFor Each fld In db.TableDefs(TName).Fields\n    If Not fld.Attributes And dbAutoIncrField Then\n        ' Check if the field is not nullable and not an auto-increment field\n        If fld.Required = True Then\n            SQLStatement = "INSERT INTO " & TName & "(" & fld.Name & ") SELECT " & fld.Name & " FROM " & TName & "_New WHERE " & fld.Name & " IS NOT NULL"\n            On Error Resume Next ' Temporarily disable error handling to catch specific errors\n            DoCmd.RunSQL SQLStatement\n            If Err > 0 Then\n                Log.WriteLine "Error inserting into field '" & fld.Name & "': " & Err.Description\n                Err.Clear\n            End If\n        End If\n    End If\nNext fld\n\nOn Error GoTo 0 ' Re-enable error handling