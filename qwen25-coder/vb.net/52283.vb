' Create a sample Hashtable\nDim hashtable As New Hashtable()\nhashtable.Add("1", "Apple")\nhashtable.Add("2", "Banana")\nhashtable.Add("3", "Cherry")\n\n' Convert Hashtable to DataTable\nDim dataTable As New DataTable()\ndataTable.Columns.Add("Key")\ndataTable.Columns.Add("Value")\n\nFor Each key As DictionaryEntry In hashtable\n    dataTable.Rows.Add(key.Key, key.Value)\nNext\n\n' Create a BindingSource and bind it to the ComboBox\nDim bindingSource As New BindingSource(dataTable, Nothing)\n\nWith Me.ComboBox1\n    .DataSource = bindingSource\n    .DisplayMember = "Value"  ' What to display in the combo box items.\n    .ValueMember = "Key"     ' Underlying value associated with each item.\nEnd With