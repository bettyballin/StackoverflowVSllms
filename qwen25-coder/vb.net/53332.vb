' Assuming your datetime parameter name is 'YourDateParam'\nDim dateValue As Nullable(Of DateTime)\n\nIf String.IsNullOrWhiteSpace(txtDate.Text) Then\n    dateValue = Nothing  ' Set to Nothing which will be converted to DBNull.Value when adding the parameter\nElse\n    dateValue = Convert.ToDateTime(txtDate.Text)\nEnd If\n\n' Add parameter to command\nUsing cmd As New SqlCommand("YourStoredProcedureName", connection)\n    cmd.CommandType = CommandType.StoredProcedure\n    Dim dateParam As New SqlParameter("@YourDateParam", SqlDbType.DateTime)\n\n    ' Check if dateValue is Nothing and set DBNull.Value accordingly\n    If Not dateValue.HasValue Then\n        dateParam.Value = DBNull.Value\n    Else\n        dateParam.Value = dateValue.Value\n    End If\n\n    cmd.Parameters.Add(dateParam)\n    ' Execute the command...\nEnd Using