Imports System.Globalization\n\nModule Module1\n    Sub Main()\n        ' Example date strings from database and user input\n        Dim dbDateString As String = "20/05/2023"\n        Dim userInputDateString As String = "25/05/2023"\n\n        ' Define the expected date format\n        Dim dateFormat As String = "dd/MM/yyyy"\n\n        ' Parse the strings to DateTime objects\n        Dim dbDate As DateTime = DateTime.ParseExact(dbDateString, dateFormat, CultureInfo.InvariantCulture)\n        Dim userInputDate As DateTime = DateTime.ParseExact(userInputDateString, dateFormat, CultureInfo.InvariantCulture)\n\n        ' Compare the two dates\n        If dbDate < userInputDate Then\n            Console.WriteLine("The user input date is after the database date.")\n        ElseIf dbDate > userInputDate Then\n            Console.WriteLine("The user input date is before the database date.")\n        Else\n            Console.WriteLine("Both dates are the same.")\n        End If\n    End Sub\nEnd Module