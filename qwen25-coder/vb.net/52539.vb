Public Class ChainInvoker\n    Public Shared Function InvokeChain(obj As Object, methodNames As List(Of String)) As Object\n        Dim currentObject = obj\n\n        For Each methodName In methodNames\n            Dim methodInfo = currentObject.GetType().GetMethod(methodName)\n            If methodInfo IsNot Nothing Then\n                currentObject = methodInfo.Invoke(currentObject, Nothing)\n            Else\n                Throw New ArgumentException($"Method {methodName} not found on type {currentObject.GetType()}")\n            End If\n        Next\n\n        Return currentObject\n    End Function\nEnd Class\n\nSub Main()\n    Dim a As New A()\n    Dim result = ChainInvoker.InvokeChain(a, New List(Of String) From {"Func", "Name"})\n    Console.WriteLine(result)\nEnd Sub