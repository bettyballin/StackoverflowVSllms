Imports System.Reflection\n\nPublic Class Form1\n    Private Sub Form1_Load(sender As Object, e As EventArgs) Handles MyBase.Load\n        Dim imageList As New ImageList()\n\n        ' Get all properties from My.Resources.MyImages class\n        For Each propInfo As PropertyInfo In GetType(My.Resources.Resource1).GetProperties(BindingFlags.Static Or BindingFlags.Public)\n            ' Check if the property is of type System.Drawing.Bitmap\n            If propInfo.PropertyType Is GetType(System.Drawing.Bitmap) Then\n                Dim imgName As String = propInfo.Name\n                Dim imgFile As System.Drawing.Image = CType(propInfo.GetValue(Nothing, Nothing), System.Drawing.Image)\n\n                ' Add to ImageList\n                imageList.Images.Add(imgName, imgFile)\n            End If\n        Next\n\n        ' Optionally, assign the ImageList to a control e.g., PictureBox\n        Me.PictureBox1.ImageList = imageList\n    End Sub\nEnd Class