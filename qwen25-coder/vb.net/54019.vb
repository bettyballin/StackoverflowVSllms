Private Sub ReadXMLAttributes(ByVal oXML As String)\n    Try\n        Dim XmlDoc As New System.Xml.XmlDocument()\n        XmlDoc.LoadXml(oXML)\n\n        Dim xmlDeclaration As System.Xml.XmlNode = XmlDoc.FirstChild\n\n        ' Find the processing instruction for mso-infoPathSolution\n        Dim pi As System.Xml.XmlProcessingInstruction = CType(xmlDeclaration.NextSibling, System.Xml.XmlProcessingInstruction)\n        If pi IsNot Nothing AndAlso pi.Name = "mso-infoPathSolution" Then\n            Dim attrValue As String = pi.Data\n            Dim oFileInfo As New InfoPathDocument()\n\n            ' Parse the data attribute by attributes\n            For Each attr In Split(attrValue, """ ")\n                Dim keyValue As String() = attr.Split("""="c) ' assuming this format is consistent\n                If keyValue.Length > 1 Then\n                    Select Case keyValue(0)\n                        Case "solutionVersion"\n                            oFileInfo.SolutionVersion = keyValue(1)\n                        Case "productVersion"\n                            oFileInfo.ProductVersion = keyValue(1)\n                        Case "PIVersion"\n                            oFileInfo.PIVersion = keyValue(1)\n                        Case "href"\n                            oFileInfo.href = keyValue(1)\n                        Case "name"\n                            oFileInfo.name = keyValue(1)\n                    End Select\n                End If\n            Next\n\n            ' Use oFileInfo as needed\n        Else\n            MsgBox("mso-infoPathSolution processing instruction not found.")\n        End If\n    Catch ex As Exception\n        MsgBox("Error loading or parsing XML: " & ex.Message)\n    End Try\nEnd Sub