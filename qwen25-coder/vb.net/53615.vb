Private Sub TestCancelButtonForBooking(bookingId As Integer)\n    Dim cancelButtonExists = _internetExplorer.Button(Find.ById(New Regex("CBooking"))).Exists\n    If Not cancelButtonExists Then\n        d_logger.LogResultTextFile($"Cancel Button does not exist for Booking ID: {bookingId}", True, False)\n    End If\nEnd Sub\n\nPublic Sub TestBookings()\n    For Each booking In createdBookings\n        Try\n            _internetExplorer.GoTo(booking.ViewUrl) ' Navigate to the view summary page of each booking\n            TestCancelButtonForBooking(booking.Id)\n        Catch ex As Exception\n            d_logger.LogResultTextFile($"Error checking Booking ID: {booking.Id} - {ex.Message}", True, False)\n        End Try\n    Next\nEnd Sub