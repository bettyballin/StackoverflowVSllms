Imports System.Net.Mail\nImports System.IO\n\nModule EmailSender\n    Sub Main()\n        Dim mailSubject As String = "Your Newsletter"\n        Dim mailRecipient As String = "recipient@example.com"\n        Dim senderEmail As String = "sender@example.com"\n        Dim smtpServer As String = "smtp.example.com" ' Change to your SMTP server\n        Dim htmlFilePath As String = "C:\path\to\newsletter.html"\n\n        ' Read HTML content from file\n        Dim bodyHtml As String\n        Using reader As New StreamReader(htmlFilePath)\n            bodyHtml = reader.ReadToEnd()\n        End Using\n\n        ' Set up the mail message with HTML content\n        Using message As New MailMessage(senderEmail, mailRecipient, mailSubject, bodyHtml)\n            message.IsBodyHtml = True\n\n            ' Set up the SMTP client\n            Using client As New SmtpClient(smtpServer)\n                client.Send(message)\n            End Using\n        End Using\n\n        Console.WriteLine("Email sent successfully.")\n    End Sub\nEnd Module