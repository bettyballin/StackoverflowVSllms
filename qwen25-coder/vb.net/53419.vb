Sub RemoveDefaultValue()\n    Dim db As DAO.Database\n    Dim tdf As DAO.TableDef\n    Dim fld As DAO.Field\n\n    Set db = CurrentDb\n    Set tdf = db.TableDefs("YourTableName")\n    \n    For Each fld In tdf.Fields\n        If fld.Name = "YourFieldName" Then\n            fld.DefaultValue = ""\n            Exit For\n        End If\n    Next fld\n    \n    Set fld = Nothing\n    Set tdf = Nothing\n    Set db = Nothing\nEnd Sub