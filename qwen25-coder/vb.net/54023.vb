Dim date1 As DateTime = ' your first date '\nDim date2 As DateTime = ' your second date '\n\n' Create new dates with just the date part (year, month, day)\nDim dateOnly1 As New DateTime(date1.Year, date1.Month, date1.Day)\nDim dateOnly2 As New DateTime(date2.Year, date2.Month, date2.Day)\n\n' Compare the date parts\nIf dateOnly1 = dateOnly2 Then\n    Console.WriteLine("The dates are the same.")\nElse\n    Console.WriteLine("The dates are different.")\nEnd If