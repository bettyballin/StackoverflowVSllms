Protected Sub Page_Load(ByVal sender As Object, ByVal e As EventArgs) Handles Me.Load\n       If Not IsPostBack Then\n           Dim months = {"Jan", "Feb", "Mar", "Apr", "May", "Jun", \n                         "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"}\n           Dim consumptionData() As Double = {120, 340, 450, 600, 800, 900, \n                                              1100, 1200, 900, 700, 400, 300}\n           \n           EnergyConsumptionChart.DataSource = months\n           Dim series As Series = EnergyConsumptionChart.Series("MonthlyConsumption")\n           For i As Integer = 0 To months.Length - 1\n               series.Points.AddXY(months(i), consumptionData(i))\n           Next\n\n           ' Optional: Set labels and other properties\n           EnergyConsumptionChart.Titles.Add("Monthly Energy Consumption")\n       End If\n   End Sub