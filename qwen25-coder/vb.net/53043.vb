Protected Sub SubmitButton_Click(ByVal sender As Object, ByVal e As EventArgs)\n        Dim tagsInput As String = TagsTextBox.Text.Trim()\n        If Not String.IsNullOrEmpty(tagsInput) Then\n            Dim tags As List(Of String) = tagsInput.Split(" "c).ToList()\n\n            ' Assuming you have a SQL connection and command set up already.\n            Using con As New SqlConnection("YourConnectionStringHere")\n                con.Open()\n\n                For Each tag In tags\n                    If Not String.IsNullOrEmpty(tag.Trim()) Then\n                        Dim query As String = "INSERT INTO Tags (TagValue) VALUES (@Tag)"\n                        Using cmd As New SqlCommand(query, con)\n                            cmd.Parameters.AddWithValue("@Tag", tag.Trim())\n                            cmd.ExecuteNonQuery()\n                        End Using\n                    End If\n                Next\n            End Using\n        End If\n    End Sub