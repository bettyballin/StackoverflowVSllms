Dim reader As XmlTextReader = New XmlTextReader(strURL)\nDim city As String = ""\nDim ds As DataSet = New DataSet()\n\n' No need to set the Namespace of the DataSet, it will be determined by the XML content.\nds.ReadXml(reader)\n\n' Loop through all tables and find the one with the desired namespace\nFor Each table As DataTable In ds.Tables\n    If table.NamespaceURI = "HomeAddress" AndAlso table.TableName = "Address" Then\n        city = table.Rows(0).Item(2).ToString()\n        Exit For\n    End If\nNext