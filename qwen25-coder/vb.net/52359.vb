' Declare necessary variables\nDim xlApp As Object\nDim xlBook As Object\n\nSub InitializeExcel()\n    ' Create a new instance of Excel\n    Set xlApp = CreateObject("Excel.Application")\n    \n    ' Make the Excel application visible (set to False if you don't want it visible)\n    xlApp.Visible = True\n    \n    ' Add a new workbook\n    Set xlBook = xlApp.Workbooks.Add\n    \n    ' You can now manipulate the workbook, add sheets, fill data, etc.\n    xlBook.Sheets(1).Cells(1, 1) = "Hello World"\nEnd Sub\n\nSub CleanUp()\n    ' Close the workbook without saving\n    xlBook.Close False\n    \n    ' Quit Excel application\n    xlApp.Quit\n    \n    ' Release objects from memory\n    Set xlBook = Nothing\n    Set xlApp = Nothing\nEnd Sub