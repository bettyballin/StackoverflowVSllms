Imports System.Xml\n\nModule ProjectCreator\n    Sub Main()\n        ' Create a new XML document for the .vbproj file\n        Dim doc As New XmlDocument()\n\n        ' Load an existing .vbproj file if needed, for this example we'll create it anew\n        ' doc.Load("ExistingProject.vbproj")\n\n        ' Define the root element of the project file\n        Dim projectNode As XmlNode =\n            doc.CreateElement("Project", "http://schemas.microsoft.com/developer/msbuild/2003")\n        doc.AppendChild(projectNode)\n\n        ' Add properties group\n        Dim propertyGroupNode As XmlElement =\n            doc.CreateElement("PropertyGroup")\n        projectNode.AppendChild(propertyGroupNode)\n\n        ' Define the OutputType property\n        Dim outputTypeNode As XmlElement =\n            doc.CreateElement("OutputType")\n        outputTypeNode.InnerText = "WinExe" ' Or ConsoleApp, Exe, etc.\n        propertyGroupNode.AppendChild(outputTypeNode)\n\n        ' Add other necessary nodes and attributes as required\n\n        ' Save the document to a file\n        Dim filePath As String = "MyProject.vbproj"\n        doc.Save(filePath)\n\n        Console.WriteLine("Project file created at: " & filePath)\n    End Sub\nEnd Module