Public Class MyClassExample\n    Public Shared Sub MySharedMethod()\n        Console.WriteLine("Shared method called.")\n    End Sub\n\n    Public Sub InstanceMethod()\n        Console.WriteLine("Instance method called.")\n    End Sub\nEnd Class\n\nSub Main()\n    ' Accessing shared method through class name\n    MyClassExample.MySharedMethod()\n\n    ' Accessing shared method through an instance - still works in VB.NET!\n    Dim myInstance As New MyClassExample()\n    myInstance.MySharedMethod()\nEnd Sub