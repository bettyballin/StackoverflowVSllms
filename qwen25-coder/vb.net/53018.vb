Module Module1\n    Sub Main()\n        Serializing()\n    End Sub\n\n    Public Class User\n        Public Sub New()\n        End Sub\n\n        Public Sub New(ByVal Username As String, ByVal UserId As Integer)\n            Name = Username\n            ID = UserId\n        End Sub\n\n        Public Property Name As String\n        Public Property ID As Integer\n    End Class\n\n    Public Sub Serializing()\n        Dim Users As New List(Of User)\n\n        Dim u As New User("bob", 1)\n        Users.Add(u)\n\n        u = New User("bill", 2) ' Ensure creating a new instance for each user\n        Users.Add(u)\n\n        u = New User("ted", 3) ' Ensure creating a new instance for each user\n        Users.Add(u)\n\n        Dim sw As New System.IO.StringWriter\n        Dim ser As New System.Xml.Serialization.XmlSerializer(GetType(List(Of User)))\n        ser.Serialize(sw, Users)\n\n        Console.WriteLine(sw.ToString)\n    End Sub\nEnd Module