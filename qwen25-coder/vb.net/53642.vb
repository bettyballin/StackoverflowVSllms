Imports Microsoft.Office.Interop\n\nPublic Sub ReadExcelFile()\n    ' Start Excel app\n    Dim exApp As New Excel.Application\n    exApp.Visible = False ' Set to True if you want to see the Excel application open\n    \n    Try\n        ' Load excel document\n        Dim workbook As Excel.Workbook = exApp.Workbooks.Open("C:\path\to\your\file.xlsx")\n        Dim worksheet As Excel.Worksheet = workbook.Sheets(1)\n        \n        ' Access cell A3\n        Dim b As String = worksheet.Cells(3, 1).Value.ToString()\n        \n        ' Output the value of cell A3\n        Console.WriteLine(b)\n    Catch ex As Exception\n        MessageBox.Show("Error: " & ex.Message)\n    Finally\n        ' Close Excel and clean up\n        exApp.Quit()\n        ReleaseObject(worksheet)\n        ReleaseObject(workbook)\n        ReleaseObject(exApp)\n    End Try\nEnd Sub\n\nPrivate Sub ReleaseObject(ByVal obj As Object)\n    Try\n        System.Runtime.InteropServices.Marshal.ReleaseComObject(obj)\n        obj = Nothing\n    Catch ex As Exception\n        obj = Nothing\n    Finally\n        GC.Collect()\n    End Try\nEnd Sub