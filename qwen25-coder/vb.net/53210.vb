Private WithEvents pdfExporter As New PDFExporter()\n\nPrivate Sub pdfExporter_FileExists(sender As Object, e As FileExistsEventArgs) Handles pdfExporter.FileExists\n    ' Show dialog to ask user if they want to overwrite or skip\n    Dim result = MessageBox.Show($"File {e.FilePath} already exists. Overwrite?", "Overwrite File", MessageBoxButtons.YesNoCancel)\n\n    If result = DialogResult.Yes Then\n        e.Overwrite = True\n    ElseIf result = DialogResult.Cancel Then\n        ' Handle cancellation appropriately, e.g., stop processing\n    End If\nEnd Sub\n\nPrivate Sub StartExport()\n    pdfExporter.CheckAndProcessFile("path_to_your_cad_file")\nEnd Sub