Imports System.Data.SqlClient\n\nPublic Module MyMacros\n    \n    Public Sub ExportDataToSqlServer()\n        Dim connectionString As String = "Server=myServerAddress;Database=myDataBase;User Id=myUsername;Password=myPassword;"\n        \n        ' This is a sample data to insert. Adjust as needed.\n        Dim someData As String = "Sample Data"\n        \n        Using connection As New SqlConnection(connectionString)\n            Dim sqlQuery As String = "INSERT INTO YourTableName (YourColumnName) VALUES (@data)"\n            \n            Dim command As New SqlCommand(sqlQuery, connection)\n            command.Parameters.AddWithValue("@data", someData)\n            \n            Try\n                connection.Open()\n                command.ExecuteNonQuery()\n                MessageBox.Show("Data inserted successfully.")\n            Catch ex As Exception\n                MessageBox.Show("An error occurred: " & ex.Message)\n            End Try\n        End Using\n    End Sub\n\nEnd Module