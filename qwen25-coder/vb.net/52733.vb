Sub Application_OnPostRequestHandlerExecute(ByVal sender As Object, ByVal e As EventArgs)\n    Dim context As HttpContext = HttpContext.Current\n    If context IsNot Nothing Then\n        Dim requestedPath As String = context.Request.Path.ToLower()\n        If requestedPath.EndsWith("webresource.axd") Then\n            Exit Sub\n        End If\n\n        ' Continue with the rest of your logic for other requests\n        ' ...\n    End If\nEnd Sub