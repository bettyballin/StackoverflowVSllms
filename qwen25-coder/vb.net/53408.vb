Imports System.Windows.Controls\n\nPublic Sub PopulateTreeView()\n    ' Create root items\n    Dim tviAAA As New TreeViewItem With {.Header = "AAAA"}\n    Dim tviBBB As New TreeViewItem With {.Header = "BBBB"}\n    Dim tviFFF As New TreeViewItem With {.Header = "FFFFF"}\n\n    ' Create child items for BBB\n    Dim tviCCC As New TreeViewItem With {.Header = "CCCC"}\n    Dim tviDDD As New TreeViewItem With {.Header = "DDDD"}\n\n    ' Create grandchild item for DDD\n    Dim tviEEE As New TreeViewItem With {.Header = "EEEE"}\n\n    ' Add child items to BBB\n    tviBBB.Items.Add(tviCCC)\n    tviBBB.Items.Add(tviDDD)\n\n    ' Add grandchild item to DDD\n    tviDDD.Items.Add(tviEEE)\n\n    ' Add root items to the TreeView\n    TreeView1.Items.Add(tviAAA)\n    TreeView1.Items.Add(tviBBB)\n    TreeView1.Items.Add(tviFFF)\nEnd Sub