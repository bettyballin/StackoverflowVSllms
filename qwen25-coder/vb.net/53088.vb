Imports System.Net\nImports System.Xml\n\nModule Module1\n    Sub Main()\n        Dim siteUrl As String = "https://your-site-url"\n        Dim listName As String = "Flecha" ' Ensure this is the internal name of your list.\n        \n        Using webClient As New WebClient()\n            ' Set credentials if necessary (e.g., SharePoint Online)\n            webClient.Credentials = CredentialCache.DefaultCredentials\n            ' Or use NetworkCredential for specific user:\n            ' webClient.Credentials = New NetworkCredential("username", "password", "domain")\n\n            Dim updateStr As String = "<Method ID='1' Cmd='Update'>" & _\n                                      "<SetList Scope='Request'>" & listName & "</SetList>" & _\n                                      "<SetVar Name='ID'>YOUR_ITEM_ID</SetVar>" & _\n                                      "<FieldRef Name='Title'/><Value Type='Text'>Ficha_router</Value>" & _\n                                      "<FieldRef Name='ID_Cliente'/><Value Type='Integer'>0000</Value>" & _\n                                      "</Method>"\n\n            Dim xmlDoc As New XmlDocument()\n            Dim elBatch As XmlElement = xmlDoc.CreateElement("Batch")\n\n            elBatch.SetAttribute("OnError", "Continue")\n            elBatch.SetAttribute("ListVersion", "1")\n            elBatch.InnerXml = updateStr\n\n            ' URL of the Lists.asmx web service\n            Dim listServiceUrl As String = $"{siteUrl}/_vti_bin/Lists.asmx"\n            webClient.Headers.Add("Content-Type", "text/xml; charset=utf-8")\n            webClient.Headers.Add("SOAPAction", "#UpdateListItems")\n\n            Dim soapRequest As String =\n                "<?xml version='1.0' encoding='utf-8'?> " &\n                "<soap:Envelope xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'" &\n                " xmlns:xsd='http://www.w3.org/2001/XMLSchema'" &\n                " xmlns:soap='http://schemas.xmlsoap.org/soap/envelope/'>" &\n                "<soap:Body>" &\n                "<UpdateListItems xmlns='http://schemas.microsoft.com/sharepoint/soap/'>" &\n                $"<listName>{listName}</listName>" &\n                "<updates>" & ElBatch.OuterXml & "</updates>" &\n                "</UpdateListItems>" &\n                "</soap:Body></soap:Envelope>"\n\n            Try\n                Dim response As String = webClient.UploadString(listServiceUrl, soapRequest)\n                Console.WriteLine("Response from SharePoint:")\n                Console.WriteLine(response)\n            Catch ex As Exception\n                Console.WriteLine("An error occurred: " & ex.Message)\n            End Try\n        End Using\n    End Sub\nEnd Module