Private Sub QResultButton_Click()\n    Dim cnn As Object\n    Dim rst As Object\n    Dim strSQL As String\n    Dim dbPath As String\n    \n    ' Set path to the current database file\n    dbPath = CurrentProject.FullName\n\n    ' Create a new recordset object\n    Set rst = CreateObject("ADODB.Recordset")\n    \n    ' Create a connection string using the .accdb (Access 2007 and later)\n    Set cnn = CreateObject("ADODB.Connection")\n    cnn.ConnectionString = "Provider=Microsoft.ACE.OLEDB.12.0;Data Source=" & dbPath & ";"\n    cnn.Open\n    \n    ' Collect user inputs\n    Dim userInput1 As String\n    Dim userInput2 As Date\n    userInput1 = Me.UserInputTextBox.Value ' Assuming you have a text box for input\n    userInput2 = Me.UserInputDate.Value      ' Assuming you have a date picker or similar control\n\n    ' Construct the SQL query with parameters\n    strSQL = "SELECT id, othercolumn, another FROM Readings2009 WHERE something > #" & Format(userInput2, "yyyy-mm-dd") & "# AND othercondition = '" & userInput1 & "'"\n    \n    ' Open the recordset with the constructed SQL\n    rst.Open strSQL, cnn\n    \n    ' Copy Recordset to a new table (or directly set as data source for a form/control)\n    Set Me.Recordset = rst\n\n    ' Clean up\n    rst.Close\n    cnn.Close\n    Set rst = Nothing\n    Set cnn = Nothing\nEnd Sub