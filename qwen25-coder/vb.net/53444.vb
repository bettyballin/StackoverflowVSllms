Sub SplitCSV()\n    Dim myString As String\n    myString = "test, 7535, '1,830,000', '5,000,000'"\n    \n    Dim oTextFieldParser As TextFieldParser\n    Set oTextFieldParser = New TextFieldParser(FileName:=Nothing)\n    oTextFieldParser.Delimiters = Array(",")\n    oTextFieldParser.HasFieldsEnclosedInQuotes = True\n    \n    ' Using the ParseLine method to handle a single line of text\n    oTextFieldParser.SetData(myString)\n    \n    Dim fields() As String\n    While Not oTextFieldParser.EndOfData\n        fields = oTextFieldParser.ReadFields()\n        \n        For Each field In fields\n            Debug.Print Trim(field) ' prints each value on a new line\n        Next field\n    Wend\n    \n    Set oTextFieldParser = Nothing\nEnd Sub