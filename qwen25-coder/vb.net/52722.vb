' Try to find existing publication by name\nDim pub = _Data.publications.FirstOrDefault(Function(s) s.name = e.Value)\n\n' If not found, create new publication\nIf pub Is Nothing Then\n    pub = New publication With {.name = e.Value}\n    _Data.publications.Add(pub)\nEnd If\n\n' This can be used to assign the publication elsewhere in your code\n.yourRecord.Publication = pub\n\n' Don't forget to save changes if you are creating new records\n_Data.SaveChanges()