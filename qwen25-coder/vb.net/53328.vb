Imports Newtonsoft.Json.Linq\n\nPublic Function GetObjectFromDatabase(ByRef obj As Object) As Object\n    Dim retrievedBytes As Byte()\n\n    Using connection As New SqlConnection("YourConnectionString")\n        connection.Open()\n\n        Dim query As String = "SELECT SerializedData FROM YourTable WHERE Id = @Id"\n        Using command As New SqlCommand(query, connection)\n            command.Parameters.AddWithValue("@Id", 1) ' Example ID\n            Dim reader As SqlDataReader = command.ExecuteReader()\n            If reader.Read() Then\n                retrievedBytes = DirectCast(reader("SerializedData"), Byte())\n            End If\n            reader.Close()\n        End Using\n    End Using\n\n    If retrievedBytes IsNot Nothing Then\n        Dim jsonString As String = Encoding.UTF8.GetString(retrievedBytes)\n        obj = JsonConvert.DeserializeObject(jsonString, GetType(YourUserDefinedObjectType))\n    End If\n\n    Return obj\nEnd Functio