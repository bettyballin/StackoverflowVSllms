Imports System.Data.OleDb\n\nPublic Function GetExcelSheetNames(filePath As String) As List(Of String)\n    Dim connectionString As String = "Provider=Microsoft.Jet.OLEDB.4.0;Data Source=" & filePath & ";Extended Properties=""Excel 8.0 Xml;HDR=YES"""\n    \n    Using connection As New OleDbConnection(connectionString)\n        connection.Open()\n        Dim schemaTable As DataTable = connection.GetOleDbSchemaTable(OleDbSchemaGuid.Tables, Nothing)\n        \n        Dim sheetNames As New List(Of String)()\n        For Each row As DataRow In schemaTable.Rows\n            sheetNames.Add(row("TABLE_NAME").ToString())\n        Next\n        \n        Return sheetNames\n    End Using\nEnd Functio