Imports System.Runtime.InteropServices\n\nPublic Class RegistrationHelper\n    <DllImport("ole32.dll")>\n    Private Shared Function OleRegSetUserType(LibID As Guid, lcid As Integer, szUserType As String) As Integer\n    End Function\n\n    <DllImport("ole32.dll", EntryPoint:="DllUnregisterServer")>\n    Private Shared Function DllRegisterServer() As Integer\n    End Function\n\n    Public Shared Function IsRegistered(clsid As String) As Boolean\n        Try\n            Dim guid As Guid = New Guid(clsid)\n            OleRegSetUserType(guid, 0, "")\n            Return True\n        Catch ex As Exception\n            Return False\n        End Try\n    End Function\nEnd Class