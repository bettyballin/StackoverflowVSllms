Private Sub btnSubmit_Enter()\n    ' Insert dummy value into tblAutonumber to ensure there is at least one record\n    DoCmd.RunSQL "INSERT INTO tblAutonumber (Dummy) VALUES ('DummyValue')"\n\n    Dim db As DAO.Database\n    Dim rst As DAO.Recordset\n    Dim strSQL As String\n\n    Set db = CurrentDb\n\n    ' Get the maximum Autonumber from tblAutonumber\n    strSQL = "SELECT MAX(Autonumber) AS MaxNum FROM tblAutonumber"\n    Set rst = db.OpenRecordset(strSQL)\n\n    If Not (rst.EOF And rst.BOF) Then\n        rst.MoveFirst\n        txtAutoNumber.Value = rst!MaxNum\n    Else\n        ' Handle the case where no records are found\n        MsgBox "No records found in tblAutonumber.", vbExclamation\n        Exit Sub\n    End If\n\n    ' Close Recordset and Database objects\n    rst.Close\n    Set rst = Nothing\n    Set db = Nothing\n\n    ' Insert into tbltesting (assuming txtAutoNumber holds the correct value)\n    DoCmd.RunSQL "INSERT INTO tbltesting (Empid, TestScenario, Owner, Event, Version, ExpResult, Variation, Status, Homestore) " & _\n                 "VALUES ('" & Me.txtEmpNo.Value & "', '" & Me.txtTestScenario.Value & "', '" & Me.txtOwner.Value & "', '" & Me.txtEvent.Value & "', '" & Me.txtVersion.Value & "', '" & Me.txtExpectedResult.Value & "', '" & Me.txtVariation.Value & "', '" & Me.txtStatus.Value & "', '" & Me.txtHomeStore.Value & "')"\n\n    ' Insert into tblContract using the max Autonumber value\n    DoCmd.RunSQL "INSERT INTO tblContract (Empid, Testid, Start1, Finish1, Store1, Start2, Finish2, Store2) " & _\n                 "VALUES ('" & Me.txtEmpNo.Value & "', '" & txtAutoNumber.Value & "', '" & Me.txtContSunStart1.Value & "', '" & Me.txtContSunFinish1.Value & "', '" & Me.txtContSunStore1.Value & "', '" & Me.txtContSunStart2.Value & "', '" & Me.txtContSunFinish2.Value & "', '" & Me.txtContSunStore2.Value & "')"\nEnd Sub