' Define an interface with one method\nPublic Interface ILogger\n    Sub Log(message As String)\nEnd Interface\n\n' Implement the interface in a class\nPublic Class ConsoleLogger Implements ILogger\n    Public Sub Log(message As String) Implements ILogger.Log\n        ' Implementation to log message to console\n        Console.WriteLine($"Log: {message}")\n    End Sub\nEnd Class\n\n' Another implementation of the same interface\nPublic Class FileLogger Implements ILogger\n    Public Sub Log(message As String) Implements ILogger.Log\n        ' Implementation to log message to a file\n        My.Computer.FileSystem.WriteAllText("log.txt", $"Log: {message}" & Environment.NewLine, True)\n    End Sub\nEnd Class\n\n' Using the interfaces in your code\nPublic Module ExampleUsage\n    Public Sub Main()\n        Dim logger As ILogger = New ConsoleLogger()\n        logger.Log("This is a log message to console.")\n\n        ' You can switch to FileLogger without changing any other part of the code\n        logger = New FileLogger()\n        logger.Log("This is a log message to file.")\n    End Sub\nEnd Module