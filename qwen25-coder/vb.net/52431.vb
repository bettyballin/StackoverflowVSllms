Try\n    Dim ds As DataSet = SqlHelper.ExecuteDataset(ConfigurationManager.ConnectionStrings("connstr").ConnectionString, CommandType.StoredProcedure, "Get_All", New SqlParameter("@userid", Session("userid")))\n\n    ' Ensure the dataset is not empty and contains at least one table\n    If ds IsNot Nothing AndAlso ds.Tables.Count > 0 Then\n        For Each ds_table As DataTable In ds.Tables\n            For Each ds_row As DataRow In ds_table.Rows\n                ' Use IsDBNull to check if the column value is null before comparing it\n                If Not Convert.IsDBNull(ds_row("isScorm")) AndAlso ds_row.Field(Of Boolean)("isScorm") Then\n                    ds_row("totalLessons") = 100\n                    ds_row("TotalQuestions") = 100\n                    ds_row("lessonscompleted") = 50\n                    ds_row("TotalCorrect") = 50\n                End If\n            Next\n        Next\n\n        ' Bind the modified DataSet to the repeater\n        rptBundles.DataSource = ds.Tables(0)\n        rptBundles.DataBind()\n    Else\n        ' Log or handle the case where no data is returned\n        Throw New Exception("No data returned from the database.")\n    End If\n\nCatch ex As Exception\n    showerror(ex, Me)\nEnd Try