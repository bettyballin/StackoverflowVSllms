Imports System.Net.Http\nImports System.Threading.Tasks\n\nModule Module1\n    Sub Main()\n        Dim apiKey As String = "YOUR_API_KEY"\n        Dim searchEngineId As String = "YOUR_CSE_ID"\n        Dim searchTerm As String = "example search term"\n\n        Dim results As String = GetSearchResults(apiKey, searchEngineId, searchTerm).Result\n        Console.WriteLine(results)\n\n        Console.ReadLine()\n    End Sub\n\n    Async Function GetSearchResults(ByVal apiKey As String, ByVal searchEngineId As String, ByVal query As String) As Task(Of String)\n        Using client As HttpClient = New HttpClient()\n            Dim requestUri As String = $"https://www.googleapis.com/customsearch/v1?q={System.Web.HttpUtility.UrlEncode(query)}&key={apiKey}&cx={searchEngineId}"\n            Dim response As HttpResponseMessage = Await client.GetAsync(requestUri)\n\n            If response.IsSuccessStatusCode Then\n                Return Await response.Content.ReadAsStringAsync()\n            Else\n                Return "An error occurred: " & response.StatusCode.ToString()\n            End If\n        End Using\n    End Function\nEnd Module