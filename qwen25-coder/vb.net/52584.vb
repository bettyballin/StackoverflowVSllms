Imports System\nImports System.IO\nImports System.Xml.Serialization\n\n<XmlRoot("MyObject")>\nPublic Class MySerializableClass\n    <XmlElement("PropertyOne")>\n    Public Property PropertyOne As String\n\n    <XmlElement("PropertyTwo")>\n    Public Property PropertyTwo As Integer\n\n    ' Default constructor is needed for serialization\n    Public Sub New()\n    End Sub\n\n    ' Example parameterized constructor\n    Public Sub New(propertyOne As String, propertyTwo As Integer)\n        Me.PropertyOne = propertyOne\n        Me.PropertyTwo = propertyTwo\n    End Sub\nEnd Class\n\nModule Module1\n    Sub Main()\n        Dim obj As New MySerializableClass("TestValue", 42)\n\n        ' Serialize the object to a file\n        Using writer As New StreamWriter("MyObject.xml")\n            Dim serializer As New XmlSerializer(GetType(MySerializableClass))\n            serializer.Serialize(writer, obj)\n        End Using\n\n        Console.WriteLine("XML has been written to MyObject.xml.")\n    End Sub\nEnd Module