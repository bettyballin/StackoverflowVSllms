<Policy xmlns="urn:oasis:names:tc:xacml:3.0:core:schema:wd-17"\n        PolicyId="EmployeeAccessPolicy"\n        RuleCombiningAlgId="urn:oasis:names:tc:xacml:3.0:rule-combining-algorithm:deny-overrides">\n    <Rule Effect="Permit" RuleId="DeveloperResourceAccessRule">\n        <Target>\n            <AnyOf>\n                <AllOf>\n                    <Match MatchId="urn:oasis:names:tc:xacml:1.0:function:string-equal">\n                        <AttributeValue DataType="http://www.w3.org/2001/XMLSchema#string">Development</AttributeValue>\n                        <AttributeDesignator AttributeId="urn:oasis:names:tc:xacml:1.0:subject-category:access-subject"\n                                             Category="urn:oasis:names:tc:xacml:1.0:subject-category:access-subject"\n                                             DataType="http://www.w3.org/2001/XMLSchema#string"\n                                             MustBePresent="true"/>\n                    </Match>\n                    <Match MatchId="urn:oasis:names:tc:xacml:1.0:function:string-equal">\n                        <AttributeValue DataType="http://www.w3.org/2001/XMLSchema#string">Read</AttributeValue>\n                        <AttributeDesignator AttributeId="urn:oasis:names:tc:xacml:1.0:action:action-id"\n                                             Category="urn:oasis:names:tc:xacml:3.0:attribute-category:action"\n                                             DataType="http://www.w3.org/2001/XMLSchema#string"\n                                             MustBePresent="true"/>\n                    </Match>\n                </AllOf>\n            </AnyOf>\n        </Target>\n    </Rule>\n</Policy>