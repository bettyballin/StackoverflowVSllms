<?xml version="1.0" encoding="utf-8" ?>\n<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">\n  <CodeSnippet Format="1.0.0">\n    <Header>\n      <Title>btag</Title>\n      <Shortcut>btag</Shortcut>\n      <Description>Wrap selected text with HTML bold tags.</Description>\n      <Author>Your Name</Author>\n    </Header>\n    <Snippet>\n      <Declarations>\n        <Literal>\n          <ID>selectedText</ID>\n          <ToolTip>Select the text to bold.</ToolTip>\n          <Default>selected text</Default>\n        </Literal>\n      </Declarations>\n      <Code Language="HTML">\n        <![CDATA[<b>$selectedText$</b>]]>\n      </Code>\n    </Snippet>\n  </CodeSnippet>\n</CodeSnippets>