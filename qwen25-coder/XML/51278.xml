<definitions name="StockQuoteService"\n             targetNamespace="http://www.example.org/stockquote"\n             xmlns:tns="http://www.example.org/stockquote"\n             xmlns:xsd="http://www.w3.org/2001/XMLSchema"\n             xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"\n             xmlns="http://schemas.xmlsoap.org/wsdl/">\n\n    <!-- Message Definitions -->\n    <message name="GetQuoteRequest">\n        <part name="symbol" type="xsd:string"/>\n    </message>\n    <message name="GetQuoteResponse">\n        <part name="price" type="xsd:decimal"/>\n    </message>\n\n    <!-- Port Type Definition -->\n    <portType name="StockQuotePortType">\n        <operation name="GetQuote">\n            <input message="tns:GetQuoteRequest"/>\n            <output message="tns:GetQuoteResponse"/>\n        </operation>\n    </portType>\n\n    <!-- Service Binding -->\n    <binding name="StockQuoteBinding" type="tns:StockQuotePortType">\n        <soap:binding style="document"\n                      transport="http://schemas.xmlsoap.org/soap/http"/>\n        <operation name="GetQuote">\n            <soap:operation soapAction="http://www.example.org/getquote"/>\n            <input>\n                <soap:body use="literal"/>\n            </input>\n            <output>\n                <soap:body use="literal"/>\n            </output>\n        </operation>\n    </binding>\n\n    <!-- Service Declaration -->\n    <service name="StockQuoteService">\n        <port name="StockQuotePort" binding="tns:StockQuoteBinding">\n            <soap:address location="http://www.example.org/quote"/>\n        </port>\n    </service>\n</definitions>