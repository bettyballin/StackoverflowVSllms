<?xml version="1.0" encoding="UTF-8"?>\n<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform"\n                xmlns:str="http://exslt.org/strings"\n                extension-element-prefixes="str"\n                version="1.0">\n\n    <!-- Assume these are your input node sets -->\n    <xsl:variable name="set1" select="/root/set1/item"/>\n    <xsl:variable name="set2" select="/root/set2/item"/>\n\n    <!-- Template to perform the match -->\n    <xsl:template match="/">\n        <matches>\n            <xsl:for-each select="$set1">\n                <xsl:variable name="current-set1-value" select="."/>\n                <xsl:for-each select="$set2">\n                    <xsl:if test="str:starts-with($current-set1-value, .)">\n                        <match>\n                            <set1><xsl:value-of select="$current-set1-value"/></set1>\n                            <set2><xsl:value-of select="."/></set2>\n                        </match>\n                    </xsl:if>\n                </xsl:for-each>\n            </xsl:for-each>\n        </matches>\n    </xsl:template>\n\n</xsl:stylesheet>