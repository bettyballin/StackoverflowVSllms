<configuration>\n     <system.web>\n       <httpRuntime maxRequestLength="4096" useFullyQualifiedRedirectUrl="false" waitChangeNotification="30" />\n       <!-- Disable directoryMonitoring -->\n       <compilation debug="false">\n         <expressionBuilders>\n           <add expressionPrefix="Resources" type="System.Web.Compilation.ResourceExpressionBuilder, System.Web, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />\n         </expressionBuilders>\n       </compilation>\n     <caching>\n       <outputCacheSettings>\n         <outputCacheProfiles>\n           <add name="NoCacheProfile" location="None" noStore="true" varyByParam="*" duration="0" enabled="false"/>\n         </outputCacheProfiles>\n       </outputCacheSettings>\n     </caching>\n\n       <!-- Disable directory monitoring for static files on UNC -->\n       <httpHandlers>\n         <remove verb="*" path="*.jpg" />\n         <add verb="*" path="*.jpg" type="System.Web.StaticFileHandler" validate="true" resourceType="File" requireAccess="Read" responseCacheLimit="10067200" \n              staticHandler="false" />\n       </httpHandlers>\n     </system.web>\n   </configuration>