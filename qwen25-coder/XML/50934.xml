<?xml version="1.0" encoding="UTF-8"?>\n<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform"\n    version="2.0">\n\n    <xsl:output method="html" indent="yes"/>\n\n    <xsl:template match="/">\n        <html>\n            <body>\n                <table border="1">\n                    <tr>\n                        <!-- Define the header labels here -->\n                        <th>A_Label</th>\n                        <th>D_Label</th>\n                        <th>E_Label</th>\n                        <th>F_Label</th>\n                    </tr>\n                    <xsl:for-each select="element1/element2/element3">\n                        <tr>\n                            <!-- Select the attributes in each row -->\n                            <td><xsl:value-of select="../../@A"/></td>\n                            <td><xsl:value-of select="../@D"/></td>\n                            <td><xsl:value-of select="@E"/></td>\n                            <td><xsl:value-of select="@F"/></td>\n                        </tr>\n                    </xsl:for-each>\n                </table>\n            </body>\n        </html>\n    </xsl:template>\n\n</xsl:stylesheet>