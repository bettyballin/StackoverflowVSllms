<!-- Example of configuring a TCP binding in WCF -->\n<system.serviceModel>\n  <services>\n    <service name="YourServiceNamespace.YourService">\n      <host>\n        <baseAddresses>\n          <add baseAddress="net.tcp://localhost:8080/YourService/" />\n        </baseAddresses>\n      </host>\n      <endpoint address="" binding="netTcpBinding" contract="YourServiceNamespace.IYourService">\n      </endpoint>\n    </service>\n  </services>\n</system.serviceModel>