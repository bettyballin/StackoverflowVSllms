<?xml version="1.0" encoding="utf-8"?>\n   <urlrewrite>\n       <!-- Rule to parse user-friendly URLs -->\n       <rule>\n           <from>^/product-(.*)$</from>\n           <to>/products.jsp?name=$1</to>\n       </rule>\n\n       <!-- Optional: Rule to create user-friendly URLs from original query parameters -->\n       <outbound-rule>\n           <from>/products\.jsp\?id=\d+&amp;name=(.*)$</from>\n           <to>/product-$1</to>\n       </outbound-rule>\n   </urlrewrite>