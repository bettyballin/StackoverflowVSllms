<configuration>\n  <system.webServer>\n    <rewrite>\n      <rules>\n        <rule name="Redirect to start.html" stopProcessing="true">\n          <match url="(.*)" />\n          <conditions logicalGrouping="MatchAny">\n            <!-- Add conditions for different domains you want to redirect -->\n            <add input="{HTTP_HOST}" pattern="^domain2\.com$" />\n            <add input="{HTTP_HOST}" pattern="^domain3\.com$" />\n          </conditions>\n          <action type="Redirect" url="http://abc.com/start.html" redirectType="Permanent" />\n        </rule>\n      </rules>\n    </rewrite>\n  </system.webServer>\n</configuration>