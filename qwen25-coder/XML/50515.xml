<!-- Define a resultMap for SomeClass -->\n<resultMap id="SomeClassResultMap" class="YourNamespace.SomeClass">\n    <result property="_strings" javaType="list" columnPrefix="string_"/>\n</resultMap>\n\n<!-- Use the resultMap in your statement -->\n<select id="getSomeClassById" resultMap="SomeClassResultMap">\n    SELECT\n        id,\n        string_value AS string_value <!-- Assuming you have multiple rows for each SomeClass instance, each with a string_value column -->\n    FROM some_table\n    WHERE id = #id#\n</select>