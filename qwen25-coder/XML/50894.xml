<jsp-config>\n    <jsp-property-group>\n        <url-pattern>*.jsp</url-pattern>\n        <el-ignored>false</el-ignored>\n        <scripting-invalid>true</scripting-invalid>\n        <page-encoding>UTF-8</page-encoding>\n        <deferred-syntax-allowed-as-literal>true</deferred-syntax-allowed-as-literal>\n        <trim-directive-whitespaces>false</trim-directive-whitespaces>\n        <include-precompiled>true</include-precompiled>\n        <!-- Enable EL failures to throw an error -->\n        <expression-factory>oracle.jsp.runtime.ELFactoryImpl</expression-factory>\n        <expression-attribute>\n            <name>failOnNullPropertyAccess</name>\n            <value>true</value>\n        </expression-attribute>\n    </jsp-property-group>\n</jsp-config>