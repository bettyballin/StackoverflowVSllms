<configuration>\n  <system.webServer>\n    <rewrite>\n      <rules>\n        <rule name="RewriteProfile" stopProcessing="true">\n          <match url="^user/profile$" />\n          <action type="Rewrite" url="/user/profile.aspx" />\n        </rule>\n        <rule name="RewriteImages" stopProcessing="true">\n          <match url="^s/(.*)$" />\n          <action type="Rewrite" url="/localLocation/{R:1}" />\n        </rule>\n      </rules>\n    </rewrite>\n  </system.webServer>\n</configuration>