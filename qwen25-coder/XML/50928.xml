<!-- Example of configuring HTTPSYS kernel cache for static content -->\n   <configuration>\n     <system.webServer>\n       <staticContent>\n         <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="30.00:00:00"/>\n         <serverRuntime frequentHitThresholdRolloverPerMinute="5" />\n       </staticContent>\n     </system.webServer>\n   </configuration>