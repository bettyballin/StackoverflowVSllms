<!-- Example of basic firewall rules in XML -->\n<?xml version="1.0" encoding="UTF-8"?>\n<Configuration>\n    <!-- Allow HTTP traffic on port 80 -->\n    <Rule Id="AllowHTTP">\n        <Match Protocol="TCP" Direction="Inbound" LocalPort="80"/>\n        <Action Type="Allow"/>\n    </Rule>\n    <!-- Allow HTTPS traffic on port 443 -->\n    <Rule Id="AllowHTTPS">\n        <Match Protocol="TCP" Direction="Inbound" LocalPort="443"/>\n        <Action Type="Allow"/>\n    </Rule>\n</Configuration>