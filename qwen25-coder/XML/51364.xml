<configuration>\n   <system.webServer>\n      <urlCompression doStaticCompression="true" />\n      <httpCompression>\n         <staticTypes>\n            <add mimeType="text/plain" enabled="true" />\n            <add mimeType="text/css" enabled="true" />\n            <add mimeType="application/javascript" enabled="true" />\n            <!-- Other MIME types can be configured similarly -->\n         </staticTypes>\n         <dynamicTypes>\n            <add mimeType="text/html" enabled="true" />\n            <!-- Configure other dynamic types as needed -->\n         </dynamicTypes>\n      </httpCompression>\n   </system.webServer>\n</configuration>