<!-- Rampart Configuration in axis2.xml -->\n<module name="rampart" />\n<parameter name="InflowSecurity">\n    <action>\n        <items>Timestamp Signature Encrypt</items>\n        <user>serviceUser</user>\n        <passwordCallbackClass>com.example.security.SecurityCallbackHandler</passwordCallbackClass>\n        <signatureCrypto>\n            <crypto provider="org.apache.rampart.crypto.provider.RampartPKCS12Crypto"\n                    file="security/file.keystore" keyPassword="keystorePW"/>\n          </signatureCrypto>\n    </action>\n</parameter>