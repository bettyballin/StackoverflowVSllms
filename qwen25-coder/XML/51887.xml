<?xml version="1.0" encoding="UTF-8"?>\n<!DOCTYPE generatorConfiguration\n  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"\n  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">\n\n<generatorConfiguration>\n    <context id="db2Tables">\n        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"\n                        connectionURL="jdbc:mysql://localhost:3306/your_database"\n                        userId="username"\n                        password="password">\n            <!-- Optional properties -->\n        </jdbcConnection>\n\n        <javaTypeResolver >\n            <property name="forceBigDecimals" value="false" />\n        </javaTypeResolver>\n\n        <javaModelGenerator targetPackage="com.yourcompany.model" targetProject="src/main/java">\n            <property name="enableSubPackages" value="true" />\n            <property name="trimStrings" value="true" />\n        </javaModelGenerator>\n\n        <sqlMapGenerator targetPackage="com.yourcompany.mapper" targetProject="src/main/resources">\n            <property name="enableSubPackages" value="true" />\n        </sqlMapGenerator>\n\n        <javaClientGenerator type="XMLMAPPER" targetPackage="com.yourcompany.dao" targetProject="src/main/java">\n            <property name="enableSubPackages" value="true" />\n        </javaClientGenerator>\n\n        <!-- Tables to generate -->\n        <table tableName="your_table_name" domainObjectName="YourDomainObject" >\n            <generatedKey column="id" sqlStatement="MySql" identity="true" />\n        </table>\n    </context>\n</generatorConfiguration>