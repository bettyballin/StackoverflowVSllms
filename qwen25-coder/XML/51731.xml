<types>\n    <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">\n        <!-- Define a complex type for an array of strings -->\n        <xs:complexType name="ArrayOfString">\n            <xs:sequence>\n                <xs:element maxOccurs="unbounded" minOccurs="0" name="string" nillable="true" type="xs:string"/>\n            </xs:sequence>\n        </xs:complexType>\n    </xs:schema>\n</types>\n\n<message name='genericString'>\n    <part name='parameter' type='xsd:string'/>\n</message>\n\n<!-- Update the message to use the new complexType -->\n<message name='genericArray'>\n    <part name='Result' type='tns:ArrayOfString'/>\n</message>\n\n<portType name='FtaPortType'>       \n    <operation name='query'>\n        <input message='tns:genericString'/>\n        <!-- Change the output to use the ArrayOfString -->\n        <output message='tns:genericArray'/>        \n    </operation>      \n</portType>