<configuration>\n    <system.webServer>\n        <!-- Ensure you have these sections enabled -->\n        <rewrite>\n            <rules>\n                <rule name="ReverseProxyInboundRule1" stopProcessing="true">\n                    <match url="(.*)" />\n                    <conditions>\n                        <add input="{HTTP_HOST}" pattern="^www.example\.net$" />\n                    </conditions>\n                    <action type="Rewrite" url="\other\{R:0}" logRewrittenUrl="false" />\n                </rule>\n            </rules>\n        </rewrite>\n\n        <!-- Ensure ARR and proxy settings are configured -->\n        <security>\n            <requestFiltering>\n                <verbs allowUnlisted="true">\n                    <add verb="PROPFIND" allowed="true" />\n                    <add verb="OPTIONS" allowed="true" />\n                    <add verb="TRACE" allowed="true" />\n                    <add verb="HEAD" allowed="true" />\n                    <add verb="PUT" allowed="true" />\n                </verbs>\n            </requestFiltering>\n        </security>\n\n        <httpProtocol>\n          <customHeaders>\n            <remove name="X-Powered-By" />\n          </customHeaders>\n        </httpProtocol>\n\n        <tracing>\n           <!-- Configure tracing according to your need -->\n        </tracing>\n\n        <urlCompression doStaticCompression="true" doDynamicCompression="true" />\n\n    </system.webServer>\n</configuration>