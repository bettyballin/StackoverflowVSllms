<configuration>\n  <system.web>\n    <httpHandlers>\n      <add verb="*" path="*.aspx" type="System.Web.UI.PageHandlerFactory" />\n    </httpHandlers>\n    <httpModules>\n      <add name="CacheModule" type="System.Web.Caching.CacheModule" />\n    </httpModules>\n  </system.web>\n  <system.webServer>\n    <httpProtocol>\n      <customHeaders>\n        <add name="Cache-Control" value="no-cache, no-store, must-revalidate" />\n        <add name="Pragma" value="no-cache" />\n        <add name="Expires" value="-1" />\n      </customHeaders>\n    </httpProtocol>\n  </system.webServer>\n</configuration>