<configuration>\n  <system.web>\n    <webServices>\n      <protocols>\n        <add name="HttpPost"/>\n        <add name="HttpGet"/>\n      </protocols>\n    </webServices>\n  </system.web>\n  <system.webServer>\n    <httpProtocol>\n      <customHeaders>\n        <add name="Access-Control-Allow-Origin" value="*"/>\n        <add name="Access-Control-Allow-Headers" value="Content-Type, Accept, Accept-Language, Accept-Encoding"/>\n        <add name="Access-Control-Allow-Methods" value="GET, POST, PUT, DELETE, OPTIONS"/>\n      </customHeaders>\n    </httpProtocol>\n  </system.webServer>\n</configuration>