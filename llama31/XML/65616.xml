<configuration>\n  <system.webServer>\n    <rewrite>\n      <rules>\n        <rule name="ReverseProxy" stopProcessing="true">\n          <match url="^other/(.*)" />\n          <action type="Rewrite" url="http://www.example.net/{R:1}" />\n          <serverVariables>\n            <set name="HTTP_HOST" value="www.example.net" />\n          </serverVariables>\n        </rule>\n      </rules>\n    </rewrite>\n  </system.webServer>\n</configuration>