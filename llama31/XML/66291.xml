<?xml version="1.0" encoding="UTF-8"?>\n\n<!DOCTYPE xsl:stylesheet SYSTEM "http://www.w3.org/1999/XSL/Transform.dtd">\n\n<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">\n\n    <xsl:import href="php.xsl" />\n\n    <xsl:template match="/">\n\n        <html>\n\n            <body>\n\n                <xsl:apply-templates select="php:function('session_start')" />\n\n                <xsl:apply-templates select="php:function('session_set', 'foo', 'bar')" />\n\n                <xsl:apply-templates select="php:function('session_get', 'foo')" />\n\n                <xsl:apply-templates select="php:function('session_destroy')" />\n\n            </body>\n\n        </html>\n\n    </xsl:template>\n\n</xsl:stylesheet>