<system.webServer>\n    <rewrite>\n        <rules>\n            <rule name="Rewrite Rule" stopProcessing="true">\n                <match url="^.*$" />\n                <conditions>\n                    <add input="{REQUEST_FILENAME}" matchType="IsNotFile" />\n                </conditions>\n                <action type="Rewrite" url="{R:1}" />\n            </rule>\n        </rules>\n    </rewrite>\n</system.webServer>