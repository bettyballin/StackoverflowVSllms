<!-- web.config configuration -->\n<configuration>\n    <system.webServer>\n        <handlers>\n            <add name="PHP5 Handler" path="*.php" verb="*" modules="FastCgiModule" scriptProcessor="C:\PHP\php-cgi.exe" resourceType="Unspecified" requireAccess="None" />\n        </handlers>\n        <rewrite>\n            <rules>\n                <rule name="Magento SEO URLs" stopProcessing="true">\n                    <match url="(.*)" />\n                    <conditions>\n                        <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true" />\n                    </conditions>\n                    <action type="Rewrite" url="index.php?{R:1}" />\n                </rule>\n            </rules>\n        </rewrite>\n    </system.webServer>\n</configuration>