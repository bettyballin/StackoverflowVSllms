<rewrite>\n  <rules>\n    <rule name="Redirect to main domain" stopProcessing="true">\n      <match url="(.*)" />\n      <conditions>\n        <add input="{HTTP_HOST}" pattern="^www\.examples\.com$" />\n        <add input="{HTTP_HOST}" pattern="^www\.ex-ample\.com$" />\n      </conditions>\n      <action type="Redirect" url="http://www.example.com/{R:1}" />\n    </rule>\n  </rules>\n</rewrite>