<rewrite>\n    <rules>\n        <rule name="DomainBasedCss" stopProcessing="true">\n            <match url="(.*)" />\n            <conditions>\n                <add input="{HTTP_HOST}" pattern="^(example1|example2|...)\.com$" />\n            </conditions>\n            <action type="Rewrite" url="{R:0}?domain={C:1}" />\n        </rule>\n    </rules>\n</rewrite>