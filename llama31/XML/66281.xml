<?xml version="1.0" encoding="UTF-8"?>\n<xsl:stylesheet version="1.0"\n  xmlns:xsl="http://www.w3.org/1999/XSL/Transform"\n  xmlns:x="http://thisns.com/">\n\n  <!-- Identity transform -->\n  <xsl:template match="@*|node()">\n    <xsl:copy>\n      <xsl:apply-templates select="@*|node()"/>\n    </xsl:copy>\n  </xsl:template>\n\n  <!-- Select nodes with no namespace and add the namespace -->\n  <xsl:template match="*[namespace-uri()='']">\n    <xsl:element name="x:{local-name()}" namespace="http://thisns.com/">\n      <xsl:apply-templates select="@*|node()"/>\n    </xsl:element>\n  </xsl:template>\n\n</xsl:stylesheet>