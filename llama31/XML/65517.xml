<system.webServer>\n    <rewrite>\n        <rules>\n            <rule name="AllowSessionIdInPath" stopProcessing="true">\n                <match url="^(.*)$" />\n                <conditions>\n                    <add input="{URL}" pattern="^/(.*);(.*)$" />\n                </conditions>\n                <action type="None" />\n            </rule>\n        </rules>\n    </rewrite>\n</system.webServer>