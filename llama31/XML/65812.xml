<!-- applicationHost.config -->\n<configuration>\n    <system.applicationHost>\n        <applicationPools>\n            <add name="PHPAppPool" />\n        </applicationPools>\n        <handlers>\n            <add name="PHPHandler" path="*.php" verb="*" modules="IsapiModule" scriptProcessor="C:\Path\To\php-cgi.exe" resourceType="Unspecified" requireAccess="None" preCondition="bitness64" />\n        </handlers>\n    </system.applicationHost>\n</configuration>