<!-- Create a web service on the sender side -->\n<soap:Envelope xmlns:soap="http://www.w3.org/2003/05/soap-envelope/">\n  <soap:Body>\n    <m:data xmlns:m="http://example.com/data">\n      <!-- sender data -->\n    </m:data>\n  </soap:Body>\n</soap:Envelope>\n\n<!-- Consume the web service on the receiver side -->\n<soap:Envelope xmlns:soap="http://www.w3.org/2003/05/soap-envelope/">\n  <soap:Body>\n    <m:data xmlns:m="http://example.com/data">\n      <!-- receiver data -->\n    </m:data>\n  </soap:Body>\n</soap:Envelope>