<?xml version="1.0" encoding="UTF-8"?>\n<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">\n\n  <!-- First pass: generate table of contents -->\n  <xsl:template match="/">\n    <html>\n      <head>\n        <title>Table of Contents</title>\n      </head>\n      <body>\n        <ul>\n          <xsl:apply-templates select="/*" mode="toc"/>\n        </ul>\n      </body>\n    </html>\n  </xsl:template>\n\n  <xsl:template match="*" mode="toc">\n    <li>\n      <a href="#{@id}">\n        <xsl:value-of select="."/>\n      </a>\n    </li>\n  </xsl:template>\n\n  <!-- Second pass: generate body of data -->\n  <xsl:template match="/">\n    <html>\n      <head>\n        <title>Body of Data</title>\n      </head>\n      <body>\n        <xsl:apply-templates select="/*"/>\n      </body>\n    </html>\n  </xsl:template>\n\n  <xsl:template match="*">\n    <h2 id="{@id}">\n      <xsl:value-of select="."/>\n    </h2>\n    <!-- Add more templates to process the rest of the data -->\n  </xsl:template>\n\n  <!-- Generate frameset -->\n  <xsl:template match="/">\n    <frameset cols="20%,80%">\n      <frame src="toc.html" name="toc"/>\n      <frame src="body.html" name="body"/>\n    </frameset>\n  </xsl:template>\n\n</xsl:stylesheet>