<?xml version="1.0" encoding="UTF-8"?>\n<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">\n\n<tiles-definitions>\n    <!-- Define a tile for the "latest news" module -->\n    <definition name="latestNews" template="/WEB-INF/jsp/latestNews.jsp">\n        <put-attribute name="newsList" value="${newsList}"/>\n    </definition>\n\n    <!-- Define a tile for the "upcoming events" module -->\n    <definition name="upcomingEvents" template="/WEB-INF/jsp/upcomingEvents.jsp">\n        <put-attribute name="eventsList" value="${eventsList}"/>\n    </definition>\n\n    <!-- Define a layout that includes the "latest news" and "upcoming events" tiles -->\n    <definition name="layout" template="/WEB-INF/jsp/layout.jsp">\n        <put-attribute name="latestNews" value="latestNews"/>\n        <put-attribute name="upcomingEvents" value="upcomingEvents"/>\n    </definition>\n</tiles-definitions>