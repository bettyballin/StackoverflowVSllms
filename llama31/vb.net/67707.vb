' Assume you have an ADO connection object (conn) and a command object (cmd)\n\n' Create a recordset to hold the data\nDim rs\nSet rs = CreateObject("ADODB.Recordset")\nrs.Fields.Append "JobID", adInteger\nrs.Fields.Append "JobName", adVarChar, 50\nrs.Open\n\n' Loop through the nodelist and add data to the recordset\nFor Each node In nodeList\n    rs.AddNew\n    rs("JobID") = node.SelectSingleNode("JobID").Text\n    rs("JobName") = node.SelectSingleNode("JobName").Text\n    rs.Update\nNext\n\n' Run through the recordset and perform batch inserts\ncmd.CommandText = "INSERT INTO Jobs (JobID, JobName) VALUES (?, ?)"\ncmd.Parameters.Append cmd.CreateParameter("@JobID", adInteger)\ncmd.Parameters.Append cmd.CreateParameter("@JobName", adVarChar, 50)\n\nrs.MoveFirst\nWhile Not rs.EOF\n    cmd.Parameters("@JobID") = rs("JobID")\n    cmd.Parameters("@JobName") = rs("JobName")\n    cmd.Execute\n    rs.MoveNext\nWend