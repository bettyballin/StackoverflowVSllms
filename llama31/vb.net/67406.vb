Private Sub AddCcColumn(columnName As String)\n    Dim NewColumn As New BinaryComponents.SuperList.Column(columnName, columnName, 110, Function(rowItem As Object) ColumnValueAccessor(rowItem, columnName))\n    _SuperList.Columns.Add(NewColumn)\nEnd Sub\n\nPrivate Function ColumnValueAccessor(rowItem As Object, columnName As String) As Object\n    Dim ToDo As ToDo = CType(rowItem, SrToDoAndException).ToDo\n    ' Use the columnName to access the correct property of the ToDo object\n    Return ToDo.GetType().GetProperty(columnName).GetValue(ToDo, Nothing)\nEnd Functio