Imports System.Reflection\n\nPublic Sub DisplayAll(ByVal Someobject As Foo)\n    For Each prop As PropertyInfo In Someobject.GetType().GetProperties()\n        If prop.CanRead Then\n            Dim value As Object = prop.GetValue(Someobject, Nothing)\n            If value IsNot Nothing Then\n                Console.WriteLine(prop.Name & "=" & value.ToString())\n            Else\n                Console.WriteLine(prop.Name & "=null")\n            End If\n        Else\n            Console.WriteLine(prop.Name & " is not readable")\n        End If\n    Next\nEnd Sub