Protected Sub RegionRepeater_ItemDataBound(sender As Object, e As RepeaterItemEventArgs) Handles RegionRepeater.ItemDataBound\n    If e.Item.ItemType = ListItemType.Item Or e.Item.ItemType = ListItemType.AlternatingItem Then\n        Dim regionLink As HyperLink = e.Item.FindControl("RegionLink")\n        regionLink.NavigateUrl = "yourpage.aspx?region=" & e.Item.DataItem("Region_ID")\n    End If\nEnd Sub\n\nProtected Sub AreaRepeater_ItemDataBound(sender As Object, e As RepeaterItemEventArgs)\n    If e.Item.ItemType = ListItemType.Item Or e.Item.ItemType = ListItemType.AlternatingItem Then\n        Dim areaLink As HyperLink = e.Item.FindControl("AreaLink")\n        areaLink.NavigateUrl = "yourpage.aspx?region=" & e.Item.DataItem("Region_ID") & "&area=" & e.Item.DataItem("Area_ID")\n    End If\nEnd Sub