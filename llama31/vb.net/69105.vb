Module Module1\n    Sub Main()\n        Dim x As Integer = 5\n\n        ' ByVal: a copy of x is made and passed to the method\n        ByValExample(x)\n        Console.WriteLine(x) ' prints 5\n\n        ' ByRef: a reference to x is passed to the method\n        ByRefExample(x)\n        Console.WriteLine(x) ' prints 10\n    End Sub\n\n    Sub ByValExample(ByVal y As Integer)\n        y = 10\n    End Sub\n\n    Sub ByRefExample(ByRef y As Integer)\n        y = 10\n    End Sub\nEnd Module