Imports System\nImports System.Reflection\n\n' ...\n\nFor Each assembly In AppDomain.CurrentDomain.GetAssemblies()\n    Dim assemblyName = assembly.GetName().Name\n    Dim assemblyVersion = assembly.GetName().Version.ToString()\n\n    ' Get the build date and time\n    Dim attribute = CType(assembly.GetCustomAttributes(GetType(AssemblyMetadataAttribute), False).FirstOrDefault(), AssemblyMetadataAttribute)\n    Dim buildDateTime = If(attribute Is Nothing, DateTime.MinValue, attribute.Date)\n\n    Console.WriteLine($"Assembly: {assemblyName}, Version: {assemblyVersion}, Build Date/Time: {buildDateTime}")\nNext