Public Class PriorityQueue(Of T)\n    Inherits List(Of T)\n\n    Public Sub Enque(ByVal item As T, Optional ByVal pushToFront As Boolean = False)\n        If pushToFront = True Then\n            Me.Insert(0, item)\n        Else\n            Me.Add(item)\n        End If\n    End Sub\n\n    Public Function Deque() As T\n        If Me.Count <> 0 Then\n            Dim item As T = Me(0)\n            Me.RemoveAt(0)\n            Return item\n        Else\n            Throw New InvalidOperationException\n        End If\n    End Function\nEnd Class