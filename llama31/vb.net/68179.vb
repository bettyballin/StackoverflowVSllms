Try\n    conn.Open()\n    Dim insertSQL As String = "insert into Employee(uName, firstName, lastName, On_Switch, On_Phone) " + "values(@uName, @firstName, @lastName, @onSwitch, @onPhone)"\n    Dim AddCom As SqlCommand = New SqlCommand(insertSQL, conn)\n    AddCom.Parameters.AddWithValue("@uName", uName)\n    AddCom.Parameters.AddWithValue("@firstName", firstName)\n    AddCom.Parameters.AddWithValue("@lastName", lastName)\n    AddCom.Parameters.AddWithValue("@onSwitch", onSwitch)\n    AddCom.Parameters.AddWithValue("@onPhone", onPhone)\n\n    Try\n        AddCom.ExecuteNonQuery()\n        lblError.Text = "User Added."\n    Catch ex As SqlException\n        If ex.Errors.Count > 0 Then\n            For Each error As SqlError In ex.Errors\n                If error.Number = 2627 Then 'Violation of PRIMARY KEY constraint\n                    ' handle primary key constraint violation\n                    lblError.Text = "User already exists."\n                Else\n                    lblError.Text = error.Message\n                End If\n            Next\n        Else\n            lblError.Text = ex.Message\n        End If\n    End Try\n    conn.Close()\nCatch ex As Exception\n    lblError.Text = ex.Message\nEnd Try