Sub GetOnlineUsers()\n    Dim olApp As New Outlook.Application\n    Dim olSession As Outlook.Namespace\n    Dim olAddressList As Outlook.AddressList\n    \n    Set olSession = olApp.GetNamespace("MAPI")\n    Set olAddressList = olSession.AddressLists("Global Address List")\n    \n    For Each olUser In olAddressList.AddressEntries\n        If olUser.AddressEntryUserType = olUserAddressEntry Then\n            ' Check if user is online\n            If olUser.GetFreeBusy(olToday, 30, False) = olBusy Then\n                Debug.Print olUser.Name & " is online"\n            End If\n        End If\n    Next olUser\nEnd Sub\n\nSub SendMessage(recipientEmail As String)\n    Dim olApp As New Outlook.Application\n    Dim olMail As Outlook.MailItem\n    \n    Set olMail = olApp.CreateItem(olMailItem)\n    olMail.To = recipientEmail\n    olMail.Body = "Hello, this is a test message"\n    olMail.Send\nEnd Sub