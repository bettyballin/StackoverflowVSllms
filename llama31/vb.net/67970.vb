Imports System.Management\n\nPublic Class Form1\n    Private Sub Button1_Click(sender As Object, e As EventArgs) Handles Button1.Click\n        Dim machineId = GetMachineId()\n        Dim processorId = GetProcessorId()\n\n        Console.WriteLine("Machine ID: " & machineId)\n        Console.WriteLine("Processor ID: " & processorId)\n    End Sub\n\n    Private Function GetMachineId() As String\n        Dim query = "SELECT UUID FROM Win32_ComputerSystemProduct"\n        Dim searcher = New ManagementObjectSearcher(query)\n        Dim info = searcher.Get()(0)\n\n        Return info("UUID").ToString()\n    End Function\n\n    Private Function GetProcessorId() As String\n        Dim query = "SELECT ProcessorId FROM Win32_Processor"\n        Dim searcher = New ManagementObjectSearcher(query)\n        Dim info = searcher.Get()(0)\n\n        Return info("ProcessorId").ToString()\n    End Function\nEnd Class