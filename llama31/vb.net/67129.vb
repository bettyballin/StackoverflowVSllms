Imports System.Data.Linq\n\nPublic Class FakeRepository\n    Implements IClientRepository\n\n    Private _clients As New List(Of Client)\n    Private _administrators As New List(Of Administrator)\n    Private _clientAdministrators As New List(Of ClientAdministrator)\n\n    Public Function GetAllClients() As IQueryable(Of Client) Implements IClientRepository.GetAllClients\n        Return _clients.AsQueryable()\n    End Function\n\n    Public Function GetAllAdministrators() As IQueryable(Of Administrator) Implements IClientRepository.GetAllAdministrators\n        Return _administrators.AsQueryable()\n    End Function\n\n    Public Function GetAllClientAdministrators() As IQueryable(Of ClientAdministrator) Implements IClientRepository.GetAllClientAdministrators\n        Return _clientAdministrators.AsQueryable()\n    End Function\n\n    Public Sub New()\n        ' Initialize some sample data\n        Dim client1 As New Client With {.ID = 1, .ClientName = "Client1"}\n        Dim client2 As New Client With {.ID = 2, .ClientName = "Client2"}\n\n        Dim admin1 As New Administrator With {.ID = 1, .UserName = "Admin1"}\n        Dim admin2 As New Administrator With {.ID = 2, .UserName = "Admin2"}\n\n        Dim clientAdmin1 As New ClientAdministrator With {.ClientID = 1, .AdministratorID = 1}\n        Dim clientAdmin2 As New ClientAdministrator With {.ClientID = 1, .AdministratorID = 2}\n\n        _clients.Add(client1)\n        _clients.Add(client2)\n\n        _administrators.Add(admin1)\n        _administrators.Add(admin2)\n\n        _clientAdministrators.Add(clientAdmin1)\n        _clientAdministrators.Add(clientAdmin2)\n\n        ' Populate the EntitySet of ClientAdministrators for each Client\n        client1.ClientAdministrators.Add(clientAdmin1)\n        client1.ClientAdministrators.Add(clientAdmin2)\n    End Sub\nEnd Class