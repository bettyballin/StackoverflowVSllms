Public Function YYWWToDate(yyww As String) As Date\n    Dim year As Integer = CInt(yyww.Substring(0, 2))\n    Dim week As Integer = CInt(yyww.Substring(2, 2))\n    Dim calendar As Calendar = CultureInfo.InvariantCulture.Calendar\n    Dim saturdayDate As Date = calendar.GetDate(year, 1, 1)\n    While calendar.GetWeekOfYear(saturdayDate, CalendarWeekRule.FirstFourDayWeek, DayOfWeek.Monday) <> week\n        saturdayDate = saturdayDate.AddDays(7)\n    End While\n    Return saturdayDate\nEnd Functio