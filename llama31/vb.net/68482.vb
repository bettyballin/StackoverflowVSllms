Imports System.Security.Cryptography\nImports System.Text\n\nPublic Function GenerateDigitalSignature(value As String) As String\n    ' Create a new SHA256 hash object\n    Dim sha256 As New SHA256Managed()\n    \n    ' Convert the value to a byte array\n    Dim valueBytes As Byte() = Encoding.UTF8.GetBytes(value)\n    \n    ' Compute the hash\n    Dim hashBytes As Byte() = sha256.ComputeHash(valueBytes)\n    \n    ' Create a new RSA object using a private key\n    Dim rsa As New RSACryptoServiceProvider()\n    \n    ' Load your private key\n    rsa.FromXmlString("Your private key here")\n    \n    ' Sign the hash\n    Dim signatureBytes As Byte() = rsa.SignHash(hashBytes, "SHA256")\n    \n    ' Convert the signature to a base64 encoded string\n    Dim signature As String = Convert.ToBase64String(signatureBytes)\n    \n    Return signature\nEnd Functio