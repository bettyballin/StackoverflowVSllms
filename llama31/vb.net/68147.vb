Private Sub Form_Submit()\n    ' Run the query and display the results\n    Dim db As DAO.Database\n    Set db = CurrentDb()\n    Dim qdf As DAO.QueryDef\n    Set qdf = db.QueryDefs("SearchQuery")\n    qdf.Parameters("[txtField1]") = txtField1.Value\n    qdf.Parameters("[txtField2]") = txtField2.Value\n    qdf.Parameters("[txtField3]") = txtField3.Value\n    Dim rs As DAO.Recordset\n    Set rs = qdf.OpenRecordset()\n    ' Display the results\n    Do Until rs.EOF\n        Debug.Print rs!Field1, rs!Field2, rs!Field3\n        rs.MoveNext\n    Loop\n    rs.Close\nEnd Sub