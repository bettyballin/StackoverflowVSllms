Dim singleValue As Single\nDim bytes(3) As Byte\nDim valueB() As Byte\n\nFor n = 0 To CDbl(ItemNumberCombo.Text) - 1\n    bytes(3) = valueB(n * 4)\n    bytes(2) = valueB(n * 4 + 1)\n    bytes(1) = valueB(n * 4 + 2)\n    bytes(0) = valueB(n * 4 + 3)\n    ' Use the BitConverter class to copy bytes to singleValue\n    singleValue = BitConverter.ToSingle(bytes, 0)\n    DText(n).Text = singleValue.ToString("0.000000E+00")\n    If DataSaveCheckBox.CheckState = 1 And FileNameText.Text <> "" Then\n        csvOutput &= DText(n).Text & ","\n    End If\nNext