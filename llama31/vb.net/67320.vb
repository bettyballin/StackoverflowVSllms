Imports System.Diagnostics\n\nModule Module1\n    Sub Main()\n        Dim pdfDoc As Object = CreateObject("AcroExch.PDDoc")\n        ' Use the pdfDoc object as needed...\n\n        ' Get the process ID of the application\n        Dim procId As Integer = GetProcessId(pdfDoc)\n\n        ' Release the pdfDoc object\n        pdfDoc = Nothing\n\n        ' Check if the process was already running before creating the object\n        If procId > 0 AndAlso Not WasProcessRunning(procId) Then\n            ' Kill the process\n            Dim proc As Process = Process.GetProcessById(procId)\n            proc.Kill()\n        End If\n    End Sub\n\n    Function GetProcessId(obj As Object) As Integer\n        Dim procId As Integer = 0\n        Dim propInfo As System.Reflection.PropertyInfo = obj.GetType().GetProperty("ProcessID")\n        If propInfo IsNot Nothing Then\n            procId = CInt(propInfo.GetValue(obj, Nothing))\n        End If\n        Return procId\n    End Function\n\n    Function WasProcessRunning(procId As Integer) As Boolean\n        For Each proc As Process In Process.GetProcesses()\n            If proc.Id = procId Then\n                Return True\n            End If\n        Next\n        Return False\n    End Function\nEnd Module