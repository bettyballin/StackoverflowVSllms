Private Sub OnHyperlinkClick(ByVal o As Object, ByVal e As RoutedEventArgs)\n    Dim hyperlink = DirectCast(e.OriginalSource, Hyperlink)\n    Dim dataContext = hyperlink.DataContext\n    Dim obj1 = dataContext.PersonName\n    Dim obj2 = dataContext.UriLink\n    \n    ' Create page to navigate to\n    Dim page As New RedirectPage(obj1, obj2)\n    Me.NavigationService.Navigate(page)\nEnd Sub