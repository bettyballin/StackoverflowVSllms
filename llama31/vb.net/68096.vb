Function calculateIQR(data As Byte()) As Double\n    Array.Sort(data)\n    Dim Q1 As Double = percentile(data, 0.25)\n    Dim Q3 As Double = percentile(data, 0.75)\n    Return Q3 - Q1\nEnd Function\n\nFunction percentile(data As Byte(), percent As Double) As Double\n    Dim index As Integer = CInt((data.Length - 1) * percent)\n    If index = data.Length - 1 Then\n        Return data(index)\n    Else\n        Return (data(index) + data(index + 1)) / 2\n    End If\nEnd Function\n\nSub removeOutliers(data As Byte())\n    Dim IQR As Double = calculateIQR(data)\n    Dim Q1 As Double = percentile(data, 0.25)\n    Dim Q3 As Double = percentile(data, 0.75)\n    Dim lowerBound As Double = Q1 - 1.5 * IQR\n    Dim upperBound As Double = Q3 + 1.5 * IQR\n    \n    For i As Integer = data.Length - 1 To 0 Step -1\n        If data(i) < lowerBound Or data(i) > upperBound Then\n            data.RemoveAt(i)\n        End If\n    Next\nEnd Sub