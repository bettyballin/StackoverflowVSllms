Imports Microsoft.Office.Interop\n\nDim wordApp As New Word.Application()\nDim wordDoc As Word.Document = wordApp.Documents.Add()\nDim excelApp As New Excel.Application()\nDim excelWorkbook As Excel.Workbook = excelApp.Workbooks.Add()\n\n' Create a new Excel worksheet\nDim worksheet As Excel.Worksheet = excelWorkbook.Worksheets.Add()\n\n' Add some data to the worksheet\nworksheet.Cells(1, 1).Value = "Hello, World!"\n\n' Embed the Excel worksheet in the Word document\nwordDoc.InlineShapes.AddOLEObject(Left:=50, Top:=50, Width:=300, Height:=200, _\n  ClassName:="Excel.Sheet", FileName:=worksheet.Name)\n\n' Save the Word document\nwordDoc.SaveAs("example.docx")\n\n' Clean up\nexcelWorkbook.Close()\nexcelApp.Quit()\nwordDoc.Close()\nwordApp.Quit()