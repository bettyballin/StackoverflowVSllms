Dim startDate As DateTime = "02/01/2009"\nDim endDate As DateTime = "08/01/2009"\n\nDim allDates = Enumerable.Range(0, (endDate - startDate).Days + 1).Select(Function(i) startDate.AddDays(i))\n\nDim orderedDates = (From od In db.OrderDetails\n                    Join o In db.Orders On od.OrderID Equals o.OrderID\n                    Where od.ProductID = "A03" AndAlso o.OrderDate >= startDate AndAlso o.OrderDate <= endDate\n                    Select o.OrderDate).Distinct()\n\nDim missingDates = allDates.Except(orderedDates)\n\nFor Each date In missingDates\n    Console.WriteLine(date.ToString("dd/MM/yyyy"))\nNext