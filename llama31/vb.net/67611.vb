Imports System.Drawing\nImports System.Drawing.Imaging\n\nPublic Sub ConvertTiffToJpeg(ByVal tiffFile As String, ByVal outputFolder As String)\n    Dim img As Image = Image.FromFile(tiffFile)\n    Dim frameCount As Integer = img.GetFrameCount(FrameDimension.Page)\n\n    For i As Integer = 0 To frameCount - 1\n        img.SelectActiveFrame(FrameDimension.Page, i)\n        Dim frame As Bitmap = New Bitmap(img)\n        frame.Save(outputFolder & "\page_" & i.ToString() & ".jpg", ImageFormat.Jpeg)\n        frame.Dispose()\n    Next\n\n    img.Dispose()\nEnd Sub