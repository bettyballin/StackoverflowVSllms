' Create a temporary dataset in the Session object\nSub StartDemo()\n    ' Create a new dataset\n    Dim demoData\n    Set demoData = Server.CreateObject("ADODB.Recordset")\n    \n    ' Populate the dataset with sample data from your MS Access database\n    demoData.Open "SELECT * FROM YourTable", YourConnectionString\n    \n    ' Store the dataset in the Session object\n    Session("DemoData") = demoData\nEnd Sub\n\n' Use the temporary dataset for demo-related operations\nSub DisplayDemoData()\n    Dim demoData\n    Set demoData = Session("DemoData")\n    \n    ' Display the data\n    Response.Write "<table>"\n    While Not demoData.EOF\n        Response.Write "<tr><td>" & demoData("YourColumn") & "</td></tr>"\n        demoData.MoveNext\n    Wend\n    Response.Write "</table>"\nEnd Sub\n\n' Discard the temporary dataset when the user ends the demo\nSub EndDemo()\n    Session("DemoData") = Nothing\nEnd Sub