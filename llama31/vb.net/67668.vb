Private Sub UserForm_Initialize()\n    ' Create a new Shell.Application object\n    Dim shell As Object\n    Set shell = CreateObject("Shell.Application")\n    \n    ' Browse for files\n    Dim filePaths As Variant\n    filePaths = shell.BrowseForFolder(0, "Select files", &H4000).Self.Path\n    \n    ' Do something with the file paths\n    For i = 1 To UBound(filePaths)\n        ' Add the file path to a text box or other control\n        Me.TextBox1.Value = Me.TextBox1.Value & filePaths(i) & vbCrLf\n    Next i\nEnd Sub