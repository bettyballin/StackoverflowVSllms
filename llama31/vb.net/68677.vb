Imports System.Reflection\n\nModule Module1\n    Sub Main()\n        Dim assembly As Assembly = Assembly.GetExecutingAssembly()\n        Dim types As Type() = assembly.GetTypes()\n\n        For Each t As Type In types\n            If t.BaseType Is GetType(docMain) AndAlso t.Name.StartsWith("doc") Then\n                Dim instance As docMain = DirectCast(Activator.CreateInstance(t), docMain)\n                addTemplateToList(instance)\n            End If\n        Next\n    End Sub\nEnd Module