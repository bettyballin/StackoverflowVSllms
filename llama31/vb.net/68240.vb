Imports System.Drawing\nImports System.Drawing.Imaging\n\n' ...\n\n' Get the property items from the TIF image\nDim propertyItems As PropertyItem() = image1.PropertyItems\n\n' Create a new list to store the compatible property items\nDim compatiblePropertyItems As New List(Of PropertyItem)\n\n' Iterate through the property items and filter out incompatible ones\nFor Each propertyItem As PropertyItem In propertyItems\n    ' Check if the property item is compatible with JPEG\n    If propertyItem.Id = &H10E OrElse ' Camera Make\n       propertyItem.Id = &H10F OrElse ' Camera Model\n       propertyItem.Id = &H132 OrElse ' Date Taken\n       propertyItem.Id = &H112 Then  ' Orientation\n        compatiblePropertyItems.Add(propertyItem)\n    End If\nNext\n\n' Set the compatible property items on the JPEG image\nFor Each propertyItem As PropertyItem In compatiblePropertyItems\n    image2.SetPropertyItem(propertyItem)\nNext\n\n' Save the JPEG image\nimage2.Save("C:\1.jpg", ImageFormat.Jpeg)