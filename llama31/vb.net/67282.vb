Imports HtmlAgilityPack\n\nPublic Function ExtractAddressInfo(url As String) As DataTable\n    Dim web As New HtmlWeb()\n    Dim doc As HtmlDocument = web.Load(url)\n\n    Dim addressInfo As New DataTable()\n    addressInfo.Columns.Add("Street")\n    addressInfo.Columns.Add("City")\n    addressInfo.Columns.Add("State")\n    addressInfo.Columns.Add("Zip")\n\n    Dim addressNodes As HtmlNodeCollection = doc.DocumentNode.SelectNodes("//div[@class='address']")\n\n    For Each node As HtmlNode In addressNodes\n        Dim street As String = node.SelectSingleNode(".//span[@class='street']").InnerText.Trim()\n        Dim city As String = node.SelectSingleNode(".//span[@class='city']").InnerText.Trim()\n        Dim state As String = node.SelectSingleNode(".//span[@class='state']").InnerText.Trim()\n        Dim zip As String = node.SelectSingleNode(".//span[@class='zip']").InnerText.Trim()\n\n        addressInfo.Rows.Add(street, city, state, zip)\n    Next\n\n    Return addressInfo\nEnd Functio