Imports System.Diagnostics\n\nModule Module1\n    Sub Main()\n        Dim projectPath As String = "C:\Path\To\Your\Project.vbproj"\n        Dim buildConfiguration As String = "Release"\n\n        Dim msbuildPath As String = "C:\Program Files (x86)\Microsoft Visual Studio\2019\BuildTools\MSBuild\Current\Bin\MSBuild.exe"\n\n        Dim process As New Process()\n        process.StartInfo.FileName = msbuildPath\n        process.StartInfo.Arguments = projectPath & " /p:Configuration=" & buildConfiguration\n        process.StartInfo.UseShellExecute = False\n        process.StartInfo.RedirectStandardOutput = True\n        process.Start()\n\n        Dim output As String = process.StandardOutput.ReadToEnd()\n        process.WaitForExit()\n\n        If process.ExitCode = 0 Then\n            Console.WriteLine("Build successful")\n        Else\n            Console.WriteLine("Build failed")\n        End If\n    End Sub\nEnd Module