Private Sub cmdSubmit_Click()\n    Dim db As DAO.Database\n    Dim qd As DAO.QueryDef\n    \n    Set db = CurrentDb()\n    Set qd = db.CreateQueryDef("", "INSERT INTO TableName (FieldName1, FieldName2, FieldName3) VALUES (@FieldName1, @FieldName2, @FieldName3)")\n    \n    qd.Parameters("@FieldName1") = Me.txtFieldName1\n    qd.Parameters("@FieldName2") = Me.txtFieldName2\n    qd.Parameters("@FieldName3") = Me.txtFieldName3\n    \n    qd.Execute\n    \n    Set qd = Nothing\n    Set db = Nothing\nEnd Sub