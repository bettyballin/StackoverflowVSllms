Imports System.Diagnostics\n\nModule Module1\n    Sub Main()\n        Dim remoteComputer As String = "RemoteComputerName"\n        Dim batchFilePath As String = "\\RemoteComputerName\SharedFolder\file.bat"\n        Dim username As String = "RemoteComputerUsername"\n        Dim password As String = "RemoteComputerPassword"\n\n        Dim startInfo As New ProcessStartInfo(batchFilePath)\n        startInfo.RedirectStandardOutput = True\n        startInfo.WindowStyle = ProcessWindowStyle.Hidden\n        startInfo.UseShellExecute = False\n        startInfo.UserName = username\n        startInfo.Password = New System.Security.SecureString\n        For Each c As Char In password\n            startInfo.Password.AppendChar(c)\n        Next\n\n        Using p As Process = Process.Start(startInfo)\n            p.WaitForExit()\n        End Using\n    End Sub\nEnd Module