Public Class FixedSizeList(Of T)\n    Private _list As List(Of T)\n    Private _maxSize As Integer\n\n    Public Sub New(maxSize As Integer)\n        _maxSize = maxSize\n        _list = New List(Of T)(maxSize)\n    End Sub\n\n    Public ReadOnly Property Items As T()\n        Get\n            Return _list.ToArray()\n        End Get\n    End Property\n\n    Public Sub Add(item As T)\n        If _list.Count < _maxSize Then\n            _list.Add(item)\n        Else\n            Throw New InvalidOperationException("List is full")\n        End If\n    End Sub\nEnd Class