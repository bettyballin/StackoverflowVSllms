Imports System.Resources\n\nDim resourceManager As New ResourceManager(GetType(My.Resources))\nDim resources As String() = resourceManager.GetResourceSet(CultureInfo.CurrentCulture, True, True).Keys.Cast(Of String)().ToArray()\n\n' Now you can access the resources as an array\nFor Each resourceName As String In resources\n    If resourceName.StartsWith("YourAudioResourcePrefix") Then\n        ' Play the audio resource\n        My.Computer.Audio.Play(My.Resources.ResourceManager.GetObject(resourceName))\n        Exit For\n    End If\nNext