Imports System\n\nPublic Module StringExtensions\n    <Extension()> _\n    Public Function ToNullable(Of T As {Structure, IConvertible})(ByVal str As String) As Nullable(Of T)\n        Dim parsedValue As T\n        If TryCast(GetType(T), Type).GetMethod("TryParse", {GetType(String), GetType(T).MakeByRefType()}).Invoke(Nothing, {str, parsedValue}) Then\n            Return New Nullable(Of T)(parsedValue)\n        Else\n            Return Nothing\n        End If\n    End Function\nEnd Module