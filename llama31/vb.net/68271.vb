Protected Sub MakeWordListBeforeTab()\n    Dim filename As String = "D:\lao\00001.txt"\n    Dim WordBeforeTabArr As New List(Of String)\n    Dim WordAfterTabArr As New List(Of String)\n\n    Using MyStream As New StreamReader(filename)\n        While Not MyStream.EndOfStream\n            Dim line As String = MyStream.ReadLine()\n            Dim words() As String = line.Split(New String() {vbTab}, StringSplitOptions.None)\n            WordBeforeTabArr.Add(words(0))\n            WordAfterTabArr.Add(words(1))\n        End While\n    End Using\n\n    ' Convert lists to arrays\n    Dim WordBeforeTabArrArray() As String = WordBeforeTabArr.ToArray()\n    Dim WordAfterTabArrArray() As String = WordAfterTabArr.ToArray()\n\n    ' Test the arrays\n    For d As Integer = 0 To WordBeforeTabArrArray.Length - 1\n        MsgBox(WordBeforeTabArrArray(d) & " - " & WordAfterTabArrArray(d))\n    Next\nEnd Sub