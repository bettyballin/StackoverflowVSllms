Function Diff(a As String, b As String) As String\n    Dim m As Integer = a.Length\n    Dim n As Integer = b.Length\n    Dim d(m + 1, n + 1) As Integer\n\n    For i As Integer = 0 To m\n        d(i, 0) = i\n    Next\n\n    For j As Integer = 0 To n\n        d(0, j) = j\n    Next\n\n    For i As Integer = 1 To m\n        For j As Integer = 1 To n\n            If a(i - 1) = b(j - 1) Then\n                d(i, j) = d(i - 1, j - 1)\n            Else\n                d(i, j) = Math.Min(d(i - 1, j) + 1, d(i, j - 1) + 1)\n            End If\n        Next\n    Next\n\n    Dim result As String = ""\n    Dim i As Integer = m\n    Dim j As Integer = n\n\n    While i > 0 AndAlso j > 0\n        If a(i - 1) = b(j - 1) Then\n            result = a(i - 1) & result\n            i -= 1\n            j -= 1\n        ElseIf d(i - 1, j) < d(i, j - 1) Then\n            result = "-" & a(i - 1) & result\n            i -= 1\n        Else\n            result = "+" & b(j - 1) & result\n            j -= 1\n        End If\n    End While\n\n    While i > 0\n        result = "-" & a(i - 1) & result\n        i -= 1\n    End While\n\n    While j > 0\n        result = "+" & b(j - 1) & result\n        j -= 1\n    End While\n\n    Return result\nEnd Functio