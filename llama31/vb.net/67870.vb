Dim ListHours()\nHoursInCurrentDay = 24\nRedim ListHours(23)\n\nFor Days = 0 To 23\n    ListHours(Days) = Days\nNext\n\nSQLARR2 = ListHours\n\nFor x = 1 to ubound(SQLARR2)\n    SQL = "SELECT COUNT(DISTINCT VISITORIP) AS TOTALVISITS"\n    SQL = SQL & " FROM STATS"\n    SQL = SQL & " WHERE DATEENTERED BETWEEN DATEADD(hh, -" & SQLARR2(x) & ", GETDATE()) AND DATEADD(hh, -" & SQLARR2(x) + 1 & ", GETDATE())"\n    Set objVisits = objConn.Execute(SQL)\n    \n    If Not objVisits.EOF Then\n        List = List + "[" & SQLARR2(x) & "," & objVisits("TOTALVISITS") & "],"\n    End If\nNext\n\nResponse.Write List