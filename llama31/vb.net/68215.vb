Imports System.Globalization\n\nDim dateString As String = "Date: Wed, 5 Nov 2008 13:12:12 -0500 (EST)"\nDim dateFormat As String = "ddd, d MMM yyyy HH:mm:ss zzz"\nDim dateValue As DateTime\n\nIf DateTime.TryParseExact(dateString.Remove(0, 6).Replace("(", "").Replace(")", "").Replace("EST", "").Trim(), dateFormat, CultureInfo.InvariantCulture, DateTimeStyles.None, dateValue) Then\n    MsgBox(Format(dateValue, "yyyyMMdd_HHmmss"))\nElse\n    MsgBox("nope")\nEnd If