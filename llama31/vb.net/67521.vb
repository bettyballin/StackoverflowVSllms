Imports System.Text.RegularExpressions\n\nPublic Class NFOData\n    Public Property JobName As String\n    Public Property JobNo As String\n    ' Add other properties as needed\nEnd Class\n\nPublic Function ParseNFOData(filename As String) As NFOData\n    Dim data As New NFOData\n    Dim regex As New Regex("^(?<key>\w+)\s*=\s*(?<value>.*)$")\n\n    Using sr As New StreamReader(filename)\n        While Not sr.EndOfStream\n            Dim line As String = sr.ReadLine\n            Dim match As Match = regex.Match(line)\n\n            If match.Success Then\n                Select Case match.Groups("key").Value\n                    Case "job_name"\n                        data.JobName = match.Groups("value").Value\n                    Case "job_no"\n                        data.JobNo = match.Groups("value").Value\n                    ' Add other cases as needed\n                End Select\n            End If\n        End While\n    End Using\n\n    Return data\nEnd Functio