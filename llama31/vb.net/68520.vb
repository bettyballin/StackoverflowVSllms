<%\n    ' Set up the Access application object\n    Dim accessApp\n    Set accessApp = CreateObject("Access.Application")\n\n    ' Open your database\n    accessApp.OpenCurrentDatabase "C:\Path\To\Your\Database.accdb"\n\n    ' Run the report and save it to a file\n    accessApp.DoCmd.OpenReport "YourReportName", acViewPreview\n    accessApp.DoCmd.OutputTo acOutputPDF, "C:\Path\To\Output\Report.pdf"\n\n    ' Clean up\n    accessApp.Quit\n    Set accessApp = Nothing\n\n    ' Return the report file as a download\n    Response.ContentType = "application/pdf"\n    Response.AddHeader "Content-Disposition", "attachment; filename=Report.pdf"\n    Response.WriteFile "C:\Path\To\Output\Report.pdf"\n%>