Imports System.Security.Cryptography\nImports System.Text\n\nPublic Class RegistrationForm\n    Private Sub btnRegister_Click(sender As Object, e As EventArgs) Handles btnRegister.Click\n        Dim registrationKey As String = txtRegistrationKey.Text\n        Dim hashedKey As String = HashRegistrationKey(registrationKey)\n\n        If hashedKey = StoredHashedKey() Then\n            ' Registration is valid, unlock application features\n            MsgBox("Registration successful!")\n        Else\n            MsgBox("Invalid registration key.")\n        End If\n    End Sub\n\n    Private Function HashRegistrationKey(registrationKey As String) As String\n        Using sha256 As New SHA256CryptoServiceProvider()\n            Dim bytes As Byte() = sha256.ComputeHash(Encoding.UTF8.GetBytes(registrationKey))\n            Return BitConverter.ToString(bytes).Replace("-", "").ToLower()\n        End Using\n    End Function\n\n    Private Function StoredHashedKey() As String\n        ' Retrieve the stored hashed registration key from your database or secure storage\n        ' For demonstration purposes, I'll just return a hardcoded value\n        Return "your_stored_hashed_key_here"\n    End Function\nEnd Class