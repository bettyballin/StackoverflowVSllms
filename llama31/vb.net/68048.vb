Sub QueryLoop()\n    Dim db As DAO.Database\n    Dim rsQueryCrit As DAO.Recordset\n    Dim rsRecords As DAO.Recordset\n    Dim strSQL As String\n    Dim qry As String\n    \n    Set db = CurrentDb()\n    Set rsQueryCrit = db.OpenRecordset("querycrit", dbOpenDynaset)\n    Set rsRecords = db.OpenRecordset("records", dbOpenDynaset)\n    \n    Do Until rsQueryCrit.EOF\n        qry = rsQueryCrit!query\n        strSQL = "SELECT * FROM records WHERE " & qry\n        rsRecords.Open strSQL, dbOpenDynaset\n        If Not rsRecords.EOF Then\n            ' Generate report or log errors\n            Debug.Print rsQueryCrit!error\n        End If\n        rsRecords.Close\n        rsQueryCrit.MoveNext\n    Loop\n    \n    rsQueryCrit.Close\n    Set rsQueryCrit = Nothing\n    Set rsRecords = Nothing\n    Set db = Nothing\nEnd Sub