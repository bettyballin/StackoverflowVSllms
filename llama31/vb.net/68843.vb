Imports System.Data.SqlClient\nImports System.Data\n\nDim connectionString As String = "Your connection string"\nDim query As String = "SELECT * FROM YourTable"\n\nDim connection As New SqlConnection(connectionString)\nDim adapter As New SqlDataAdapter(query, connection)\nDim table As New DataTable()\n\nconnection.Open()\nadapter.Fill(table)\nconnection.Close()\n\nDim row As DataRow = table.Rows(0) ' Move to the first row\n\n' Move to the next row\nPrivate Sub MoveNext()\n    If row IsNot Nothing AndAlso row.RowIndex < table.Rows.Count - 1 Then\n        row = table.Rows(row.RowIndex + 1)\n    End If\nEnd Sub\n\n' Move to the previous row\nPrivate Sub MovePrevious()\n    If row IsNot Nothing AndAlso row.RowIndex > 0 Then\n        row = table.Rows(row.RowIndex - 1)\n    End If\nEnd Sub\n\n' Move to the first row\nPrivate Sub MoveFirst()\n    row = table.Rows(0)\nEnd Sub\n\n' Move to the last row\nPrivate Sub MoveLast()\n    row = table.Rows(table.Rows.Count - 1)\nEnd Sub