Public Class SiteComparer\n    Implements IEqualityComparer(Of Object)\n\n    Public Function Equals(x As Object, y As Object) As Boolean Implements IEqualityComparer(Of Object).Equals\n        Dim xSite = CType(x, Object)\n        Dim ySite = CType(y, Object)\n        Return xSite.SiteID = ySite.SiteID\n    End Function\n\n    Public Function GetHashCode(obj As Object) As Integer Implements IEqualityComparer(Of Object).GetHashCode\n        Dim site = CType(obj, Object)\n        Return site.SiteID.GetHashCode()\n    End Function\nEnd Class