Imports Microsoft.Build.Construction\n\nModule Module1\n    Sub Main()\n        ' Create a new project\n        Dim project As ProjectRootElement = ProjectRootElement.Create("MyProject.vbproj")\n\n        ' Add a target framework\n        project.AddProperty("TargetFramework", "net35")\n\n        ' Add a compile item\n        project.AddItem("Compile", "Module1.vb")\n\n        ' Add a reference\n        project.AddItem("Reference", "System")\n\n        ' Save the project\n        project.Save()\n    End Sub\nEnd Module