Imports System.IO\n\nModule Module1\n    Sub Main()\n        ' Define the source file path\n        Dim sourceFilePath As String = Path.Combine(Application.StartupPath, "myicon.ico")\n        \n        ' Get the path to the startup folder\n        Dim startupFolder As String = Environment.GetFolderPath(Environment.SpecialFolder.Startup)\n        \n        ' Define the destination file path\n        Dim destinationFilePath As String = Path.Combine(startupFolder, "myicon.ico")\n        \n        Try\n            ' Copy the file to the startup folder\n            File.Copy(sourceFilePath, destinationFilePath, True)\n            Console.WriteLine("Icon copied successfully.")\n        Catch ex As Exception\n            Console.WriteLine("Error copying icon: " & ex.Message)\n        End Try\n    End Sub\nEnd Module