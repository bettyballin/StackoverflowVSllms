Dim rdoSession As New Redemption.RDOSession\nDim rdoAddressLists As Redemption.RDOAddressLists\nDim rdoAddressList As Redemption.RDOAddressList\n\nSet rdoSession = CreateObject("Redemption.RDOSession")\nrdoSession.Logon\n\nSet rdoAddressLists = rdoSession.AddressLists\n\n' Get the "X" address list\nSet rdoAddressList = rdoAddressLists.Item("X")\n\n' Get the GAL address list\nSet rdoGAL = rdoAddressLists.Item("Global Address List")\n\n' Get the Contacts address list\nSet rdoContacts = rdoAddressLists.Item("Contacts")\n\n' Reorder the address lists\nrdoAddressLists.Remove rdoContacts\nrdoAddressLists.Add rdoAddressList, 1\nrdoAddressLists.Add rdoContacts, 2\nrdoAddressLists.Add rdoGAL, 3\n\n' Save the changes\nrdoSession.Save