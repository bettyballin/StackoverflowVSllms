Sub ExportDataToDatabase()\n    Dim cn As ADODB.Connection\n    Dim rs As ADODB.Recordset\n    Dim sql As String\n    \n    ' Create a connection object\n    Set cn = New ADODB.Connection\n    \n    ' Open a connection to the database\n    cn.Open "Provider=SQLOLEDB.1;Data Source=myServerAddress;Initial Catalog=myDataBase;User ID=myUsername;Password=myPassword;"\n    \n    ' Create a recordset object\n    Set rs = New ADODB.Recordset\n    \n    ' Set the SQL query\n    sql = "INSERT INTO myTable (column1, column2) VALUES ('" & Range("A1").Value & "', '" & Range("B1").Value & "')"\n    \n    ' Execute the query\n    rs.Open sql, cn, adOpenStatic, adLockOptimistic\n    \n    ' Close the recordset\n    rs.Close\n    \n    ' Close the connection\n    cn.Close\n    \n    ' Release the objects\n    Set rs = Nothing\n    Set cn = Nothing\nEnd Sub