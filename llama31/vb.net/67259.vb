Private Sub ExecuteSQLScript()\n    Dim sqlScript As String\n    Dim resData As Variant\n    \n    ' Load the SQL script from the resource\n    resData = LoadResData("SQL_SCRIPT", "TEXT")\n    \n    ' Create a stream from the loaded resource\n    Dim strm As ADODB.Stream\n    Set strm = CreateStreamFromResource(resData)\n    \n    ' Read the SQL script from the stream\n    sqlScript = strm.ReadText\n    \n    ' Execute the SQL script on the server\n    Dim cn As ADODB.Connection\n    Set cn = New ADODB.Connection\n    cn.Open "Your connection string here"\n    cn.Execute sqlScript\n    \n    ' Clean up\n    Set strm = Nothing\n    Set cn = Nothing\nEnd Sub