Public Function CleanText(ByVal str As String) As String\n    If Not String.IsNullOrEmpty(str) Then\n        Dim indexChars() As String = {"<a", "<img", "<input", "<span", "<p", "<ul", "<div", "<embed", "<object", "<param"}\n        For i As Integer = 0 To indexChars.GetUpperBound(0)\n            Dim regex As New System.Text.RegularExpressions.Regex(indexChars(i) + ".*?>", System.Text.RegularExpressions.RegexOptions.IgnoreCase)\n            str = regex.Replace(str, "")\n        Next\n    End If\n    Return str\nEnd Functio