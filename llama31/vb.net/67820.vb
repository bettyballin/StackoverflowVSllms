Public Property TotalLogins() As System.Nullable(Of Integer)\n    Get\n        Return Me._TotalLogins\n    End Get\n    Protected Set(ByVal value As System.Nullable(Of Integer))\n        If value < Me._TotalLogins Then\n            Throw New InvalidOperationException("Cannot decrease total logins")\n        End If\n        Me._TotalLogins = value\n        Me.OnPropertyChanged("TotalLogins")\n    End Set\nEnd Property