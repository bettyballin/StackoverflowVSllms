Imports OfficeOpenXml\n\nModule ExcelReader\n    Sub Main()\n        Dim filePath As String = "C:\path\to\your\file.xlsx"\n        Using package As New ExcelPackage(New FileInfo(filePath))\n            Dim worksheet As ExcelWorksheet = package.Workbook.Worksheets(1)\n\n            ' Read data from the worksheet\n            For row As Integer = 1 To worksheet.Dimension.Rows\n                For col As Integer = 1 To worksheet.Dimension.Columns\n                    Dim value As Object = worksheet.Cells(row, col).Value\n                    Console.WriteLine($"Cell ({row}, {col}) = {value}")\n                Next\n            Next\n        End Using\n    End Sub\nEnd Module