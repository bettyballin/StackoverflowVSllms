Imports System.Collections.Generic\n\n' ...\n\nDim sqlData As DataGridView = ' your SQL Server data grid\nDim cadData As HashSet(Of String) = New HashSet(Of String)()\n\n' Load CAD system data into the HashSet\nFor Each partNumber As String In ' your CAD system data\n    cadData.Add(partNumber)\nNext\n\n' Compare SQL Server data with CAD system data\nFor Each row As DataGridViewRow In sqlData.Rows\n    Dim partNumber As String = row.Cells("PartNumber").Value.ToString()\n    If cadData.Contains(partNumber) Then\n        row.DefaultCellStyle.BackColor = Color.Transparent\n        cadData.Remove(partNumber)\n    Else\n        row.DefaultCellStyle.BackColor = Color.Yellow\n    End If\nNext\n\n' Add remaining CAD system data to the grid with red background\nFor Each partNumber As String In cadData\n    Dim newRow As DataGridViewRow = sqlData.Rows.Add()\n    newRow.Cells("PartNumber").Value = partNumber\n    newRow.DefaultCellStyle.BackColor = Color.Red\nNext