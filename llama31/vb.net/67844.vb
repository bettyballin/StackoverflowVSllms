Sub ExportToCSV()\n    Dim db As DAO.Database\n    Dim rs As DAO.Recordset\n    Dim fso As Object\n    Dim csvFile As Object\n    \n    Set db = CurrentDb()\n    Set rs = db.OpenRecordset("YourTable", dbOpenSnapshot)\n    \n    ' Select specific columns\n    rs.Fields.Append "Column1"\n    rs.Fields.Append "Column2"\n    rs.Fields.Append "Column3"\n    \n    Set fso = CreateObject("Scripting.FileSystemObject")\n    Set csvFile = fso.CreateTextFile("C:\Path\To\Your\CSV\File.csv", True)\n    \n    ' Write header row\n    csvFile.WriteLine "Column1,Column2,Column3"\n    \n    ' Write data rows\n    While Not rs.EOF\n        csvFile.WriteLine rs!Column1 & "," & rs!Column2 & "," & rs!Column3\n        rs.MoveNext\n    Wend\n    \n    csvFile.Close\n    Set csvFile = Nothing\n    Set fso = Nothing\n    rs.Close\n    Set rs = Nothing\n    db.Close\n    Set db = Nothing\nEnd Sub