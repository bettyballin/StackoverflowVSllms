Imports System.Xml.Serialization\n\nPublic Class Pair(Of TString, TObject)\n    Implements IXmlSerializable\n\n    Public Property Value1 As TString\n    Public Property Value2 As TObject\n\n    Public Function GetSchema() As XmlSchema Implements IXmlSerializable.GetSchema\n        Return Nothing\n    End Function\n\n    Public Sub ReadXml(reader As XmlReader) Implements IXmlSerializable.ReadXml\n        Value1 = reader.ReadString()\n        Value2 = CType(reader.ReadObject(), TObject)\n    End Sub\n\n    Public Sub WriteXml(writer As XmlWriter) Implements IXmlSerializable.WriteXml\n        writer.WriteStartElement("value1")\n        writer.WriteString(Value1)\n        writer.WriteEndElement()\n\n        writer.WriteStartElement("value2")\n        writer.WriteObject(Value2)\n        writer.WriteEndElement()\n    End Sub\nEnd Class