Imports System\n\nModule RockPaperScissors\n    Sub Main()\n        Dim random As New Random()\n        Dim computerChoice As Integer = random.Next(1, 4)\n        Console.Write("Enter your choice (1 = Rock, 2 = Paper, 3 = Scissors): ")\n        Dim userChoice As Integer = CInt(Console.ReadLine())\n        ' Determine the winner\n        If userChoice = computerChoice Then\n            Console.WriteLine("It's a tie!")\n        ElseIf (userChoice = 1 And computerChoice = 3) Or (userChoice = 2 And computerChoice = 1) Or (userChoice = 3 And computerChoice = 2) Then\n            Console.WriteLine("You win!")\n        Else\n            Console.WriteLine("Computer wins!")\n        End If\n    End Sub\nEnd Module