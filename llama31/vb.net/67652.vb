Dim ConnLocation As String\nDim ConnDSN As String\nDim ConnCreateCube As String\nDim Connection As Object\n\n' Define connection properties\nConnLocation = "C:\test.cub"\nConnDSN = "TEST"\nConnCreateCube = "CREATE CUBE [TestCube] (DIMENSION [account_code])"\n\n' Create an ADODB Connection object\nSet Connection = CreateObject("ADODB.Connection")\n\n' Set the provider and open the connection\nConnection.Provider = "msolap"\nConnection.Open "Provider=msolap;DataSource=" & ConnLocation\n\n' Create the cube\nConnection.Execute ConnCreateCube\n\n' Clean up\nSet Connection = Nothing