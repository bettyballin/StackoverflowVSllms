Imports System.Diagnostics\n\nPublic Sub CompactDatabase(databasePath As String)\n    Dim accessExecutable As String = "C:\Program Files\Microsoft Office\root\Office16\MSACCESS.EXE"\n    Dim compactCommand As String = "/compact " & databasePath\n\n    Using process As New Process()\n        process.StartInfo.FileName = accessExecutable\n        process.StartInfo.Arguments = compactCommand\n        process.StartInfo.WindowStyle = ProcessWindowStyle.Hidden\n        process.Start()\n        process.WaitForExit()\n    End Using\nEnd Sub