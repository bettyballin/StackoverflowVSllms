Try\n    Me.Adapter.Fill(dataTable)\nCatch ex As Exception\n    For Each TRow As DataRow In dataTable.Rows\n        If TRow.HasErrors Then\n            Trace.Write(TRow.RowError)\n            ' Get more error information\n            Dim errors() As DataColumn = TRow.GetErrors()\n            For Each errorColumn As DataColumn In errors\n                Trace.Write(errorColumn.ColumnName & ": " & errorColumn.Error)\n            Next\n        End If\n    Next\nEnd Try