Sub ExportDataToSQLServer()\n    ' Define the connection string\n    Dim connectionString As String\n    connectionString = "Provider=SQLOLEDB.1;Data Source=<your_server_name>;Initial Catalog=<your_database_name>;User ID=<your_username>;Password=<your_password>"\n\n    ' Create an ADO connection object\n    Dim connection As Object\n    Set connection = CreateObject("ADODB.Connection")\n    connection.Open connectionString\n\n    ' Create an ADO command object\n    Dim command As Object\n    Set command = CreateObject("ADODB.Command")\n    command.ActiveConnection = connection\n\n    ' Define the SQL query to insert data\n    Dim sqlQuery As String\n    sqlQuery = "INSERT INTO <your_table_name> (column1, column2) VALUES ('value1', 'value2')"\n\n    ' Execute the SQL query\n    command.CommandText = sqlQuery\n    command.Execute\n\n    ' Close the connection\n    connection.Close\nEnd Sub