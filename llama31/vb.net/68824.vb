Imports Dapper\n\nPublic Class MyProc\n    Private _connectionString As String\n\n    Public Sub New(connectionString As String)\n        _connectionString = connectionString\n    End Sub\n\n    Public Function Execute(param1 As String, param2 As String, param3 As String) As String\n        Using connection As New SqlConnection(_connectionString)\n            connection.Open()\n            Dim parameters As New DynamicParameters()\n            parameters.Add("@Param1", param1)\n            parameters.Add("@Param2", param2)\n            parameters.Add("@Param3", param3)\n            Dim result As String = connection.Execute("MyProc", parameters, commandType:=CommandType.StoredProcedure)\n            Return result\n        End Using\n    End Function\nEnd Class