Sub CreateOutlookAppointment()\n    Dim olApp As Object\n    Dim olApt As Object\n    \n    ' Create an instance of Outlook\n    Set olApp = CreateObject("Outlook.Application")\n    \n    ' Create a new appointment\n    Set olApt = olApp.CreateItem(1) ' 1 = olAppointmentItem\n    \n    ' Set the appointment properties\n    With olApt\n        .Subject = "Test Appointment"\n        .Start = DateAdd("d", 30, Range("A1").Value) ' Assume date is in cell A1\n        .End = DateAdd("h", 1, .Start)\n        .Save\n    End With\n    \n    ' Clean up\n    Set olApt = Nothing\n    Set olApp = Nothing\nEnd Sub