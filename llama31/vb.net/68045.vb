Sub UploadFileSFTP()\n    Const cSFTPHost As String = "your_sftp_host"\n    Const cSFTPUser As String = "your_sftp_username"\n    Const cSFTPPassword As String = "your_sftp_password"\n    Const cSFTPPort As Integer = 22\n    \n    Dim session As New WinSCPSHC.Session\n    Dim sessionOptions As New WinSCPSHC.SessionOptions\n    \n    sessionOptions.Protocol = WinSCPSHC.Protocol_SFTP\n    sessionOptions.HostName = cSFTPHost\n    sessionOptions.UserName = cSFTPUser\n    sessionOptions.Password = cSFTPPassword\n    sessionOptions.PortNumber = cSFTPPort\n    sessionOptions.GiveUpSecurityAndAcceptAnySshHostKey = True\n    \n    session.Open sessionOptions\n    \n    Dim file As String\n    file = "path_to_your_file.txt"\n    \n    session.PutFile file, "/remote/path/to/your/file.txt"\n    \n    session.Close\nEnd Sub