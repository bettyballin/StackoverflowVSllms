Dim cn, cmd, rs\n\n' Create a connection object\nSet cn = Server.CreateObject("ADODB.Connection")\ncn.Open "your_connection_string"\n\n' Create a command object\nSet cmd = Server.CreateObject("ADODB.Command")\ncmd.ActiveConnection = cn\n\n' Define the SQL query with parameters\ncmd.CommandText = "SELECT * FROM your_table WHERE name = ? AND age = ?"\n\n' Add parameters to the command\ncmd.Parameters.Append cmd.CreateParameter("@name", adVarChar, adParamInput, 50)\ncmd.Parameters.Append cmd.CreateParameter("@age", adInteger, adParamInput)\n\n' Set parameter values\ncmd.Parameters("@name").Value = "John"\ncmd.Parameters("@age").Value = 30\n\n' Execute the query\nSet rs = cmd.Execute\n\n' Process the results\nWhile Not rs.EOF\n    Response.Write rs("column_name")\n    rs.MoveNext\nWend\n\n' Clean up\nrs.Close\nSet rs = Nothing\ncmd.Parameters.Clear\nSet cmd = Nothing\ncn.Close\nSet cn = Nothing