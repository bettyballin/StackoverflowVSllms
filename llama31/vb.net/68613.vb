Private Const SNAP_DISTANCE As Integer = 50 ' adjust this value as needed\n\nPrivate predefinedPoints As List(Of Point) = New List(Of Point) From {\n    New Point(100, 100),\n    New Point(200, 200),\n    New Point(300, 300)\n    ' add more points as needed\n}\n\nPrivate Sub PictureBox1_MouseUp(sender As Object, e As MouseEventArgs) Handles PictureBox1.MouseUp\n    Dim pictureBox As PictureBox = DirectCast(sender, PictureBox)\n    Dim mousePoint As Point = e.Location\n\n    For Each point In predefinedPoints\n        If Distance(mousePoint, point) <= SNAP_DISTANCE Then\n            pictureBox.Location = point\n            Exit For\n        End If\n    Next\nEnd Sub\n\nPrivate Function Distance(p1 As Point, p2 As Point) As Integer\n    Return Math.Sqrt(Math.Pow(p2.X - p1.X, 2) + Math.Pow(p2.Y - p1.Y, 2))\nEnd Functio