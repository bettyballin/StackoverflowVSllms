Public Sub btnSubmit_click(ByVal sender As Object, ByVal e As System.EventArgs)\n    Dim queryString As StringBuilder = New StringBuilder("nextpage.aspx?")\n\n    Dim aBoat As String = Request.QueryString("apBoat")\n    Dim bBoat As String = Request.QueryString("bBoat")\n    Dim cBoat As String = Request.QueryString("cBoat")\n\n    If Not String.IsNullOrEmpty(aBoat) Then\n        queryString.Append("apb=").Append(aBoat).Append("&")\n    End If\n\n    If Not String.IsNullOrEmpty(bBoat) Then\n        queryString.Append("bBoat=").Append(bBoat).Append("&")\n    End If\n\n    If Not String.IsNullOrEmpty(cBoat) Then\n        queryString.Append("cBoat=").Append(cBoat)\n    End If\n\n    Response.Redirect(queryString.ToString())\nEnd Sub