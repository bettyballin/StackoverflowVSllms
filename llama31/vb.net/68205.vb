Imports System.IO\nImports System.Text.RegularExpressions\n\nPublic Class VCardReader\n    Public Function ReadVCard(ByVal filePath As String) As Dictionary(Of String, String)\n        Dim vCardData As New Dictionary(Of String, String)\n        Dim lines As String() = File.ReadAllLines(filePath)\n\n        For Each line As String In lines\n            If line.StartsWith("BEGIN:VCARD") Then\n                Continue For\n            ElseIf line.StartsWith("END:VCARD") Then\n                Exit For\n            End If\n\n            Dim match As Match = Regex.Match(line, "(.+?):(.+)")\n            If match.Success Then\n                Dim key As String = match.Groups(1).Value.ToUpper()\n                Dim value As String = match.Groups(2).Value\n\n                If vCardData.ContainsKey(key) Then\n                    vCardData(key) &= vbCrLf & value\n                Else\n                    vCardData.Add(key, value)\n                End If\n            End If\n        Next\n\n        Return vCardData\n    End Function\n\n    Public Function ReadVCalendar(ByVal filePath As String) As Dictionary(Of String, String)\n        Dim vCalendarData As New Dictionary(Of String, String)\n        Dim lines As String() = File.ReadAllLines(filePath)\n\n        For Each line As String In lines\n            If line.StartsWith("BEGIN:VCALENDAR") Then\n                Continue For\n            ElseIf line.StartsWith("END:VCALENDAR") Then\n                Exit For\n            End If\n\n            Dim match As Match = Regex.Match(line, "(.+?):(.+)")\n            If match.Success Then\n                Dim key As String = match.Groups(1).Value.ToUpper()\n                Dim value As String = match.Groups(2).Value\n\n                If vCalendarData.ContainsKey(key) Then\n                    vCalendarData(key) &= vbCrLf & value\n                Else\n                    vCalendarData.Add(key, value)\n                End If\n            End If\n        Next\n\n        Return vCalendarData\n    End Function\nEnd Class