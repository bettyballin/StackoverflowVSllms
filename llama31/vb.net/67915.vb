Sub WriteDataToSQL()\n    ' Define database connection parameters\n    Dim connectionString As String\n    connectionString = "Driver={ODBC Driver 17 for SQL Server};" & _\n                       "Server=your_server_name;" & _\n                       "Database=your_database_name;" & _\n                       "UID=your_username;" & _\n                       "PWD=your_password;"\n\n    ' Create an ADO connection object\n    Dim conn As New ADODB.Connection\n    conn.Open connectionString\n\n    ' Define the SQL query to update the data\n    Dim sqlQuery As String\n    sqlQuery = "UPDATE your_table_name SET column1 = ?, column2 = ? WHERE id = ?"\n\n    ' Create an ADO command object\n    Dim cmd As New ADODB.Command\n    cmd.ActiveConnection = conn\n    cmd.CommandText = sqlQuery\n\n    ' Define the parameters for the query\n    Dim param1 As New ADODB.Parameter\n    param1.Name = "column1"\n    param1.Type = adVarChar\n    param1.Size = 50\n    param1.Value = Range("A2").Value\n\n    Dim param2 As New ADODB.Parameter\n    param2.Name = "column2"\n    param2.Type = adVarChar\n    param2.Size = 50\n    param2.Value = Range("B2").Value\n\n    Dim param3 As New ADODB.Parameter\n    param3.Name = "id"\n    param3.Type = adInteger\n    param3.Value = Range("C2").Value\n\n    ' Add the parameters to the command\n    cmd.Parameters.Append param1\n    cmd.Parameters.Append param2\n    cmd.Parameters.Append param3\n\n    ' Execute the query\n    cmd.Execute\n\n    ' Clean up\n    Set param1 = Nothing\n    Set param2 = Nothing\n    Set param3 = Nothing\n    Set cmd = Nothing\n    conn.Close\n    Set conn = Nothing\nEnd Sub