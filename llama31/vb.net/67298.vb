Public Class ValidationItem\n    ' existing properties and code\n\n    Public Overrides Function Equals(obj As Object) As Boolean\n        If obj Is Nothing OrElse Not GetType(ValidationItem).Equals(obj.GetType()) Then\n            Return False\n        End If\n\n        Dim other As ValidationItem = DirectCast(obj, ValidationItem)\n        Return Me.Item = other.Item AndAlso Me.Message = other.Message\n    End Function\n\n    Public Overrides Function GetHashCode() As Integer\n        Return Me.Item.GetHashCode() Xor Me.Message.GetHashCode()\n    End Function\nEnd Class