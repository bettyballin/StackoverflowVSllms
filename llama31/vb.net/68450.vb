Imports System.Collections.Generic\n\nPublic Class MultiDimensionalHashtable(Of TKey, TValue)\n    Private dictionary As Dictionary(Of TKey, Dictionary(Of TKey, TValue))\n\n    Public Sub New()\n        dictionary = New Dictionary(Of TKey, Dictionary(Of TKey, TValue))\n    End Sub\n\n    Public ReadOnly Property Item(ByVal key1 As TKey) As Dictionary(Of TKey, TValue)\n        Get\n            If Not dictionary.ContainsKey(key1) Then\n                dictionary(key1) = New Dictionary(Of TKey, TValue)\n            End If\n            Return dictionary(key1)\n        End Get\n    End Property\n\n    Public Sub Add(ByVal key1 As TKey, ByVal key2 As TKey, ByVal value As TValue)\n        If Not dictionary.ContainsKey(key1) Then\n            dictionary(key1) = New Dictionary(Of TKey, TValue)\n        End If\n        dictionary(key1).Add(key2, value)\n    End Sub\n\n    Public Function ContainsKey(ByVal key1 As TKey, ByVal key2 As TKey) As Boolean\n        If Not dictionary.ContainsKey(key1) Then\n            Return False\n        End If\n        Return dictionary(key1).ContainsKey(key2)\n    End Function\nEnd Class